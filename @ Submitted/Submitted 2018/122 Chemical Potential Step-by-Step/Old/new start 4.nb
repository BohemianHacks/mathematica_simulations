Cell["", "DetailsSection",ExpressionUUID->"aa6635b1-9943-4635-98fe-2ee369dff52f"],

Cell[TextData[{
 "The chemical potential ",
 Cell[BoxData[
  FormBox["\[Mu]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "13851708-5c8d-4c27-bca6-3b4d75e1d77f"],
 " is equal to the Gibbs free energy ",
 Cell[BoxData[
  FormBox["G", TraditionalForm]], "InlineMath",ExpressionUUID->
  "cf8fbec4-0bf0-4b4c-b289-54b60f1d6e6a"],
 " for a single component. The differential of the Gibbs free energy is:"
}], "DetailNotes",
 CellChangeTimes->{{3.6925480549640656`*^9, 3.69254809625708*^9}},
 CellID->629425773,ExpressionUUID->"6ff991f6-5acd-40ae-9e51-48f041c995c6"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[DifferentialD]", "G"}], "=", 
    RowBox[{
     RowBox[{"V", " ", "d", "\[InvisibleSpace]", "P"}], "-", 
     RowBox[{"S", " ", "d", "\[InvisibleSpace]", "T"}]}]}], TraditionalForm]],
   "InlineMath",ExpressionUUID->"721a695e-4b41-4806-91c0-b2b46a166571"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6925480549640656`*^9, 3.692548118282198*^9}, {
  3.693159790054532*^9, 3.6931598037332044`*^9}, {3.694465632375845*^9, 
  3.6944656334170647`*^9}, {3.7421219886286783`*^9, 3.742121990405795*^9}},
 CellID->77248943,ExpressionUUID->"94c0ab35-e615-470e-9ac3-905416254a7e"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]], "InlineMath",ExpressionUUID->
  "7cee32b2-5149-42b2-8c1b-a552b780a3e5"],
 " is volume, ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath",ExpressionUUID->
  "677be2cc-ae06-4a03-b06d-89b05c2138a4"],
 " is pressure, ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]], "InlineMath",ExpressionUUID->
  "78ab8b7f-b65b-4717-ab07-d85b742c9668"],
 " is entropy and ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath",ExpressionUUID->
  "3957ae6c-919c-4836-baca-02a3e6b73b78"],
 " is temperature. For a single component system, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"G", "=", "\[Mu]"}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"06366df8-182c-4831-a14f-f41c174a3b2a"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.6925480549640656`*^9, 3.6925481398915167`*^9}, {
   3.6926291304006824`*^9, 3.692629141751136*^9}, 3.6942874985003347`*^9, 
   3.694528646594795*^9},
