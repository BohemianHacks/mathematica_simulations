Cell[CellGroupData[{

Cell["", "DetailsSection",ExpressionUUID->"3dcf33d1-6f91-4be9-92dc-082d924b0c5c"],

Cell["\<\
The volume and density are calculated from the Peng-Robinson equation of \
state:\
\>", "DetailNotes",
 CellChangeTimes->{{3.676150902487013*^9, 3.6761509249721804`*^9}, 
   3.6761512252907863`*^9},
 CellID->113927855,ExpressionUUID->"109bddf1-a59a-4d3f-9e97-9e5e5179a4aa"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     FractionBox[
      RowBox[{"R", " ", "T"}], 
      RowBox[{"V", "-", "b"}]], "-", 
     FractionBox["a", 
      RowBox[{
       SuperscriptBox["V", "2"], "+", 
       RowBox[{"2", " ", "V", " ", "b"}], "-", 
       SuperscriptBox["b", "2"]}]]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"aa5f15f6-25fd-43d0-b112-f29ce732fa34"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.676150902487013*^9, 3.676150953615452*^9}},
 CellID->5796550,ExpressionUUID->"ee157f62-02ff-48e2-8e47-0944ebcf1b6e"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"a", "=", 
    RowBox[{"0.457", " ", 
     FractionBox[
      RowBox[{
       SuperscriptBox["R", "2"], " ", 
       SubsuperscriptBox["T", "c", "2"]}], 
      SubscriptBox["P", "c"]], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{"\[Kappa]", " ", 
         RowBox[{"(", 
          RowBox[{"1", "-", 
           SqrtBox[
            RowBox[{"T", "/", 
             SubscriptBox["T", "c"]}]]}], ")"}]}]}], ")"}], "2"]}]}], 
   TraditionalForm]], "InlineMath",ExpressionUUID->
  "99312f39-5b22-48fc-8884-f5164443307e"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.676150902487013*^9, 3.6761509891302276`*^9}, {
  3.676151040877079*^9, 3.6761510555916805`*^9}},
 CellID->548693058,ExpressionUUID->"fb98d04a-61ac-4aa4-828e-2b47ded0e81e"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"b", "=", 
    RowBox[{"0.0778", " ", 
     FractionBox[
      RowBox[{"R", " ", 
       SubscriptBox["T", "c"]}], 
      SubscriptBox["P", "c"]]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"e0237992-5740-4690-ae58-30ee37eeaa92"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.676150902487013*^9, 3.676151001022301*^9}, {
  3.67615105254916*^9, 3.6761510583336926`*^9}},
 CellID->1248694,ExpressionUUID->"610438d7-8cbe-49e8-904a-94fa78cd7674"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Kappa]", "=", 
    RowBox[{"0.375", "+", 
     RowBox[{"1.54", " ", "\[Omega]"}], "-", 
     RowBox[{"0.27", " ", 
      SuperscriptBox["\[Omega]", "2"]}]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"0ba5cbab-32a5-41ca-85fd-e5d84d4c5520"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.676150902487013*^9, 3.6761510201744304`*^9}},
 CellID->147558564,ExpressionUUID->"5cd96113-a457-42dc-94e6-272c2702e77c"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath",ExpressionUUID->
  "2eab87ac-1475-489c-a6ac-d1f8be8dc99a"],
 " is pressure (MPa), ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath",ExpressionUUID->
  "f27dbc80-7c7f-4e4d-b379-bca14f66eaef"],
 " is temperature (K), the subscript ",
 Cell[BoxData[
  FormBox["c", TraditionalForm]], "InlineMath",ExpressionUUID->
  "4ff236ee-9bb0-4a5a-9a95-68cea6feea26"],
 " represents the critical property, ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]], "InlineMath",ExpressionUUID->
  "2eb38e16-777a-4a41-84d6-5225fc088e63"],
 " is the molar volume (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["cm", "3"], "/", "mol"}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"43f53465-85c5-4c54-8db9-c2ba7903d417"],
 "), ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]], "InlineMath",ExpressionUUID->
  "4e41e52f-7dc2-43f2-9b58-aa070abd989e"],
 " is the ideal gas constant, ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]], "InlineMath",ExpressionUUID->
  "73455a59-dd58-430d-aeb9-fb5c1c9ce46b"],
 ", ",
 Cell[BoxData[
  FormBox["b", TraditionalForm]], "InlineMath",ExpressionUUID->
  "4872d60f-82e7-4de8-9bb6-baed92a774f6"],
 ", and ",
 Cell[BoxData[
  FormBox["\[Kappa]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "796ae8dd-ffea-482e-b6da-447b1726366b"],
 " are Peng-Robinson constants, and ",
 Cell[BoxData[
  FormBox["\[Omega]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "c5c5d2f3-f974-4eb0-8476-cfc47c4539d5"],
 " is the acentric factor."
}], "DetailNotes",
 CellChangeTimes->{{3.676150902487013*^9, 3.676151192349085*^9}},
 CellID->818126333,ExpressionUUID->"ba83c61a-eb85-44f8-8eea-7f662b6f9c40"],

Cell[TextData[{
 "The molar density is ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Rho]", "=", 
    RowBox[{"1", "/", "V"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"4dd40cb5-c3f5-47b0-9bef-c33d10179415"],
 " (",
 Cell[BoxData[
  FormBox[
   RowBox[{"mol", "/", 
    SuperscriptBox["cm", "3"]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"460fb01c-379c-4be2-af2c-6206f68236de"],
 ")."
}], "DetailNotes",
 CellChangeTimes->{{3.6763105575810575`*^9, 3.6763105575966578`*^9}, 
   3.6775904432453933`*^9},
 CellID->534252177,ExpressionUUID->"3237105a-cb80-45a7-bc7f-abd15ae09930"],

