Cell[TextData[{
 "Heat is generated at a constant rate in either wall ",
 Cell[BoxData[
  FormBox[
   StyleBox["A",
    FontSlant->"Plain"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "315ec3e7-fe39-4f7c-b7f8-d6f4b0effb77"],
