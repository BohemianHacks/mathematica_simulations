  FormBox[
   RowBox[{"i", "=", 
    RowBox[{"(", 
     RowBox[{
      StyleBox["b",
       FontSlant->"Plain"], ",", 
      StyleBox["w",
       FontSlant->"Plain"]}], 
     StyleBox[")",
      FontSlant->"Plain"]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"e0eb6cc8-808e-46ff-a930-43985dee397c"],
 " (bar), ",
