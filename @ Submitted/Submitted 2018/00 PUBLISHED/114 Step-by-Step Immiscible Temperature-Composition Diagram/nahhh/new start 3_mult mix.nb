Cell[TextData[{
 "solve the second equation for ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["T", 
    StyleBox["b",
     FontSlant->"Plain"], "sat"], TraditionalForm]], "InlineMath",
  ExpressionUUID->"cfc423cd-c0db-437d-a7f3-69e7038e02d3"],
