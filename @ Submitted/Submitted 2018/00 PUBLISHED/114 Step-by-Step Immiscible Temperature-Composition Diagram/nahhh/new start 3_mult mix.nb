  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["P", 
      StyleBox["b",
       FontSlant->"Plain"], "sat"], "(", 
     SubsuperscriptBox["T", 
      StyleBox["b",
       FontSlant->"Plain"], "sat"], ")"}], "=", 
    RowBox[{
     SubscriptBox["y", 
      StyleBox["b",
       FontSlant->"Plain"]], " ", "P"}]}], TraditionalForm]], "InlineMath",
