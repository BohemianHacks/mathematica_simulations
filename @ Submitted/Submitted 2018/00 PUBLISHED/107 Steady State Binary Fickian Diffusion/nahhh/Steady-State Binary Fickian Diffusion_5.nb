Cell[CellGroupData[{

Cell["", "DetailsSection",ExpressionUUID->"f6e9f3a0-6c8b-44d6-8afb-5d8af6f4c12e"],

Cell["The diffusive flux and total flux are:", "DetailNotes",
 CellChangeTimes->{{3.713879482893546*^9, 3.713879497495224*^9}, {
  3.714486834259737*^9, 3.714486843430588*^9}},
 CellID->774539260,ExpressionUUID->"031b22f5-965e-4870-9c53-688da0733f51"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["J", "diff"], "=", 
    FractionBox[
     RowBox[{
      SubscriptBox["C", 
       StyleBox["m",
        FontSlant->"Plain"]], " ", 
      SubscriptBox["D", "AB"], " ", 
      SuperscriptBox["P", "sat"]}], 
     RowBox[{"L", " ", "P"}]]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "6acbc5f6-cde8-4844-97c7-9e3ce9c56d2f"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.713879482893546*^9, 3.7138795170095263`*^9}},
 CellID->762084428,ExpressionUUID->"684a87f4-c6db-4780-a19e-81dec6b2c132"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["J", "tot"], "=", 
    RowBox[{
     SubscriptBox["D", "AB"], " ", 
     FractionBox[
      SubscriptBox["C", 
       StyleBox["m",
        FontSlant->"Plain"]], "L"], " ", "ln", " ", 
     RowBox[{"(", 
      FractionBox["1", 
       RowBox[{"1", "-", 
        RowBox[{
         SuperscriptBox["P", "sat"], "/", "P"}]}]], ")"}]}]}], 
   TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "620ece22-b078-4e34-a04c-69987dfdfdca"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.713879482893546*^9, 3.713879541085451*^9}},
 CellID->43381661,ExpressionUUID->"3a0e3000-96d2-482f-8ef7-d3106aa4df6c"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["J", "diff"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "9dd95a91-fc17-45ab-9f48-0b5cf647571e"],
 " is the diffusive flux (",
 Cell[BoxData[
  FormBox[
   RowBox[{"mol", "/", 
    RowBox[{"[", 
     RowBox[{
      SuperscriptBox[
       StyleBox["m",
        FontSlant->"Plain"], "2"], " ", 
      StyleBox["s",
       FontSlant->"Plain"]}], "]"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"1d5d9a7a-b742-4fff-a310-8a931b734082"],
 "), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["J", "tot"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "767c891c-9d0c-4049-840c-f3f9e53eaad9"],
 " is the total flux  (",
 Cell[BoxData[
  FormBox[
   RowBox[{"mol", "/", 
    RowBox[{"[", 
     RowBox[{
      SuperscriptBox[
       StyleBox["m",
        FontSlant->"Plain"], "2"], " ", 
      StyleBox["s",
       FontSlant->"Plain"]}], "]"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"8815ce9d-0e55-4d9e-9df4-dcb242e3ad33"],
 "), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["C", 
    StyleBox["m",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",ExpressionUUID->
  "c22a8ee6-c608-4031-af92-43e4ff7c29ec"],
 " is molar concentration (",
 Cell[BoxData[
  FormBox[
   RowBox[{"mol", "/", 
    SuperscriptBox[
     StyleBox["m",
      FontSlant->"Plain"], "3"]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"6018b6a0-cad5-42d0-ba29-cd8ac430ab9c"],
 "), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["D", "AB"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "23df7f9e-3c26-4f93-a27e-13f8205b2200"],
 " is the diffusivity (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["cm", "2"], "/", 
    StyleBox["s",
     FontSlant->"Plain"]}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "922dbd7d-f366-40c8-8e74-2412cc0f24f7"],
 "), ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["P", "sat"], TraditionalForm]], "InlineMath",
  ExpressionUUID->"6b3e60be-6274-45ee-8980-51062905d497"],
 " is vapor pressure (atm), ",
 Cell[BoxData[
  FormBox["L", TraditionalForm]], "InlineMath",ExpressionUUID->
  "6172d5f9-87a6-4b46-81d5-29528c105458"],
 " is the tube height (m), and ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath",ExpressionUUID->
  "5b437665-ef7e-477b-bcca-d08805bb7ce7"],
 " is total pressure (atm)."
}], "DetailNotes",
 CellChangeTimes->{{3.713879482893546*^9, 3.7138796563660555`*^9}, {
  3.714486788529892*^9, 3.714486791401513*^9}},
 CellID->100117653,ExpressionUUID->"b2ee9c40-b624-4ec5-81ce-b7d5ff345cb9"],

Cell["The vapor pressure is calculated using the Antoine equation:", \
"DetailNotes",
 CellChangeTimes->{{3.713879482893546*^9, 3.7138796658632956`*^9}},
 CellID->717097745,ExpressionUUID->"33fb4cb2-df85-403b-9610-545b833de59c"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["P", "sat"], "=", 
    RowBox[{"10", "^", 
     RowBox[{"(", 
      RowBox[{"A", "-", 
       FractionBox["B", 
        RowBox[{"T", "+", "C"}]]}], ")"}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "2b286626-7727-4e13-a732-a4f0b5ef5201"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.713879482893546*^9, 3.7138796767329674`*^9}},
 CellID->18518463,ExpressionUUID->"c767725d-681b-4e8c-b279-2e2ee358a672"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "73207df7-0dd7-4b93-900f-9944cbcee278"],
 ", ",
 Cell[BoxData[
  FormBox["B", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "bc6016af-28c0-4c75-93ce-c77da022e3d0"],
 ", and ",
 Cell[BoxData[
  FormBox["C", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "af177ce7-2b9a-4035-8445-78fb34e22fac"],
 " are Antoine constants, and ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "eb23242b-b0f9-4a1e-81a6-9a7f57501f71"],
 " is temperature (\[Degree]C)."
}], "DetailNotes",
 CellChangeTimes->{{3.713879482893546*^9, 3.7138796965820694`*^9}},
 CellID->4443154,ExpressionUUID->"e3535555-20f0-47cc-8f76-5ac2a6f4905c"],

Cell["\<\
If the diffusive flux dominates (greater than 90% of the total flux), we \
assume that the convective flux is zero, the concentration profile is:\
\>", "DetailNotes",
 CellChangeTimes->{{3.713879482893546*^9, 3.713879733339256*^9}},
 CellID->30235470,ExpressionUUID->"ca66fdd1-e459-4e43-b212-fe2f4a67b1cc"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{
     RowBox[{"-", 
      FractionBox[
       RowBox[{"z", " ", 
        SuperscriptBox["P", "sat"]}], 
       RowBox[{"L", " ", "P"}]]}], "+", 
     FractionBox[
      SuperscriptBox["P", "sat"], "P"]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "ec5d329d-2a85-4111-9ad9-ece6dc1e7731"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.713879482893546*^9, 3.713879753178956*^9}},
 CellID->424589608,ExpressionUUID->"e21084ec-c0b5-4ae4-9bea-29076de322d8"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["y", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "f5ec89d4-1012-40fc-bd6c-0b023d4f6dac"],
 " is mole fraction, and ",
 Cell[BoxData[
  FormBox["z", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "3e9720d8-b5ac-4fc7-a4f4-46a92c54150e"],
 " is tube height (m)."
}], "DetailNotes",
 CellChangeTimes->{{3.713879482893546*^9, 3.713879760896586*^9}, {
  3.713880236050452*^9, 3.7138802410606775`*^9}},
 CellID->174845225,ExpressionUUID->"bd194190-fc8c-4f88-8bce-6de6b31bcf4c"],

Cell["\<\
When the convective flux dominates (greater than 10% of the total flux), the \
concentration profile is:\
\>", "DetailNotes",
 CellChangeTimes->{{3.713879482893546*^9, 3.7138797932725573`*^9}},
 CellID->188342943,ExpressionUUID->"ed3c524d-85cc-4a35-8dfd-9b82ab238b94"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{"1", "-", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox[
         RowBox[{"1", "-", 
          SubscriptBox["y", 
           RowBox[{
            StyleBox["A",
             FontSlant->"Plain"], ",", "L"}]]}], 
         RowBox[{"1", "-", 
          SubscriptBox["y", 
           RowBox[{
            StyleBox["A",
             FontSlant->"Plain"], ",", "0"}]]}]], ")"}], 
       RowBox[{"z", "/", "L"}]], " ", 
      RowBox[{"(", 
       RowBox[{"1", "-", 
        SubscriptBox["y", 
         RowBox[{
          StyleBox["A",
           FontSlant->"Plain"], ",", "0"}]]}], ")"}]}]}]}], TraditionalForm]],
   "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "cd58483d-e2be-490d-b475-4513dfffcc54"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.713879482893546*^9, 3.7138798275991526`*^9}},
 CellID->142189744,ExpressionUUID->"ecd22594-72d4-4939-909f-a6eedeeade73"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", 
     RowBox[{
      StyleBox["A",
       FontSlant->"Plain"], ",", "L"}]], "=", "0"}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"3cd72672-09d1-413b-b226-b57c3e2db5c3"],
 " is the mole fraction at ",
 Cell[BoxData[
  FormBox[
   RowBox[{"z", "=", "L"}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "68a0beed-0094-48ff-b1e6-771338313f59"],
 ", and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", 
     RowBox[{
      StyleBox["A",
       FontSlant->"Plain"], ",", "0"}]], "=", 
    RowBox[{
     SuperscriptBox["P", "sat"], "/", "P"}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"f62b0f58-d1ac-46d5-a85f-fb6184f7b327"],
 " is the mole fraction at ",
 Cell[BoxData[
  FormBox[
   RowBox[{"z", "=", "0"}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "f273850b-80c2-429a-b850-5717af019f1d"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.713879482893546*^9, 3.7138798571529074`*^9}, {
