  FormBox[
   RowBox[{"y", "=", 
    RowBox[{
     RowBox[{"-", 
      FractionBox[
       RowBox[{"z", " ", 
        SuperscriptBox["P", "sat"]}], 
       RowBox[{"L", " ", "P"}]]}], "+", 
     FractionBox[
      SuperscriptBox["P", "sat"], "P"]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "ec5d329d-2a85-4111-9ad9-ece6dc1e7731"],
