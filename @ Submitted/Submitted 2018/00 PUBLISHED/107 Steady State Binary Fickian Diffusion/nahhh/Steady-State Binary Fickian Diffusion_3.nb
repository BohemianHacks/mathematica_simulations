 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{"1", "-", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox[
         RowBox[{"1", "-", 
          SubscriptBox["y", 
           RowBox[{
            StyleBox["A",
             FontSlant->"Plain"], ",", "L"}]]}], 
         RowBox[{"1", "-", 
          SubscriptBox["y", 
           RowBox[{
            StyleBox["A",
             FontSlant->"Plain"], ",", "0"}]]}]], ")"}], 
       RowBox[{"z", "/", "L"}]], " ", 
      RowBox[{"(", 
       RowBox[{"1", "-", 
        SubscriptBox["y", 
         RowBox[{
          StyleBox["A",
           FontSlant->"Plain"], ",", "0"}]]}], ")"}]}]}]}], TraditionalForm]],
   "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "cd58483d-e2be-490d-b475-4513dfffcc54"],
 ","
}], "DetailNotes",
