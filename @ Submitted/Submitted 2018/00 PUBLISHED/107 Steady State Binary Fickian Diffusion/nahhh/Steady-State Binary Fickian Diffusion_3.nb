Cell["", "DetailsSection",ExpressionUUID->"f6e9f3a0-6c8b-44d6-8afb-5d8af6f4c12e"],

Cell["The diffusive flux and total flux are:", "DetailNotes",
 CellChangeTimes->{{3.713879482893546*^9, 3.713879497495224*^9}, {
  3.714486834259737*^9, 3.714486843430588*^9}},
 CellID->774539260,ExpressionUUID->"031b22f5-965e-4870-9c53-688da0733f51"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["J", "diff"], "=", 
    FractionBox[
     RowBox[{
      SubscriptBox["C", 
       StyleBox["m",
        FontSlant->"Plain"]], " ", 
      SubscriptBox["D", "AB"], " ", 
      SuperscriptBox["P", "sat"]}], 
     RowBox[{"L", " ", "P"}]]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "6acbc5f6-cde8-4844-97c7-9e3ce9c56d2f"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.713879482893546*^9, 3.7138795170095263`*^9}},
 CellID->762084428,ExpressionUUID->"684a87f4-c6db-4780-a19e-81dec6b2c132"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["J", "tot"], "=", 
    RowBox[{
     SubscriptBox["D", "AB"], " ", 
     FractionBox[
      SubscriptBox["C", 
       StyleBox["m",
        FontSlant->"Plain"]], "L"], " ", "ln", " ", 
     RowBox[{"(", 
      FractionBox["1", 
       RowBox[{"1", "-", 
        RowBox[{
         SuperscriptBox["P", "sat"], "/", "P"}]}]], ")"}]}]}], 
   TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "620ece22-b078-4e34-a04c-69987dfdfdca"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.713879482893546*^9, 3.713879541085451*^9}},
 CellID->43381661,ExpressionUUID->"3a0e3000-96d2-482f-8ef7-d3106aa4df6c"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["J", "diff"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "9dd95a91-fc17-45ab-9f48-0b5cf647571e"],
 " is the diffusive flux (",
 Cell[BoxData[
  FormBox[
   RowBox[{"mol", "/", 
    RowBox[{"[", 
     RowBox[{
      SuperscriptBox[
       StyleBox["m",
        FontSlant->"Plain"], "2"], " ", 
      StyleBox["s",
       FontSlant->"Plain"]}], "]"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"1d5d9a7a-b742-4fff-a310-8a931b734082"],
 "), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["J", "tot"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "767c891c-9d0c-4049-840c-f3f9e53eaad9"],
 " is the total flux  (",
 Cell[BoxData[
  FormBox[
   RowBox[{"mol", "/", 
    RowBox[{"[", 
     RowBox[{
      SuperscriptBox[
       StyleBox["m",
        FontSlant->"Plain"], "2"], " ", 
      StyleBox["s",
       FontSlant->"Plain"]}], "]"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"8815ce9d-0e55-4d9e-9df4-dcb242e3ad33"],
 "), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["C", 
    StyleBox["m",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",ExpressionUUID->
  "c22a8ee6-c608-4031-af92-43e4ff7c29ec"],
 " is molar concentration (",
 Cell[BoxData[
  FormBox[
   RowBox[{"mol", "/", 
    SuperscriptBox[
     StyleBox["m",
      FontSlant->"Plain"], "3"]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"6018b6a0-cad5-42d0-ba29-cd8ac430ab9c"],
 "), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["D", "AB"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "23df7f9e-3c26-4f93-a27e-13f8205b2200"],
 " is the diffusivity (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["cm", "2"], "/", 
    StyleBox["s",
     FontSlant->"Plain"]}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "922dbd7d-f366-40c8-8e74-2412cc0f24f7"],
 "), ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["P", "sat"], TraditionalForm]], "InlineMath",
  ExpressionUUID->"6b3e60be-6274-45ee-8980-51062905d497"],
 " is vapor pressure (atm), ",
 Cell[BoxData[
  FormBox["L", TraditionalForm]], "InlineMath",ExpressionUUID->
  "6172d5f9-87a6-4b46-81d5-29528c105458"],
 " is the tube height (m), and ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath",ExpressionUUID->
  "5b437665-ef7e-477b-bcca-d08805bb7ce7"],
 " is total pressure (atm)."
}], "DetailNotes",
 CellChangeTimes->{{3.713879482893546*^9, 3.7138796563660555`*^9}, {
  3.714486788529892*^9, 3.714486791401513*^9}},
 CellID->100117653,ExpressionUUID->"75abd9f3-03bc-4249-bef2-c0f278b5cd3c"],

Cell["The vapor pressure is calculated using the Antoine equation:", \
"DetailNotes",
 CellChangeTimes->{{3.713879482893546*^9, 3.7138796658632956`*^9}},
 CellID->717097745,ExpressionUUID->"33fb4cb2-df85-403b-9610-545b833de59c"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["P", "sat"], "=", 
    RowBox[{"10", "^", 
     RowBox[{"(", 
      RowBox[{"A", "-", 
       FractionBox["B", 
        RowBox[{"T", "+", "C"}]]}], ")"}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "2b286626-7727-4e13-a732-a4f0b5ef5201"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.713879482893546*^9, 3.7138796767329674`*^9}},
 CellID->18518463,ExpressionUUID->"c767725d-681b-4e8c-b279-2e2ee358a672"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "73207df7-0dd7-4b93-900f-9944cbcee278"],
 ", ",
 Cell[BoxData[
  FormBox["B", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "bc6016af-28c0-4c75-93ce-c77da022e3d0"],
 ", and ",
 Cell[BoxData[
  FormBox["C", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "af177ce7-2b9a-4035-8445-78fb34e22fac"],
 " are Antoine constants, and ",
