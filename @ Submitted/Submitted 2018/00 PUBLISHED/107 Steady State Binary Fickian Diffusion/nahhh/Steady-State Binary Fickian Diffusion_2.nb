Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Manipulate", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "Dab", ",", "Cm", ",", "A1", ",", "B1", ",", "C1", ",", "VP", ",", 
       "VPhot", ",", "Jdiff", ",", "Jtot", ",", "yCold", ",", "yA0", ",", 
       "yAL", ",", "yConv", ",", "L2", ",", "p1", ",", "p2"}], "}"}], ",", 
     "\[IndentingNewLine]", " ", 
     RowBox[{
      RowBox[{"Dab", "=", 
       RowBox[{
        RowBox[{"(", " ", 
         RowBox[{"1.17", "*", 
          RowBox[{"10", "^", 
           RowBox[{"-", "9"}]}]}], ")"}], "*", 
        FractionBox[
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"T", "+", "273"}], ")"}], 
          RowBox[{"3", "/", "2"}]], "Ptot"]}]}], ";", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"cm", "^", "2"}], "/", "s"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"Cm", "=", 
       FractionBox["Ptot", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"8.20567", "*", 
           RowBox[{"10", "^", 
            RowBox[{"-", "5"}]}]}], ")"}], "*", 
         RowBox[{"(", 
          RowBox[{"T", "+", "273"}], ")"}]}]]}], ";", 
      RowBox[{"(*", 
       RowBox[{"mol", "/", 
        RowBox[{"m", "^", "3"}]}], "*)"}], "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"A1", "=", "6.90940"}], ";", 
      RowBox[{"B1", "=", "1349.820"}], ";", 
      RowBox[{"C1", "=", "209.385"}], ";", "\[IndentingNewLine]", 
      RowBox[{"VP", "=", 
       RowBox[{
        FractionBox["1", "760"], "*", 
        RowBox[{"10", "^", 
         RowBox[{"(", 
          RowBox[{"A1", "-", 
           FractionBox["B1", 
            RowBox[{"T", "+", "C1"}]]}], ")"}]}]}]}], ";", 
      RowBox[{"(*", 
       RowBox[{
       "using", " ", "antoine", " ", "eq", " ", "to", " ", "get", " ", "VP", 
        " ", "in", " ", "atm"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"VPhot", "=", 
       RowBox[{
        FractionBox["1", "760"], "*", 
        RowBox[{"10", "^", 
         RowBox[{"(", 
          RowBox[{"A1", "-", 
           FractionBox["B1", 
            RowBox[{"T", "+", "C1"}]]}], ")"}]}]}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"Jdiff", "=", 
       FractionBox[
        RowBox[{"Cm", "*", "Dab", "*", "VP"}], 
        RowBox[{"L", "*", "Ptot"}]]}], ";", 
      RowBox[{"(*", 
       RowBox[{"Diffusive", " ", "flux", " ", 
        RowBox[{
         RowBox[{"mol", "/", 
          RowBox[{"m", "^", "2"}]}], "/", "s"}]}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"Jtot", "=", 
       RowBox[{"Dab", "*", 
        FractionBox["Cm", "L"], "*", 
        RowBox[{"Log", "[", 
         FractionBox["1", 
          RowBox[{"1", "-", 
           RowBox[{"(", 
            FractionBox["VP", "Ptot"], ")"}]}]], "]"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"yCold", "[", "z_", "]"}], ":=", 
       RowBox[{
        RowBox[{"-", 
         FractionBox[
          RowBox[{"VP", "*", "z"}], 
          RowBox[{"L", "*", "Ptot"}]]}], "+", 
        FractionBox["VP", "Ptot"]}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"yA0", "=", 
       RowBox[{"VPhot", "/", "Ptot"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"yAL", "=", "0"}], ";", 
      RowBox[{"(*", 
       RowBox[{"boundry", " ", "Condition"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"yConv", "[", "z_", "]"}], ":=", 
       RowBox[{"1", "-", 
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           FractionBox[
            RowBox[{"1", "-", "yAL"}], 
            RowBox[{"1", "-", "yA0"}]], ")"}], 
          RowBox[{"z", "/", "L"}]], "*", 
         RowBox[{"(", 
          RowBox[{"1", "-", "yA0"}], ")"}]}]}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"L2", "=", 
       RowBox[{"0.5", "*", 
        RowBox[{"(", 
         RowBox[{"1", "+", 
          RowBox[{"Rescale", "[", 
           RowBox[{"L", ",", 
            RowBox[{"{", 
             RowBox[{"0", ",", "120"}], "}"}]}], "]"}]}], ")"}]}]}], ";", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"height", " ", "of", " ", "vapor"}], ",", " ", 
        RowBox[{"not", " ", "sure", " ", "if", " ", 
         RowBox[{"right", ".", " ", "Rescale"}], " ", "rescales", " ", 
         "range", " ", "from", " ", "0", " ", "to", " ", "1"}]}], "*)"}], 
      "\[IndentingNewLine]", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"ParametricPlot3D", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Cos", "[", "u", "]"}], ",", 
          RowBox[{"Sin", "[", "u", "]"}], ",", 
          RowBox[{"2", "v"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"u", ",", "0", ",", 
          RowBox[{"2", "Pi"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"v", ",", "0", ",", "L2"}], "}"}], ",", 
        RowBox[{"ColorFunction", "\[Rule]", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"Opacity", "[", 
            RowBox[{
             RowBox[{"0.2", "+", 
              RowBox[{"0.8", "*", 
               RowBox[{"Rescale", "[", 
                RowBox[{
                 RowBox[{"If", "[", 
                  RowBox[{
                   RowBox[{
                    FractionBox["Jdiff", "Jtot"], " ", ">", " ", "0.9"}], ",", 
                   RowBox[{"yCold", "[", "#", "]"}], ",", 
                   RowBox[{"yConv", "[", "#", "]"}]}], "]"}], ",", 
                 RowBox[{"{", 
                  RowBox[{"0.004645", ",", "0.8523"}], "}"}]}], "]"}]}]}], 
             ",", 
             RowBox[{"RGBColor", "[", 
              RowBox[{"0", ",", "0.6", ",", "0"}], "]"}]}], "]"}], "&"}], 
          ")"}]}], ",", 
        RowBox[{"ColorFunctionScaling", "\[Rule]", "False"}], ",", 
        RowBox[{"BoundaryStyle", "\[Rule]", "Black"}], ",", 
        RowBox[{"Mesh", "\[Rule]", "None"}]}], "]"}]}]}], 
    "\[IndentingNewLine]", "]"}], ",", "\[IndentingNewLine]", 
   RowBox[{"Grid", "[", 
    RowBox[{
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
