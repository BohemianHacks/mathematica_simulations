   3.699196965079908*^9}, {3.699197003333939*^9, 3.6991970691557302`*^9}, {
   3.699197203656665*^9, 3.6991972141343613`*^9}, {3.699273877243849*^9, 
   3.699273905760281*^9}},
 CellID->685182713,ExpressionUUID->"e74d6618-0614-4672-8f49-3feeef103dfe"],

