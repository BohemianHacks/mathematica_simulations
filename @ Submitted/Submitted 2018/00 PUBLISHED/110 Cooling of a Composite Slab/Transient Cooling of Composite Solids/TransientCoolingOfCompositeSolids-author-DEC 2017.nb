  FormBox[
   RowBox[{
    SubscriptBox["T", "\[Infinity]"], "=", "0"}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"b5ac8461-4b72-403f-84bf-9c255115bf74"],
