Cell[CellGroupData[{

Cell["", "DetailsSection",ExpressionUUID->"2d8a56b8-eba5-4b8d-99a8-0d03754f1fae"],

Cell[TextData[{
 "The saturation pressure of component ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "=", 
    RowBox[{"(", 
     RowBox[{"1", ",", "2"}], ")"}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "26d7e040-75ca-4b1b-9319-2ce28a4a253e"],
 " are calculated using the Antoine equation:"
}], "DetailNotes",
 CellChangeTimes->{{3.72408646939902*^9, 3.7240865126857257`*^9}, {
  3.7240869550127783`*^9, 3.7240869561177855`*^9}},
 CellID->527391816,ExpressionUUID->"63ec3571-98a4-42c8-a930-5c4716d49f0c"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["P", "i", "sat"], "=", 
    RowBox[{"10", "^", 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["A", "i"], "-", 
       FractionBox[
        SubscriptBox["B", "i"], 
        RowBox[{"T", "+", 
         SubscriptBox["C", "i"]}]]}], ")"}]}]}], TraditionalForm]], 
  "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "27442e4f-5c45-425b-874f-e60d7658e0d3"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.72408646939902*^9, 3.7240865319978657`*^9}},
 CellID->392526358,ExpressionUUID->"2a20bb43-fd28-40d7-81b7-c12e43e5975e"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "=", "1"}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "882ef0e3-d28f-4c4f-a536-babd7f29a0d4"],
 " for ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "96abeb6e-2f78-4d7f-8304-be193f7d752e"],
 "-hexane, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "=", "2"}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "09b4a926-698a-48dc-8e69-b6b05ef79697"],
 " for ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "67f9aacf-f40e-4777-9bc4-48a463a9273c"],
 "-octane, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "i"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "0f15cf36-1872-43b0-bd81-19105d289e81"],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["B", "i"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "41d3c6bb-d4a9-4817-a568-1a7dd5f413bd"],
 ", and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["C", "i"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "2a58a4dc-74d9-4d6e-894e-3ca9aca2031c"],
 " are the Antoine constants, ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["P", "i", "sat"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "567b4025-92ba-4b1b-a49d-544e6cf9ab90"],
 " is saturation pressure (bar), and ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "134f8250-3b69-419c-b851-1ffe2ab312aa"],
 " is temperature (\[Degree]C)."
}], "DetailNotes",
 CellChangeTimes->{{3.72408646939902*^9, 3.7240865846194663`*^9}, {
  3.7240869622902327`*^9, 3.724086979318178*^9}},
 CellID->525649404,ExpressionUUID->"438f0b5c-a9f4-401a-a6a2-cedf3d3808b0"],

Cell[TextData[{
 "Raoult's law is used to calculate the bubble-point and dew-point pressures \
using ",
 Cell[BoxData[
  FormBox["K", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "25c19d32-3dbf-4072-a3a5-84cba303f30f"],
 " factors:"
}], "DetailNotes",
 CellChangeTimes->{{3.72408646939902*^9, 3.724086615757695*^9}},
 CellID->63088248,ExpressionUUID->"463449a1-166c-4ee0-82c2-d0eeb52c1df3"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["K", "i"], "=", 
    RowBox[{
     FractionBox[
      SubscriptBox["y", "i"], 
      SubscriptBox["x", "i"]], "=", 
     FractionBox[
      SubsuperscriptBox["P", "i", "sat"], "P"]}]}], TraditionalForm]], 
  "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "008dc46c-878c-412d-aabc-cf851dbcf902"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.72408646939902*^9, 3.724086642689494*^9}},
 CellID->365286202,ExpressionUUID->"91a9ac6b-bae0-4100-b87c-5ebaed2d4fca"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["y", "i"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "3f9c2b89-48bb-4082-8e16-c1557196372f"],
 " is the vapor mole fraction ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["y", "1"], "+", 
     SubscriptBox["y", "2"]}], "=", "1"}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "92706376-7858-4f4e-b4e0-91772a4a1b98"],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "i"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "3c8d0269-deb5-4df7-8be0-a27488fbd064"],
 " is the liquid mole fraction ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["x", "1"], "+", 
     SubscriptBox["x", "2"]}], "=", "1"}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "7bab5ca6-2c66-4b63-8fdf-785cbcb74e29"],
 ", and ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "37033833-73bd-4fde-930a-e531f1f9e960"],
 " is the total pressure (bar)."
}], "DetailNotes",
 CellChangeTimes->{{3.72408646939902*^9, 3.7240866511727667`*^9}, {
  3.7240868388186426`*^9, 3.724086892304064*^9}, {3.7240869392992487`*^9, 
  3.724086950792344*^9}},
 CellID->9096200,ExpressionUUID->"2d9bf49f-1240-4786-bef3-633861c10514"],

Cell[TextData[{
 "The bubble-point pressure curve is calculated using ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Sum]", 
     RowBox[{
      SubscriptBox["K", "i"], " ", 
      SubscriptBox["x", "i"]}]}], "=", "1"}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "2a58b59b-1e9a-47d2-bc0a-3d63e4a0c251"],
 ":"
}], "DetailNotes",
 CellChangeTimes->{{3.72408646939902*^9, 3.7240866511727667`*^9}, {
  3.7240868388186426`*^9, 3.7240869164916635`*^9}},
 CellID->122311603,ExpressionUUID->"e3a8d6d0-665b-4c33-aef5-8ef86c49f43e"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", "1"], " ", 
      SubsuperscriptBox["P", "1", "sat"]}], "+", 
     RowBox[{
      SubscriptBox["x", "x"], " ", 
      SubsuperscriptBox["P", "2", "sat"]}]}]}], TraditionalForm]], 
  "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "c19b7902-94fe-4022-9bbf-a39f3ec94ab0"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.72408646939902*^9, 3.7240866511727667`*^9}, {
  3.7240868388186426`*^9, 3.7240869369760256`*^9}},
 CellID->359768648,ExpressionUUID->"882166b4-50fa-467c-b7d7-52540e620863"],

Cell[TextData[{
 "THe dew-point pressure curve is calculated using ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Sum]", 
     RowBox[{
      SubscriptBox["y", "i"], "/", 
      SubscriptBox["K", "i"]}]}], "=", "1"}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "c48cc7cd-065a-425d-a98d-21d851c0ccbb"],
 ":"
}], "DetailNotes",
 CellChangeTimes->{{3.7240869849956226`*^9, 3.724087002790353*^9}},
 CellID->191284873,ExpressionUUID->"8e747cd4-d2cf-40eb-8d59-cf0063dfb59c"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       FractionBox[
        SubscriptBox["y", "1"], 
        SubsuperscriptBox["P", "1", "sat"]], "+", 
       FractionBox[
        SubscriptBox["y", "2"], 
        SubsuperscriptBox["P", "2", "sat"]]}], ")"}], 
     RowBox[{"-", "1"}]]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "ee61c4a7-d7df-419b-8438-00bdb6a16a66"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.7240869849956226`*^9, 3.724087027338956*^9}},
 CellID->31383769,ExpressionUUID->"773782a0-1ed2-4f36-a420-364930cd80a9"],

