Cell[TextData[{
 "The saturation pressure of component ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "=", 
    RowBox[{"(", 
     RowBox[{"1", ",", "2"}], ")"}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "9b736bd6-183c-4772-a62e-54030f179825"],
 " is calculated using the Antoine equation:"
}], "DetailNotes",
 CellChangeTimes->{{3.724166834572256*^9, 3.7241668510609508`*^9}},
 CellID->124708632,ExpressionUUID->"de57c868-026c-4f94-a772-82cd05783ffc"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["P", "i", "sat"], "=", 
    RowBox[{"10", "^", 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["A", "i"], "-", 
       FractionBox[
        SubscriptBox["B", "i"], 
        RowBox[{"T", "+", 
         SubscriptBox["C", "i"]}]]}], ")"}]}]}], TraditionalForm]], 
  "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "338c2b4b-ea73-4476-a7e0-ebd6248b90ee"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.724166834572256*^9, 3.7241668686558294`*^9}},
 CellID->322382538,ExpressionUUID->"5731ae08-08e8-40be-b119-ffeefa419c69"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "i"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "3d80fa22-bf5f-4ebd-9cde-618eb10028bd"],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["B", "i"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "34bcd56d-81a0-4b23-9be4-54fc52b0c1cc"],
 ", and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["C", "i"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "41e59ee4-4281-43df-b3e6-0b95509c58b7"],
 " are Antoine constants, ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["P", "i", "sat"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "15df67a0-7e8b-431b-8452-f7fbdcf0f062"],
 " is saturation pressure (bar), and ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "b79a0d0a-c184-4c47-9295-6ae7b327c05b"],
 " is temperature (\[Degree]C)."
}], "DetailNotes",
 CellChangeTimes->{{3.724166834572256*^9, 3.7241669044008074`*^9}},
 CellID->170877755,ExpressionUUID->"b34c9f67-8a4f-4ec1-bd48-0036cc9486bc"],

Cell[TextData[{
 "THe two-parameter Margules model is used to calculate the activity \
coefficients for a non-ideal liquid mixture of components ",
 Cell[BoxData[
  FormBox["1", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "eaca8f3b-d80e-4e54-8ac7-6f6ab954b563"],
 " and ",
 Cell[BoxData[
  FormBox["2", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "5a30af6a-bbbc-46bb-8d80-8cab432e9c89"],
 ". This model fits the excess Gibbs free energy:"
}], "DetailNotes",
 CellChangeTimes->{{3.724166919706081*^9, 3.724166965108123*^9}},
 CellID->17041983,ExpressionUUID->"19d39456-cf5f-42b7-aa56-39ffa4a40b10"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     SuperscriptBox["G", "E"], 
     RowBox[{"R", " ", "T"}]], "=", 
    RowBox[{
     SubscriptBox["x", "1"], " ", 
     SubscriptBox["x", "2"], " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        SubscriptBox["A", "21"], " ", 
        SubscriptBox["x", "1"]}], "+", 
       RowBox[{
        SubscriptBox["A", "12"], " ", 
        SubscriptBox["x", "2"]}]}], ")"}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "43b1ad8e-e26c-4cf6-ae27-6035e23b74d2"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.724166919706081*^9, 3.724166990319048*^9}},
 CellID->35696391,ExpressionUUID->"79025dc6-d957-468d-89d2-9d7f58325636"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["G", "E"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "703a1fb3-1fec-489c-8664-6e8bd5759148"],
 " is Gibbs energy, and ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "7b1d07f4-b7a6-4828-bba1-a91789f11494"],
 " is the gas constant."
}], "DetailNotes",
 CellChangeTimes->{{3.724166919706081*^9, 3.7241670067655244`*^9}},
 CellID->472114,ExpressionUUID->"f8d037e4-d049-4317-b909-79328bf54144"],

Cell["THe activity coefficients are calculated:", "DetailNotes",
 CellChangeTimes->{{3.724166919706081*^9, 3.724167024788207*^9}},
 CellID->290513101,ExpressionUUID->"4b397bbf-26ef-4aaf-b8c3-ad1f4ad8c5d4"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"ln", " ", 
     SubscriptBox["\[Gamma]", "1"]}], "=", 
    RowBox[{
     SubsuperscriptBox["x", "2", "2"], " ", 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["A", "12"], "+", 
       RowBox[{"2", " ", 
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["A", "21"], "-", 
          SubscriptBox["A", "12"]}], ")"}], " ", 
        SubscriptBox["x", "1"]}]}], ")"}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "e2e5f497-57f1-426c-b303-5cdf89f133c9"],
 ","
}], "DetailNotes",
