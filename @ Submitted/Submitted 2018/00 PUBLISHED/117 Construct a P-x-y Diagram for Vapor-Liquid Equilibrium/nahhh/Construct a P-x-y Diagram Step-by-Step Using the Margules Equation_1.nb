 CellID->269996304,ExpressionUUID->"c0d2a376-7647-4d12-9bf7-c08ee8625298"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"ln", " ", 
     SubscriptBox["\[Gamma]", "2"]}], "=", 
    RowBox[{
     SubsuperscriptBox["x", "1", "2"], " ", 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["A", "21"], "+", 
       RowBox[{"2", " ", 
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["A", "12"], "-", 
          SubscriptBox["A", "21"]}], ")"}], " ", 
        SubscriptBox["x", "2"]}]}], ")"}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "7a0681a9-daa8-41e2-9922-e6cb099ed66f"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.724166919706081*^9, 3.724167075684286*^9}},
 CellID->69750176,ExpressionUUID->"20082e3b-7843-4628-879b-9f1a0e44a2b5"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "i"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "efbfc049-9141-4a92-958b-e4dcee9a3a82"],
 " is the liquid mole fraction and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["x", "1"], "+", 
     SubscriptBox["x", "2"]}], "=", "1"}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "0a62e2f1-7e64-4155-914a-7897bcd45dfc"],
 ", and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "12"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "0fa1842a-baa5-4115-9157-eb7a1daf36b2"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "21"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "ae1fe50c-0df0-4022-8c47-c0c9577ab403"],
 " are the Margules parameters."
}], "DetailNotes",
 CellChangeTimes->{{3.724166919706081*^9, 3.724167107011836*^9}},
 CellID->141949186,ExpressionUUID->"2bf99dd2-1ae9-47ba-9ce4-f0682608639b"],

Cell[TextData[{
 "The modified Raoult's law is used to calculate the bubble-point and \
dew-point pressures using ",
 Cell[BoxData[
  FormBox["K", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "a663a4a3-5545-495a-a5e7-92b53705c3d7"],
 " factors:"
}], "DetailNotes",
 CellChangeTimes->{{3.724166919706081*^9, 3.7241671399737887`*^9}},
 CellID->197714283,ExpressionUUID->"0887f1e2-76cd-4edf-b4d3-118e45c0878f"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["K", "i"], "=", 
    RowBox[{
     FractionBox[
      SubscriptBox["y", "i"], 
      SubscriptBox["x", "i"]], "=", 
     RowBox[{
      SubscriptBox["\[Gamma]", "i"], " ", 
      FractionBox[
       SubsuperscriptBox["P", "i", "sat"], "P"]}]}]}], TraditionalForm]], 
  "InlineMath",
