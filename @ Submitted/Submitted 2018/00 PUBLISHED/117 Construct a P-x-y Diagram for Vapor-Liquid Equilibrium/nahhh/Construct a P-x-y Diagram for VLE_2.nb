Cell[CellGroupData[{

Cell["", "DetailsSection",ExpressionUUID->"493f6ba9-fd8d-4594-9a91-3134545096b8"],

Cell[TextData[{
 "The saturation pressure of each component ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "=", 
    RowBox[{"(", 
     RowBox[{"1", ",", "2"}], ")"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"fed73e28-47c1-4c2a-bd19-fc51d737eae8"],
 " is calculated using the Antoine equation:"
}], "DetailNotes",
 CellChangeTimes->{{3.72408646939902*^9, 3.7240865126857257`*^9}, {
  3.7240869550127783`*^9, 3.7240869561177855`*^9}, {3.7268460389673443`*^9, 
  3.726846044941232*^9}},
 CellID->527391816,ExpressionUUID->"1105a416-e3fb-4a30-a6b5-f5502c506ddb"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["P", "i", "sat"], "=", 
    RowBox[{"10", "^", 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["A", "i"], "-", 
       FractionBox[
        SubscriptBox["B", "i"], 
        RowBox[{"T", "+", 
         SubscriptBox["C", "i"]}]]}], ")"}]}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"9f6640c3-7afb-41be-9dda-c95089d1c1fd"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.72408646939902*^9, 3.7240865319978657`*^9}},
 CellID->392526358,ExpressionUUID->"1f20d832-eb76-443d-aceb-26ff33a883a6"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "=", "1"}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "10b125b0-99a3-4dfa-bcfe-0d94b113e4db"],
 " for ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath",ExpressionUUID->
  "373e3598-5d74-4651-872e-76245c015f1b"],
 "-hexane, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "=", "2"}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "2991b4dd-acde-423c-9a8c-ecd86aaac931"],
 " for ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath",ExpressionUUID->
  "4567fb81-9c89-4775-ba08-13ad2fa195f9"],
 "-octane, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "i"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "a884cb25-ec86-4d8b-a25f-ea6f068019ab"],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["B", "i"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "0da007f3-759f-4a02-b967-88f60e5f830f"],
 ", and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["C", "i"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "6d26e117-67cc-4287-9df2-42a965643052"],
 " are the Antoine constants, ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["P", "i", "sat"], TraditionalForm]], "InlineMath",
  ExpressionUUID->"360196e6-158e-447c-8fae-2da5a03ea3ef"],
 " is saturation pressure (bar), and ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath",ExpressionUUID->
  "9cfb7170-2f7c-4cfe-bdf7-f14e39390172"],
 " is temperature (\[Degree]C)."
}], "DetailNotes",
 CellChangeTimes->{{3.72408646939902*^9, 3.7240865846194663`*^9}, {
  3.7240869622902327`*^9, 3.724086979318178*^9}},
 CellID->525649404,ExpressionUUID->"9f992411-4a26-4918-ae6e-3cd8590f2749"],

Cell[TextData[{
 "Raoult's law is used to calculate the bubble-point and dew-point pressures \
using ",
 Cell[BoxData[
  FormBox["K", TraditionalForm]], "InlineMath",ExpressionUUID->
  "658867f7-ddce-4cc9-a689-731ff57a9452"],
 " factors:"
}], "DetailNotes",
 CellChangeTimes->{{3.72408646939902*^9, 3.724086615757695*^9}},
 CellID->63088248,ExpressionUUID->"e9018857-9007-4f0e-901c-1c1c998c1bbd"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["K", "i"], "=", 
    RowBox[{
     FractionBox[
      SubscriptBox["y", "i"], 
      SubscriptBox["x", "i"]], "=", 
     FractionBox[
      SubsuperscriptBox["P", "i", "sat"], "P"]}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"efdde012-50cb-4ce4-907d-4f730ffd512c"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.72408646939902*^9, 3.724086642689494*^9}},
 CellID->365286202,ExpressionUUID->"b0b8588b-05a9-4f52-bce8-bb7c623cd5a2"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["y", "i"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "f717345c-a41d-4fe2-bc93-55b8c599e41c"],
 " is the vapor mole fraction (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["y", "1"], "+", 
     SubscriptBox["y", "2"]}], "=", "1"}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"0a775693-f47f-4bf9-be1b-a60d63e88459"],
 "), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "i"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "a0270039-f714-46c3-851d-2cd6543657ff"],
 " is the liquid mole fraction (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["x", "1"], "+", 
     SubscriptBox["x", "2"]}], "=", "1"}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"281d9e74-8e35-41a0-88ff-be2805e3e966"],
 "), and ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath",ExpressionUUID->
  "b3d99a33-5b70-4242-86a5-b9fc6b95620d"],
 " is the total pressure (bar)."
}], "DetailNotes",
 CellChangeTimes->{{3.72408646939902*^9, 3.7240866511727667`*^9}, {
  3.7240868388186426`*^9, 3.724086892304064*^9}, {3.7240869392992487`*^9, 
  3.724086950792344*^9}, {3.7268461103261285`*^9, 3.726846135804891*^9}},
 CellID->9096200,ExpressionUUID->"905b2c58-e159-451e-8284-304d0f317c56"],

Cell[TextData[{
 "The bubble-point pressure curve is calculated using ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Sum]", 
     RowBox[{
      SubscriptBox["K", "i"], " ", 
      SubscriptBox["x", "i"]}]}], "=", "1"}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"2e8618a7-c436-40f3-bfaa-e6735f289d73"],
 ":"
}], "DetailNotes",
 CellChangeTimes->{{3.72408646939902*^9, 3.7240866511727667`*^9}, {
  3.7240868388186426`*^9, 3.7240869164916635`*^9}},
 CellID->122311603,ExpressionUUID->"2265c818-dd3d-4743-9102-398f7141eaf8"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", "1"], " ", 
      SubsuperscriptBox["P", "1", "sat"]}], "+", 
     RowBox[{
      SubscriptBox["x", "x"], " ", 
      SubsuperscriptBox["P", "2", "sat"]}]}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"d2841732-7c06-442e-b563-8f505f2bab1f"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.72408646939902*^9, 3.7240866511727667`*^9}, {
  3.7240868388186426`*^9, 3.7240869369760256`*^9}},
 CellID->359768648,ExpressionUUID->"fc5874af-6ef5-4c72-b038-a337d12792ef"],

Cell[TextData[{
 "THe dew-point pressure curve is calculated using ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Sum]", 
     RowBox[{
      SubscriptBox["y", "i"], "/", 
      SubscriptBox["K", "i"]}]}], "=", "1"}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"16404b06-e2bd-4e77-af8b-be750e2a4c06"],
 ":"
}], "DetailNotes",
 CellChangeTimes->{{3.7240869849956226`*^9, 3.724087002790353*^9}},
 CellID->191284873,ExpressionUUID->"97ed1701-1ce3-46e8-80b4-ae2e8992ad77"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       FractionBox[
        SubscriptBox["y", "1"], 
        SubsuperscriptBox["P", "1", "sat"]], "+", 
       FractionBox[
        SubscriptBox["y", "2"], 
        SubsuperscriptBox["P", "2", "sat"]]}], ")"}], 
     RowBox[{"-", "1"}]]}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "f02b95f1-c7db-40d4-a375-aec6ab54d1ed"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.7240869849956226`*^9, 3.724087027338956*^9}},
 CellID->31383769,ExpressionUUID->"0c719a12-3327-4c5f-8d0e-f078b135fa76"],

