Cell[TextData[{
 "The saturation pressure of component ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "=", 
    RowBox[{"(", 
     RowBox[{"1", ",", "2"}], ")"}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "26d7e040-75ca-4b1b-9319-2ce28a4a253e"],
