Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["K", "i"], "=", 
    RowBox[{
     FractionBox[
      SubscriptBox["y", "i"], 
      SubscriptBox["x", "i"]], "=", 
     FractionBox[
      SubsuperscriptBox["P", "i", "sat"], "P"]}]}], TraditionalForm]], 
