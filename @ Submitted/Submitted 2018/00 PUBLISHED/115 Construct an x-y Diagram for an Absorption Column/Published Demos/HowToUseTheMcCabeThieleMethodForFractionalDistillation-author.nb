Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["y", 
      RowBox[{"n", "+", "1"}]], " ", "V"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", "n"], " ", "L"}], "+", 
     RowBox[{
      SubscriptBox["x", 
       StyleBox["D",
        FontSlant->"Plain"]], " ", "D"}]}]}], TraditionalForm]], "InlineMath"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.690214542559614*^9, 3.6902146979853153`*^9}, {
