Cell[TextData[{
 "rearranging the mass balance to solve for ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["y", 
    RowBox[{"N", "+", "1"}]], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "fa3c117f-c4bf-4975-bac1-b4972e496147"],
 ":"
}], "DetailNotes",
