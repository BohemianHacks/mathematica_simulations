Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", "op"], "=", 
    RowBox[{
     SubscriptBox["y", 
      RowBox[{"N", "+", "1"}]], "=", 
     RowBox[{
      RowBox[{
       FractionBox["L", "V"], " ", 
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["x", "N"], "-", 
         SubscriptBox["x", "0"]}], ")"}]}], "+", 
      SubscriptBox["y", "1"]}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "08d20250-844b-45c9-86c4-1f0af4a393c7"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.723993868002286*^9, 3.723993881804785*^9}, {
  3.723993913068585*^9, 3.7239939715261903`*^9}},
 CellID->34378748,ExpressionUUID->"4432c035-6ee6-4236-8b35-18869e0d2d80"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "0"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "359099b1-3a03-4f79-a42f-8b3aa5243e93"],
 " is the mole ratio of impurity in the inlet liquid stream (ppm), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["y", 
    RowBox[{"N", "+", "1"}]], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "cc249a8c-b765-4bb5-805f-4ffb0f97af21"],
 " is the mole ratio of impurity in the inlet vapor stream (ppm), ",
