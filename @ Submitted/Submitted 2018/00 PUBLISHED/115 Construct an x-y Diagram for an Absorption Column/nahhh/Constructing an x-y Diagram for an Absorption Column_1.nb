Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["x", "0"], " ", "L"}], "+", 
     RowBox[{
      SubscriptBox["y", 
       RowBox[{"N", "+", "1"}]], " ", "V"}]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", "N"], " ", "L"}], "+", 
     RowBox[{
      SubscriptBox["y", "1"], " ", "V"}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "04600c97-af4b-4e76-8f02-87c1dcf6dc36"],
