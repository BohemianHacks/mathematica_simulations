   RowBox[{
    SubscriptBox["F", 
     StyleBox["A",
      FontSlant->"Plain"]], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        SubscriptBox["F", 
         StyleBox["R",
          FontSlant->"Plain"]], " ", 
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["y", 
           StyleBox["R",
            FontSlant->"Plain"]], "-", 
          RowBox[{
           RowBox[{"d", "/", "sin"}], " ", "\[Theta]"}]}], ")"}]}], "+", 
       RowBox[{"W", " ", 
        RowBox[{
         RowBox[{"h", "/", "2"}], "/", "sin"}], " ", "\[Theta]"}]}], ")"}], 
     "/", "h"}]}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "1322c08b-a681-4758-b97b-038418f328ca"],
 ","
}], "DetailNotes",
