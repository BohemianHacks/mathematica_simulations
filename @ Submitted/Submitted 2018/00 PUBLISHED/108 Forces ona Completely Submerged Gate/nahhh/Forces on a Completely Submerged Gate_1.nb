 CellID->386529268,ExpressionUUID->"a9252277-f83f-487d-94cf-4f3b049f534b"],

Cell[TextData[{
 "therefore since ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", 
     StyleBox["R",
      FontSlant->"Plain"]], "=", 
    RowBox[{"\[Gamma]", " ", 
     SubscriptBox["y", 
      StyleBox["c",
       FontSlant->"Plain"]], " ", "sin", " ", "\[Theta]", " ", "A"}]}], 
   TraditionalForm]], "InlineMath",ExpressionUUID->
  "cf445cac-1a48-4228-9750-a5c08623efd1"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", 
     StyleBox["R",
      FontSlant->"Plain"]], "=", 
    FractionBox[
     RowBox[{
      SubscriptBox["\[Integral]", "A"], 
      RowBox[{
       SuperscriptBox["y", "2"], 
       RowBox[{"\[DifferentialD]", "A"}]}]}], 
     RowBox[{
      SubscriptBox["y", 
       StyleBox["c",
        FontSlant->"Plain"]], " ", "A"}]]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"22f61255-3560-46bb-a008-f60c6de2c345"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.7184615439536395`*^9, 3.7184615923254843`*^9}, {
  3.718463239920003*^9, 3.7184638888237844`*^9}},
 CellID->606321689,ExpressionUUID->"da15197f-81c2-4456-9f67-09ecf62a9a2b"],

Cell[TextData[{
 "The integral in the numerator is the second moment of the area (moment of \
inertia) ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["I", "x"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "40d2f4af-355e-4823-87ac-a20e2693cb34"],
 ", with respect to an axis formed by the intersection of a plane containing \
the surface and the free surface (",
 Cell[BoxData[
  FormBox["x", TraditionalForm]], "InlineMath",ExpressionUUID->
  "4d0296d8-c580-46dd-a7c6-cd3d074a07ff"],
 "-axis). Thus we can write:"
}], "DetailNotes",
 CellChangeTimes->{{3.7184615439536395`*^9, 3.7184615923254843`*^9}, {
  3.718463239920003*^9, 3.718463975989419*^9}},
 CellID->57137808,ExpressionUUID->"0ee1f617-f3f7-4cce-8b59-5ad58830c6fa"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", 
     StyleBox["R",
      FontSlant->"Plain"]], "=", 
    FractionBox[
     SubscriptBox["I", "x"], 
     RowBox[{
      SubscriptBox["y", 
       StyleBox["c",
        FontSlant->"Plain"]], " ", "A"}]]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"127975ce-e278-48d4-8916-c8b9e05ddbb4"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.7184615439536395`*^9, 3.7184615923254843`*^9}, {
  3.718463239920003*^9, 3.7184640038575473`*^9}},
 CellID->57975750,ExpressionUUID->"63233f22-97f3-49be-92cf-5aa7c9fc5ffc"],

Cell[TextData[{
 "The parallel axis theorem is used to define ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["I", "x"], "=", 
    RowBox[{
     SubscriptBox["I", "xc"], "+", 
     RowBox[{"A", " ", 
      SubsuperscriptBox["y", 
       StyleBox["c",
        FontSlant->"Plain"], "2"]}]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"7cbe1b78-e0ec-48be-bb11-4cb9790d3561"],
 " where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["I", "xc"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "61d3f4d4-76c4-4c69-bcbb-71230d0c1eb3"],
 " is the second moment of the area with respect to an axis passing through \
its centroid and parallel to the ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]], "InlineMath",ExpressionUUID->
  "4babe599-8aaa-463d-8b57-7d39ec590fc2"],
 "-axis, thus:"
}], "DetailNotes",
 CellChangeTimes->{{3.7184615439536395`*^9, 3.7184615923254843`*^9}, {
  3.718463239920003*^9, 3.7184640838051405`*^9}},
 CellID->544912,ExpressionUUID->"a11398a6-1dfc-4494-b2c4-d346a3efd524"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", 
     StyleBox["R",
      FontSlant->"Plain"]], "=", 
    RowBox[{
     FractionBox[
      SubscriptBox["I", "xc"], 
      RowBox[{
       SubscriptBox["y", 
        StyleBox["c",
         FontSlant->"Plain"]], " ", "A"}]], "+", 
     SubscriptBox["y", 
      StyleBox["c",
       FontSlant->"Plain"]]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"3581e4ae-f691-4a72-a000-8a199accf821"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.7184615439536395`*^9, 3.7184615923254843`*^9}, {
  3.718463239920003*^9, 3.7184641078572626`*^9}},
 CellID->150309794,ExpressionUUID->"5764616b-d78e-46bb-99ba-9a3e21549fd3"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["I", "xc"], "=", 
    RowBox[{
     FractionBox["1", "12"], " ", 
     SubscriptBox["w", "2"], " ", 
     SuperscriptBox["h", "3"]}]}], TraditionalForm]], "InlineMath",
