Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Sum]", "M"}], "=", 
    RowBox[{"0", "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["F", 
        StyleBox["R",
         FontSlant->"Plain"]], " ", 
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["y", 
          StyleBox["R",
           FontSlant->"Plain"]], "-", "d"}], ")"}]}], "-", 
      RowBox[{
       FractionBox["W", 
        RowBox[{"cos", " ", "\[Theta]"}]], " ", 
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["y", 
          StyleBox["c",
           FontSlant->"Plain"]], "-", "d"}], ")"}]}], "-", 
      RowBox[{
       SubscriptBox["F", 
        StyleBox[
         RowBox[{"A", " "}],
         FontSlant->"Plain"]], " ", "h"}]}]}]}], TraditionalForm]], 
  "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "00d94c2c-a3ca-49ce-925f-c7fdedbd51e9"],
