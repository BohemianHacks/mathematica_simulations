 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", 
     StyleBox["A",
      FontSlant->"Plain"]], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        SubscriptBox["F", 
         StyleBox["R",
          FontSlant->"Plain"]], " ", 
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["y", 
           StyleBox["R",
            FontSlant->"Plain"]], "-", 
          RowBox[{
           RowBox[{"d", "/", "sin"}], " ", "\[Theta]"}]}], ")"}]}], "+", 
       RowBox[{"W", " ", 
        RowBox[{
         RowBox[{"h", "/", "2"}], "/", "sin"}], " ", "\[Theta]"}]}], ")"}], 
     "/", "h"}]}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "60f63ada-8566-4a36-ae5d-591c425fb20c"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.7184615439536395`*^9, 3.7184615923254843`*^9}, {
