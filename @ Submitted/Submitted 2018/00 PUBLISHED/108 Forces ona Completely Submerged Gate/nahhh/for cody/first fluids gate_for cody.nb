  FormBox[
   RowBox[{
    StyleBox["N",
     FontSlant->"Plain"], "/", 
    SuperscriptBox[
     StyleBox["m",
      FontSlant->"Plain"], "3"]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"f08a84dd-0e18-424e-878d-cf88c68122ab"],
