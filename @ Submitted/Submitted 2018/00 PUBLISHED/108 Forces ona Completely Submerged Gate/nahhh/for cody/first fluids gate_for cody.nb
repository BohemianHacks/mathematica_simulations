  3.715003643373125*^9, 3.715003674328498*^9}, {3.7150049877334642`*^9, 
  3.7150050252052526`*^9}},
 CellID->23676269,ExpressionUUID->"ba43e1e2-f67d-43d3-9363-51263db4ef3b"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", 
     StyleBox["R",
      FontSlant->"Plain"]], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[Integral]", "A"], " ", 
      RowBox[{"\[Gamma]", " ", "h", " ", 
       RowBox[{"\[DifferentialD]", "A"}]}]}], "=", 
     RowBox[{
      SubscriptBox["\[Integral]", "A"], " ", 
      RowBox[{"\[Gamma]", " ", "y", " ", "sin", " ", "\[Theta]", " ", 
       RowBox[{"\[DifferentialD]", "A"}]}]}]}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"cf4dadec-9d80-4883-a031-44e1d3da3c9e"],
 ","
}], "DetailNotes",
