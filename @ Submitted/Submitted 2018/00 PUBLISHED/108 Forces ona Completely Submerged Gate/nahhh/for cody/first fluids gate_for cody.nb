  FormBox[
   RowBox[{"h", "=", 
    RowBox[{"y", " ", "sin", " ", "\[Theta]"}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"5eac5c7d-60bd-41d1-bd4e-c7ea404d3590"],
