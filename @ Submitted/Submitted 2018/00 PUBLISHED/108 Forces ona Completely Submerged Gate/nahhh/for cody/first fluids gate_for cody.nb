  FormBox[
   SubscriptBox["F", 
    StyleBox["R",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",ExpressionUUID->
  "2e672e3c-2b51-4581-a2c4-16796fd107ba"],
