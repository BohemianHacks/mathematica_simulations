Cell[TextData[{
 "The ",
 Cell[BoxData[
  FormBox["y", TraditionalForm]], "InlineMath",ExpressionUUID->
  "4745ce93-e14e-48a9-8255-23521a90475c"],
 "-coordinate ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["y", 
    StyleBox["R",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",ExpressionUUID->
  "a5157b1d-3079-41d4-899a-fd9e5319f213"],
