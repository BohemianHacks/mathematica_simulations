Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", 
     StyleBox["R",
      FontSlant->"Plain"]], "=", 
    RowBox[{
     RowBox[{"\[Gamma]", " ", 
      SubscriptBox["y", 
       StyleBox["c",
        FontSlant->"Plain"]], " ", "sin", " ", "\[Theta]", " ", "A"}], "=", 
     RowBox[{"\[Gamma]", " ", 
