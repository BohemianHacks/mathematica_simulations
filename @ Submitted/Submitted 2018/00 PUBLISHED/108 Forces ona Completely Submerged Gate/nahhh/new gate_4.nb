Cell[TextData[{
 "The integral ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Integral]", "A"], 
    RowBox[{"y", 
     RowBox[{"\[DifferentialD]", "A"}]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"d92664dc-c762-404b-bcec-88ea9929d465"],
 " is the first moment of the area with respect to the ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]], "InlineMath",ExpressionUUID->
  "62eeb04c-d490-4f4e-8dde-a4cbcf84cd71"],
 "-axis, so:"
}], "DetailNotes",
