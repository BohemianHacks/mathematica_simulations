Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Integral]", "A"], 
     RowBox[{"y", 
      RowBox[{"\[DifferentialD]", "A"}]}]}], "=", 
    RowBox[{
     SubscriptBox["y", 
      StyleBox["c",
       FontSlant->"Plain"]], " ", "A"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"c716402e-2aae-4002-9f39-8bcc85c6e218"],
 ","
