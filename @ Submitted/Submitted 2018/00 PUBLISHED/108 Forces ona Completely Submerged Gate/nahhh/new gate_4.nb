Cell[TextData[{
 "This Demonstration determines the magnitude of the applied force needed to \
keep a gate that is ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["w", "2"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "a038be5c-a644-45fa-b9f2-10fe26dc659d"],
 " meters wide and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["d", "1"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "8d9553a0-dc79-4c88-87cf-8a9c8b224a93"],
 " meters tall, closed (see Figure 1)."
}], "DetailNotes",
 CellChangeTimes->{{3.7184615439536395`*^9, 3.7184615923254843`*^9}, {
  3.718462156955473*^9, 3.718462156955473*^9}},
 CellID->202600676,ExpressionUUID->"b358c824-1497-4a68-bdcc-b4c5c5349f07"],

Cell["\<\
The magnitude of the resultant force is found by summing the differential \
forces over the entire surface:\
\>", "DetailNotes",
 CellChangeTimes->{{3.7184615439536395`*^9, 3.7184615923254843`*^9}, {
  3.718463239920003*^9, 3.718463275403777*^9}},
 CellID->448693411,ExpressionUUID->"146fa9ad-a1cd-4dad-9e23-476198597e4e"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", 
     StyleBox["R",
      FontSlant->"Plain"]], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[Integral]", "A"], 
      RowBox[{"\[Gamma]", " ", "h", 
       RowBox[{"\[DifferentialD]", "A"}]}]}], "=", 
     RowBox[{
      SubscriptBox["\[Integral]", "A"], 
      RowBox[{"\[Gamma]", " ", "y", " ", "sin", " ", "\[Theta]", 
       RowBox[{"\[DifferentialD]", "A"}]}]}]}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"e4cad820-6d11-4e26-8834-9ff6bd5465eb"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.7184615439536395`*^9, 3.7184615923254843`*^9}, {
  3.718463239920003*^9, 3.718463327297228*^9}},
 CellID->29556107,ExpressionUUID->"2ec3f075-1ed0-4a35-a3b8-a27b1adbf007"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"h", "=", 
    RowBox[{"y", " ", "sin", " ", "\[Theta]"}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"06251f6a-5328-45dc-9cc8-4781fb49259d"],
 " is the vertical distance from the fluid surface to the centroid center \
(m), ",
 Cell[BoxData[
  FormBox["y", TraditionalForm]], "InlineMath",ExpressionUUID->
  "f4d314aa-2d1e-49b1-924e-65ee005abe1f"],
 " is the ",
 Cell[BoxData[
  FormBox["y", TraditionalForm]], "InlineMath",ExpressionUUID->
  "4ebedf53-416a-4f21-8ed5-17268caa3e20"],
 "-coordinate of the height along the diagonal (m), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["F", 
    StyleBox["R",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",ExpressionUUID->
  "dea6c8df-2c35-4dad-afc2-f4b05f77d1f0"],
 " is the resultant force (N), ",
 Cell[BoxData[
  FormBox[
   RowBox[{"A", "=", 
    RowBox[{"h", " ", 
     SubscriptBox["w", "2"]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"9eb42540-d786-430e-bde7-79d7bed7bae2"],
 " is area (",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    StyleBox["m",
     FontSlant->"Plain"], "2"], TraditionalForm]], "InlineMath",
  ExpressionUUID->"d70f79f6-f7c3-4959-93da-05de61a6b299"],
 "), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["w", "2"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "65718f47-e538-44d9-b699-2e69eb19f64b"],
 " is the width of the gate (m), ",
 Cell[BoxData[
  FormBox["\[Theta]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "d68a29b4-dde4-4be4-8211-0636db5f8e28"],
 " is angle (degrees), and ",
 Cell[BoxData[
  FormBox["\[Gamma]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "c4f6bbfa-d0ea-4090-bb95-b169d3c6c218"],
 " is the specific weight of water (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["N",
     FontSlant->"Plain"], "/", 
    SuperscriptBox[
     StyleBox["m",
      FontSlant->"Plain"], "3"]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"aa1f5559-c7a9-438f-9bd5-145cb9a28c1d"],
 ")."
}], "DetailNotes",
 CellChangeTimes->{{3.7184615439536395`*^9, 3.7184615923254843`*^9}, {
  3.718463239920003*^9, 3.718463482532589*^9}, {3.7184643336481733`*^9, 
  3.7184643401574154`*^9}},
