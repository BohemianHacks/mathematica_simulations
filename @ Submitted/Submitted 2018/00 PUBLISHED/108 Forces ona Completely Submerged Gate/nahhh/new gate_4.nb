Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Sum]", "M"}], "=", 
    RowBox[{"0", "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["F", 
