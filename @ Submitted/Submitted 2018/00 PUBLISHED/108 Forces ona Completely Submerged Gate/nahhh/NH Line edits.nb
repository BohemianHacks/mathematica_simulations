       RowBox[{"\[DifferentialD]", "F"}]}]}], "=", 
     RowBox[{
      SubscriptBox["\[Integral]", "A"], 
      RowBox[{"\[Gamma]", " ", 
       SuperscriptBox["y", "2"], " ", "sin", " ", "\[Theta]", 
