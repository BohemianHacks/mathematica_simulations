Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["h", 
     StyleBox["c",
      FontSlant->"Plain"]], "=", 
    RowBox[{
     SubscriptBox["y", 
