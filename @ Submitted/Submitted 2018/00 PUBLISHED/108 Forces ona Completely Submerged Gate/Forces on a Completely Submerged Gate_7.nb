 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", 
     StyleBox["R",
      FontSlant->"Plain"]], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[Integral]", "A"], 
      RowBox[{"\[Gamma]", " ", "h", 
       RowBox[{"\[DifferentialD]", "A"}]}]}], "=", 
     RowBox[{
      SubscriptBox["\[Integral]", "A"], 
      RowBox[{"\[Gamma]", " ", "y", " ", "sin", " ", "\[Theta]", 
       RowBox[{"\[DifferentialD]", "A"}]}]}]}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"671e7759-fcb1-4139-adc4-bb40362cb497"],
