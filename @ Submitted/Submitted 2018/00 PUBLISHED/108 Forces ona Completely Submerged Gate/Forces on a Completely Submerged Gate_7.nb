Cell[TextData[{
 "When ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["F", 
    StyleBox["R",
     FontSlant->"Plain"]], TraditionalForm]],ExpressionUUID->
  "e035546a-dd2c-47ff-a4fb-2281278b05c4"],
 " is substituted into this equation, and the right side is integrated from d \
to d+L, then ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["y", 
    StyleBox["R",
     FontSlant->"Plain"]], TraditionalForm]],ExpressionUUID->
  "6d582d60-d722-4d80-b228-d66ae54d3e5b"],
 "  becomes:\[LineSeparator] ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", 
     StyleBox["R",
      FontSlant->"Plain"]], "=", 
    RowBox[{"d", "+", 
     FractionBox[
      RowBox[{
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"d", "+", "L"}], ")"}], "2"], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"2", "L"}], "-", "d"}], ")"}]}], " ", "+", 
       SuperscriptBox["d", "3"]}], 
      RowBox[{"6", 
       SubscriptBox["Ly", "c"]}]]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"2da5a987-6420-4f84-9891-446c9c834a4f"]
}], "DetailNotes",
