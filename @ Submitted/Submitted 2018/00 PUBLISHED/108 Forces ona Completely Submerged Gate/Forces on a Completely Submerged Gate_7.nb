Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["F", 
     StyleBox["R",
      FontSlant->"Plain"]], " ", 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["y", 
       StyleBox["R",
        FontSlant->"Plain"]], "-", "d"}], ")"}]}], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[Integral]", "A"], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"y", "-", "d"}], ")"}], 
      RowBox[{"\[DifferentialD]", "F"}]}]}], "=", 
    RowBox[{
     SubscriptBox["\[Integral]", "A"], 
     RowBox[{"\[Gamma]", " ", "y", " ", 
      RowBox[{"(", 
       RowBox[{"y", "-", "d"}], ")"}], " ", "sin", " ", "\[Theta]", 
      RowBox[{"\[DifferentialD]", "A"}]}]}]}]}], 
  TraditionalForm]], \
"InlineMath",ExpressionUUID->"2db03b90-4b9d-46c1-92b3-32d356baf421"]], \
"DetailNotes",
 CellChangeTimes->{{3.7184615439536395`*^9, 3.7184615923254843`*^9}, {
