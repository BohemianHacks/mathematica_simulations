gate (m), ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", 
     StyleBox["c",
      FontSlant->"Plain"]], "=", 
    RowBox[{"d", "+", 
     RowBox[{"L", "/", "2"}]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"2682389c-f0ca-4f53-8e72-9f011c981047"],
