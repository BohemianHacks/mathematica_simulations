   RowBox[{"h", "=", 
    RowBox[{"y", " ", "sin", " ", "\[Theta]"}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"9fcafb24-dd40-4cc0-8bd5-2b6b8d165d02"],
