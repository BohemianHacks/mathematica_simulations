 CellID->507476432,ExpressionUUID->"59b55cc0-e17f-4b38-b4e9-24ffb76059ad"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["G", 
     StyleBox["E",
      FontSlant->"Plain"]], "=", 
    RowBox[{"x", " ", 
     RowBox[{"(", 
      RowBox[{"1", "-", "x"}], ")"}], " ", 
     RowBox[{"(", 
      RowBox[{"a", "+", 
       RowBox[{"b", " ", 
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{"2", " ", "x"}]}], ")"}]}]}], ")"}]}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"11126f4d-c30b-4662-b29b-e5dcd9b5e248"],
