        RowBox[{"x2", "[", "i", "]"}]}]}], ")"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"i", "=", "0"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"While", "[", 
   RowBox[{
    RowBox[{"i", "<", "101"}], ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"x2", "[", "i", "]"}], "=", 
       RowBox[{"i", "*", "0.01"}]}], ",", 
