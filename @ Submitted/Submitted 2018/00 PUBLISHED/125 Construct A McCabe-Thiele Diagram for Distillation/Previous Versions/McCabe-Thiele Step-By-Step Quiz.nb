       RowBox[{"x2", "[", "i", "]"}], ",", 
       RowBox[{"y2", "[", "i", "]"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "0", ",", "100"}], "}"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{"eqgraphic", " ", "=", " ", 
  RowBox[{"Graphics", "[", 
