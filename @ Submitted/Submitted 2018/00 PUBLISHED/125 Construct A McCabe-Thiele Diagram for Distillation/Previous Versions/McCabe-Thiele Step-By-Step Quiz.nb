 RowBox[{
  RowBox[{"CWat", "=", 
   RowBox[{"RandomReal", "[", 
    RowBox[{"{", 
     RowBox[{"225", ",", "240"}], "}"}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"VPMet", "=", 
   SuperscriptBox["10", 
    RowBox[{"AMet", "-", 
     FractionBox["BMet", 
      RowBox[{"CMet", "+", "T"}]]}]]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"VPWat", "=", 
   SuperscriptBox["10", 
    RowBox[{"AWat", "-", 
     FractionBox["BWat", 
      RowBox[{"CWat", "+", "T"}]]}]]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"mve", "=", "1"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"Clear", "[", 
   RowBox[{"T", ",", "i"}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Gamma]1", "[", "i_", "]"}], "=", 
   RowBox[{"Exp", "[", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"x2", "[", "i", "]"}], "2"], "*", 
     RowBox[{"(", 
      RowBox[{"A12", "+", 
       RowBox[{"2", "*", 
        RowBox[{"(", 
         RowBox[{"A21", "-", "A12"}], ")"}], "*", 
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{"x2", "[", "i", "]"}]}], ")"}]}]}], ")"}]}], "]"}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Gamma]2", "[", "i_", "]"}], "=", 
   RowBox[{"Exp", "[", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
