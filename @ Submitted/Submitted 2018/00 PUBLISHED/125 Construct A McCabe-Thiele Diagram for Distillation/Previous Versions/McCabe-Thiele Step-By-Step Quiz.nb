 RowBox[{
  RowBox[{"P", "=", 
   RowBox[{"RandomInteger", "[", 
    RowBox[{"{", 
     RowBox[{"200", ",", "2000"}], "}"}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"AMet", "=", "8.08097"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"BMet", "=", 
   RowBox[{"RandomReal", "[", 
    RowBox[{"{", 
     RowBox[{"1530", ",", "1570"}], "}"}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"CMet", "=", 
   RowBox[{"RandomReal", "[", 
    RowBox[{"{", 
     RowBox[{"240", ",", "250"}], "}"}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"AWat", "=", 
   RowBox[{"RandomReal", "[", 
    RowBox[{"{", 
     RowBox[{"8", ",", "8.05"}], "}"}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"BWat", "=", 
