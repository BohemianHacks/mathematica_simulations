            RowBox[{"VPWat", "*", 
             RowBox[{"\[Gamma]1", "[", "i", "]"}], "*", 
             RowBox[{"(", 
              RowBox[{"1", "-", 
               RowBox[{"x2", "[", "i", "]"}]}], ")"}]}], "+", 
            RowBox[{"VPMet", "*", 
             RowBox[{"\[Gamma]2", "[", "i", "]"}], "*", 
             RowBox[{"x2", "[", "i", "]"}]}]}]}], ",", 
          RowBox[{"{", 
           RowBox[{"T", ",", "60"}], "}"}], ",", 
          RowBox[{"AccuracyGoal", "\[Rule]", "3"}]}], "]"}], "[", 
        RowBox[{"[", 
         RowBox[{"1", ",", "2"}], "]"}], "]"}]}], ",", " ", 
      RowBox[{
       RowBox[{"y2", "[", "i", "]"}], "=", 
       RowBox[{
        RowBox[{
         RowBox[{"x2", "[", "i", "]"}], "+", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"VPMet", "*", 
               RowBox[{"\[Gamma]2", "[", "i", "]"}], "*", 
               RowBox[{
                RowBox[{"x2", "[", "i", "]"}], "/", "P"}]}], ")"}], "-", 
             RowBox[{"x2", "[", "i", "]"}]}], ")"}], "*", "mve"}], ")"}]}], "/.", 
        RowBox[{"T", "->", 
         RowBox[{"T", "[", "i", "]"}]}]}]}], ",", 
      RowBox[{"i", "++"}]}], "}"}]}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"tb", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"{", 
