Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["x", "0"], " ", "L"}], "+", 
     RowBox[{
      SubscriptBox["y", 
       RowBox[{"N", "+", "1"}]], " ", "V"}]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", "N"], " ", "L"}], "+", 
     RowBox[{
      SubscriptBox["y", "1"], " ", "V"}]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"4c4c839c-1fc7-4a27-ba22-afc3337ea496"],
 ","
}], "DetailNotes",
