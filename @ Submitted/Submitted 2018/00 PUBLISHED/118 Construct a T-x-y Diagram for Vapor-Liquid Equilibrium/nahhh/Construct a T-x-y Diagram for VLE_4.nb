Cell["", "DetailsSection",ExpressionUUID->"51a96b95-018d-4fc1-ba62-5685de103ca7"],

Cell[TextData[{
 "The saturation pressure of each component ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "=", 
    RowBox[{"(", 
     RowBox[{"1", ",", "2"}], ")"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"c81e25ea-6c80-40fb-b0ac-0f83802215df"],
 " is calculated using the Antoine equation:"
}], "DetailNotes",
 CellChangeTimes->{{3.724598236630128*^9, 3.7245982687267604`*^9}, {
  3.7268491580081196`*^9, 3.7268491598048987`*^9}},
 CellID->166405933,ExpressionUUID->"6f75a16b-db79-4fa8-8d85-aeb9b73f72bd"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["P", "i", "sat"], "=", 
    RowBox[{"10", "^", 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["A", "i"], "-", 
       FractionBox[
        SubscriptBox["B", "i"], 
        RowBox[{"T", "+", 
         SubscriptBox["C", "i"]}]]}], ")"}]}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"2ed21d39-4dcf-4427-abaf-488e97a81654"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.724598236630128*^9, 3.7245982875537004`*^9}},
 CellID->304109836,ExpressionUUID->"c08ee4a7-5f01-4734-a167-dbf06adee6b0"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["P", "i", "sat"], TraditionalForm]], "InlineMath",
  ExpressionUUID->"a9189dc6-4336-404f-af39-9b7fe5c4b047"],
 " is the saturation pressure (bar), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "i"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "8724b843-42a9-4719-bd37-f4324d51efb5"],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["B", "i"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "f2c94a00-299a-47a3-b530-e1717b660ab3"],
 ", and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["C", "i"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "41d28264-14a4-4de1-899d-a657670a8fab"],
 " are Antoine constants, ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath",ExpressionUUID->
  "4010cf38-c551-4861-8d08-3c66e7fdeff6"],
 " is temperature (\[Degree]C), and component ",
 Cell[BoxData[
  FormBox["1", TraditionalForm]], "InlineMath",ExpressionUUID->
  "2e31771b-f2fd-4071-871d-d1739304fd8d"],
 " is ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath",ExpressionUUID->
  "b9c97099-5926-4a52-b9dd-8ec9e73faa64"],
 "-hexane, and component ",
 Cell[BoxData[
  FormBox["2", TraditionalForm]], "InlineMath",ExpressionUUID->
  "f381ffad-554f-4c3e-b872-7f91bc72e2b5"],
 " is ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath",ExpressionUUID->
  "bae00af2-5606-49a2-9eda-209754e3e220"],
 "-octane."
}], "DetailNotes",
 CellChangeTimes->{{3.724598236630128*^9, 3.7245983539461813`*^9}},
 CellID->490228613,ExpressionUUID->"fa462750-4f63-4bcd-b245-bc45cb9f5fbd"],

Cell[TextData[{
 "Raoult's law is used to calculate the bubble-point and dew-point pressures \
using ",
 Cell[BoxData[
  FormBox["K", TraditionalForm]], "InlineMath",ExpressionUUID->
  "deb7f0e2-46e6-4da7-8041-cb05f1f51af2"],
 " factors:"
}], "DetailNotes",
 CellChangeTimes->{{3.724598236630128*^9, 3.724598389611444*^9}},
 CellID->12022239,ExpressionUUID->"abd9e541-a6c8-4ea3-bfd5-be1ba60b79d7"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["K", "i"], "=", 
    RowBox[{
     FractionBox[
      SubscriptBox["y", "i"], 
      SubscriptBox["x", "i"]], "=", 
     FractionBox[
      SubsuperscriptBox["P", "i", "sat"], "P"]}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"ffca803c-d5c0-4228-b986-9764a5013c29"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.724598236630128*^9, 3.7245984201418576`*^9}},
 CellID->13452163,ExpressionUUID->"6bfaa2ec-6d3d-4a57-97a3-5bbdc2cf32fb"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["y", "i"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "f4742f79-4550-4172-984b-ff12ffe0d14b"],
 " is the vapor mole fraction (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["y", "1"], "+", 
     SubscriptBox["y", "2"]}], "=", "1"}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"dab6e0a9-4460-456c-9a23-d0309917f68b"],
 "), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "i"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "4f084c43-c6cb-4223-aa8f-8329e742f80b"],
 " is the liquid mole fraction (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["x", "1"], "+", 
     SubscriptBox["x", "2"]}], "=", "1"}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"eead989c-d329-49c7-be60-d76c597d4c26"],
 "), and ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath",ExpressionUUID->
  "46201991-50a2-4c2c-b364-1714eed71798"],
 " is the total pressure."
}], "DetailNotes",
 CellChangeTimes->{{3.724598236630128*^9, 3.724598468635822*^9}, {
  3.726849194182328*^9, 3.7268492148051767`*^9}},
 CellID->31782332,ExpressionUUID->"c4ad516d-0793-4615-a4d5-0591fbeee1b4"],

Cell[TextData[{
 "The saturation temperature is solved for by setting ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["P", "i", "sat"], TraditionalForm]], "InlineMath",
  ExpressionUUID->"061c9da5-5331-4bce-add9-c563617fb408"],
 " equal to ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath",ExpressionUUID->
  "fdb800f5-3dcf-4049-bcef-061f96710b46"],
 " and solving for ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath",ExpressionUUID->
  "6fbb6751-895a-4d83-8aa7-2c0b11528f6a"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.7245984720366535`*^9, 3.7245985082493095`*^9}},
 CellID->291433600,ExpressionUUID->"3a0174f9-8147-43b0-8395-8ef7679fe86c"],

Cell[TextData[{
 "The bubble-point pressure is calculated using ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Sum]", 
     RowBox[{
      SubscriptBox["K", "i"], " ", 
      SubscriptBox["x", "i"]}]}], "=", "1"}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"0c437964-ada4-4e05-bc6c-4c6f25517fe6"],
 ":"
}], "DetailNotes",
 CellChangeTimes->{{3.7245984720366535`*^9, 3.7245985599148855`*^9}},
 CellID->419147508,ExpressionUUID->"5ba27b93-1738-4af6-b42f-5294a07cd87f"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", "1"], " ", 
      SubsuperscriptBox["P", "1", "sat"]}], "+", 
     RowBox[{
      SubscriptBox["x", "2"], " ", 
      SubsuperscriptBox["P", "2", "sat"]}]}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"7ecfed43-8547-47b0-89e5-8c04ab30c172"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.7245984720366535`*^9, 3.7245985483527994`*^9}},
 CellID->7171723,ExpressionUUID->"074c62fd-2d26-4603-a1ac-a5fc1c791519"],

Cell[TextData[{
 "The dew-point pressure is calculated using ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Sum]", 
     RowBox[{
      SubscriptBox["y", "i"], "/", 
      SubscriptBox["K", "i"]}]}], "=", "1"}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"734fd7ed-e104-4f02-9583-9bbeb7c89daf"],
 ":"
}], "DetailNotes",
 CellChangeTimes->{{3.7245985518934193`*^9, 3.724598580152432*^9}},
 CellID->63744691,ExpressionUUID->"11c8708c-c50e-44fb-b6a9-54b6c6c16439"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       FractionBox[
        SubscriptBox["x", "1"], 
        SubsuperscriptBox["P", "1", "sat"]], "+", 
       FractionBox[
        SubscriptBox["x", "2"], 
        SubsuperscriptBox["P", "2", "sat"]]}], ")"}], 
     RowBox[{"-", "1"}]]}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "e9426d73-09ef-4123-8841-20c3a19ffe5a"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.7245985518934193`*^9, 3.7245986089666576`*^9}},
 CellID->197863040,ExpressionUUID->"915ebc1b-94ea-4b5d-97ee-33512d4d6fe1"],

