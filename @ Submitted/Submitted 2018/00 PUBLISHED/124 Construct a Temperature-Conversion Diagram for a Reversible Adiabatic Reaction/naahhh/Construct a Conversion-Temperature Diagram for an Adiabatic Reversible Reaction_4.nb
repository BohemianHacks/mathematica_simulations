Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["K", 
    StyleBox["m",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",ExpressionUUID->
  "6a9e5d95-bc67-4f05-9e8d-e7fb1874cf22"],
 " is the equilibrium constant at temperature ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["T", 
    StyleBox["m",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",ExpressionUUID->
  "60a9a03c-2b27-4479-a609-015d768dc203"],
