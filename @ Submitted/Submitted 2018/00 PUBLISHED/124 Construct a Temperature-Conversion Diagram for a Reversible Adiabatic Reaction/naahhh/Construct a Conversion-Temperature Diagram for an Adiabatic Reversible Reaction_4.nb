Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["K", 
     StyleBox["e",
      FontSlant->"Plain"]], "=", 
    RowBox[{
     SubscriptBox["K", 
      StyleBox["m",
       FontSlant->"Plain"]], " ", 
     SuperscriptBox["e", 
      RowBox[{"[", 
       RowBox[{
        FractionBox[
         RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", "H"}], "R"], " ", 
        RowBox[{"(", 
         RowBox[{
          FractionBox["1", 
           SubscriptBox["T", 
            StyleBox["m",
             FontSlant->"Plain"]]], "-", 
          FractionBox["1", "T"]}], ")"}]}], "]"}]]}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"4ca63607-34c5-41d6-9101-db0e8571ac61"],
 ","
}], "DetailNotes",
