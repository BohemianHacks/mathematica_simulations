      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "+", "\[Alpha]"}], ")"}], " ", "Cp", " ", 
       RowBox[{"(", 
        RowBox[{"T", "-", 
         SubscriptBox["T", 
          StyleBox["f",
           FontSlant->"Plain"]]}], ")"}]}], 
