Cell["\<\
At a given pressure and volume, the temperature is calculated from the \
Peng-Robinson equation of state:\
\>", "DetailNotes",
 CellChangeTimes->{{3.616248610307424*^9, 3.616248706028899*^9}, 
   3.6195453299453487`*^9, {3.67371414526272*^9, 3.6737141452647204`*^9}, {
   3.6737141838153954`*^9, 3.673714184698903*^9}, {3.673714322882474*^9, 
   3.6737143783557496`*^9}, {3.6737869921697187`*^9, 3.673787000167119*^9}},
 CellID->24376353],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     FractionBox[
      RowBox[{"R", " ", "T"}], 
      RowBox[{"V", "-", "b"}]], "-", 
     FractionBox["a", 
      RowBox[{
       SuperscriptBox["V", "2"], "+", 
       RowBox[{"2", " ", "V", " ", "b"}], "-", 
       SuperscriptBox["b", "2"]}]]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.616248610307424*^9, 3.616248706028899*^9}, 
   3.6195453299453487`*^9, {3.67371414526272*^9, 3.673714177299322*^9}},
 CellID->532484372],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"a", "=", 
    RowBox[{"0.457", " ", 
     FractionBox[
      RowBox[{
       SuperscriptBox["R", "2"], " ", 
       SubsuperscriptBox["T", 
        StyleBox["c",
         FontSlant->"Plain"], "2"]}], 
      SubscriptBox["P", 
       StyleBox["c",
        FontSlant->"Plain"]]], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{"\[Kappa]", " ", 
         RowBox[{"(", 
          RowBox[{"1", "-", 
           SqrtBox[
            RowBox[{"T", "/", "Tc"}]]}], ")"}]}]}], ")"}], "2"]}]}], 
   TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.616248610307424*^9, 3.616248758815918*^9}, 
   3.619545331362939*^9, {3.6745846954004498`*^9, 3.6745847180140977`*^9}},
 CellID->664680889],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"b", "=", 
    RowBox[{"0.078", " ", 
     FractionBox[
      RowBox[{"R", " ", 
       SubscriptBox["T", 
        StyleBox["c",
         FontSlant->"Plain"]]}], 
      SubscriptBox["P", 
       StyleBox["c",
        FontSlant->"Plain"]]]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.616248610307424*^9, 3.616248820758461*^9}, 
   3.619545334974325*^9, {3.6745847225666356`*^9, 3.674584723435154*^9}},
 CellID->26190243],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Kappa]", "=", 
    RowBox[{"0.375", "+", 
     RowBox[{"1.54", " ", "\[Omega]"}], "-", 
     RowBox[{"0.270", " ", 
      SuperscriptBox["\[Omega]", "2"]}]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.616248610307424*^9, 3.616248994470397*^9}, {
   3.616249033669639*^9, 3.6162491794079747`*^9}, 3.617711882442876*^9, {
   3.618000391606111*^9, 3.6180004717424*^9}, {3.618000540355756*^9, 
   3.6180005980917673`*^9}, 3.618000687936089*^9, {3.618057646509813*^9, 
   3.618057653382206*^9}, {3.6181549441057434`*^9, 3.618154956695464*^9}, {
   3.6187741220203915`*^9, 3.6187741299028425`*^9}, 3.6195452292981377`*^9, {
   3.6195453471243896`*^9, 3.619545348046767*^9}, {3.619548428143958*^9, 
   3.619548433470006*^9}, {3.6198014085263743`*^9, 3.6198014111823745`*^9}, {
   3.630760238324813*^9, 3.630760239374873*^9}, {3.665336538118905*^9, 
   3.6653365426564307`*^9}},
 CellID->326283920],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath"],
 " is pressure (MPa), ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]], "InlineMath"],
 " is the ideal gas constant (",
 Cell[BoxData[
  FormBox[
   RowBox[{"8.314", " ", 
    SuperscriptBox["cm", "3"], " ", 
    RowBox[{
     RowBox[{"MPa", "/", "mol"}], "/", "K"}]}], TraditionalForm]], 
  "InlineMath"],
 "), ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath"],
 " is temperature (K), ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]], "InlineMath"],
 " is the molar volume (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["cm", "3"], "/", "mol"}], TraditionalForm]], "InlineMath"],
 "), ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox["b", TraditionalForm]], "InlineMath"],
 " are Peng\[Dash]Robinson constants, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["P", 
    StyleBox["c",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " is the critical pressure (MPa), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["T", 
    StyleBox["c",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " is the critical temperature (K), and ",
 Cell[BoxData[
  FormBox["\[Omega]", TraditionalForm]], "InlineMath"],
 " is the acentric factor. "
}], "DetailNotes",
 CellChangeTimes->{{3.616248610307424*^9, 3.616248994470397*^9}, {
