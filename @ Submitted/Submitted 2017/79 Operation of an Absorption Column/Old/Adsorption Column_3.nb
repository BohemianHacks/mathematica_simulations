Cell[CellGroupData[{

Cell["", "DetailsSection"],

Cell["A mole balance is done on the absorption column:", "DetailNotes",
 CellChangeTimes->{{3.6998804127279234`*^9, 3.699880430499605*^9}},
 CellID->237613890],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["x", "0"], " ", "L"}], "+", 
     RowBox[{
      SubscriptBox["y", 
       RowBox[{"n", "+", "1"}]], " ", "V"}]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", "n"], " ", "L"}], "+", 
     RowBox[{
      SubscriptBox["y", "1"], " ", "V"}]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6998804127279234`*^9, 3.6998804311880064`*^9}, {
  3.699880465322937*^9, 3.699880489403638*^9}, {3.6999633576500015`*^9, 
  3.69996336746085*^9}},
 CellID->237742517],

Cell["rearrange to get the operating line for the absorber:", "DetailNotes",
 CellChangeTimes->{{3.6998804127279234`*^9, 3.6998804311880064`*^9}, {
  3.699880465322937*^9, 3.6998805034135237`*^9}},
 CellID->106309455],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", 
     RowBox[{"n", "+", "1"}]], "=", 
    RowBox[{
     RowBox[{
      FractionBox["L", "V"], " ", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["x", "n"], "-", 
        SubscriptBox["x", "0"]}], ")"}]}], "+", 
     SubscriptBox["y", "1"]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6998804127279234`*^9, 3.6998804311880064`*^9}, {
  3.699880465322937*^9, 3.6998805384342966`*^9}, {3.699963373142876*^9, 
  3.6999633834583263`*^9}},
 CellID->676069286],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["L", TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]], "InlineMath"],
 " are the solute-free liquid and vapor molar flow rates, ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox["y", TraditionalForm]], "InlineMath"],
 " are the mole fractions of solute in the liquid and vapor streams, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "0"], TraditionalForm]], "InlineMath"],
 " is the mole fraction in the inlet liquid stream, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["y", 
    RowBox[{"n", "+", "1"}]], TraditionalForm]], "InlineMath"],
 " is the mole fraction in the inlet vapor stream, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "n"], TraditionalForm]], "InlineMath"],
 " is the mole fraction in the outlet liquid stream, and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["y", "1"], TraditionalForm]], "InlineMath"],
 " is the mole fraction in the outlet vapor stream."
}], "DetailNotes",
 CellChangeTimes->{{3.6998804127279234`*^9, 3.6998804311880064`*^9}, {
  3.699880465322937*^9, 3.6998806131298795`*^9}, {3.6998806508195305`*^9, 
  3.699880751674843*^9}, {3.699963386055746*^9, 3.6999634612139378`*^9}, {
  3.69996352962606*^9, 3.699963532316265*^9}},
 CellID->120177033],

Cell["The equilibrium line is linear:", "DetailNotes",
 CellChangeTimes->{{3.6998804127279234`*^9, 3.6998804311880064`*^9}, {
   3.699880465322937*^9, 3.6998806131298795`*^9}, {3.6998806508195305`*^9, 
   3.6998807497052393`*^9}, {3.6998808118456755`*^9, 3.6998808276079535`*^9}, 
   3.69996346544836*^9},
 CellID->1035463846],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", "eq"], "=", 
    RowBox[{
     FractionBox["H", "P"], " ", 
     SubscriptBox["x", "n"]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6998804127279234`*^9, 3.6998804311880064`*^9}, {
  3.699880465322937*^9, 3.6998806131298795`*^9}, {3.6998806508195305`*^9, 
  3.6998807497052393`*^9}, {3.6998808118456755`*^9, 3.699880844262244*^9}, {
  3.699963469846184*^9, 3.69996347320319*^9}},
 CellID->306537432],

Cell[TextData[{
