Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"G", "(", "s", ")"}], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"Y", "(", "s", ")"}], 
      RowBox[{"U", "(", "s", ")"}]], "=", 
     FractionBox["K", 
      RowBox[{
       RowBox[{"\[Tau]", " ", "s"}], "+", "1"}]]}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"e102608a-b1cd-48f8-acdc-47994c6717d6"],
 ","
