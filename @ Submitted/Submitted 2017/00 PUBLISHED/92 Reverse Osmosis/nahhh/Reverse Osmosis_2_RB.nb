Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"k", "=", 
    RowBox[{"0.04433", " ", 
     SuperscriptBox[
      RowBox[{"(", 
       FractionBox[
        RowBox[{"\[Omega]", " ", "d"}], "v"], ")"}], "0.75"], " ", 
     SuperscriptBox["Sc", "0.33"], " ", 
     FractionBox[
      SubscriptBox["D", "AB"], "d"]}]}], TraditionalForm]], "InlineMath",
  FontSize->12,ExpressionUUID->"43213e2d-ee1d-42c6-a5ef-42996e17645c"],
 ","
}], "DetailNotes",
