Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["J", "max"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "58d82f40-9851-4f0f-9846-391b8eb436c3"],
 " is maximum water flux through the membrane (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["g",
     FontSlant->"Plain"], "/", 
    RowBox[{"[", 
     RowBox[{
      SuperscriptBox[
       StyleBox["m",
        FontSlant->"Plain"], "2"], " ", 
      StyleBox["s",
       FontSlant->"Plain"]}], "]"}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "db787e4a-0c36-4c09-96d5-c086086ef5b6"],
 "), and ",
 Cell[BoxData[
  FormBox["\[Rho]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "aefb2e6d-16d8-4719-bd1a-d93769eab32b"],
 " is the density of water (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["g",
     FontSlant->"Plain"], "/", 
    SuperscriptBox[
     StyleBox["m",
      FontSlant->"Plain"], "3"]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"52724180-a74f-4f27-b343-c0db9b40d9c6"],
 ")."
}], "DetailNotes",
 CellChangeTimes->{{3.7136134599878683`*^9, 3.7136134950878763`*^9}, {
  3.7136135287278004`*^9, 3.713613578392641*^9}, {3.713700506843617*^9, 
  3.713700538544446*^9}, {3.714136641704439*^9, 3.71413665952752*^9}},
 FontSize->12,
 CellID->712905442,ExpressionUUID->"a85b294e-f863-4889-8a7c-30463fe3d491"],

Cell["Solve for the solvent flux:", "DetailNotes",
 CellChangeTimes->{{3.7136134599878683`*^9, 3.7136134950878763`*^9}, {
  3.7136135287278004`*^9, 3.7136136044171295`*^9}},
 FontSize->12,
 CellID->161630125,ExpressionUUID->"1f2db088-f043-4dd5-897b-7815efcd47a0"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   StyleBox[
    RowBox[{
     SuperscriptBox["J", "\[Prime]"], "=", 
     FractionBox[
      RowBox[{
       SubsuperscriptBox["J", "max", "\[Prime]"], " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", "P"}], "-", 
         "\[CapitalDelta]\[Pi]"}], "\[InvisibleSpace]", ")"}]}], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", "P"}], "-", 
         RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", "\[Pi]"}]}], ")"}], 
       "+", 
       RowBox[{
        SuperscriptBox["J", "\[Prime]"], " ", 
        FractionBox["t", 
         SuperscriptBox["K", "\[Prime]"]]}]}]]}],
    FontSize->12], TraditionalForm]], "InlineMath",
  FontSize->14,ExpressionUUID->"69e63c1c-0ecc-4c4d-81ba-ad99b04598ff"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.7136134599878683`*^9, 3.7136134950878763`*^9}, {
   3.7136135287278004`*^9, 3.713613740517914*^9}, {3.713700551173162*^9, 
   3.713700607783239*^9}, {3.713700638253134*^9, 3.713700641014896*^9}, 
   3.7141369587974606`*^9},
 FontSize->12,
 CellID->208540846,ExpressionUUID->"deec0089-420f-468e-a11d-ed31b7c3b22e"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", "P"}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"78a88e77-b30b-4164-864d-d459e034515f"],
 " is mechanical pressure (atm), ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", "\[Pi]"}], 
   TraditionalForm]], "InlineMath",ExpressionUUID->
  "d5670462-d75c-4ff7-93d5-6307895d0763"],
 " is osmotic pressure (atm), and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["K", "\[Prime]"], "/", "t"}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"071f972a-a8ea-4b46-9de0-8b18eb43c78a"],
 " is permeability of the membrane to the solvent (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["g",
     FontSlant->"Plain"], "/", 
    RowBox[{"[", 
     RowBox[{"atm", " ", 
      StyleBox["s",
       FontSlant->"Plain"], " ", 
      SuperscriptBox[
       StyleBox["m",
        FontSlant->"Plain"], "2"]}], "]"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"d6f59571-eb81-4b08-9c21-af283fb8085c"],
 ")."
}], "DetailNotes",
 CellChangeTimes->{{3.7136134599878683`*^9, 3.7136134950878763`*^9}, {
  3.7136135287278004`*^9, 3.7136138707663636`*^9}, {3.713700647969657*^9, 
  3.713700672466238*^9}},
 FontSize->12,
 CellID->155787706,ExpressionUUID->"15db443e-bb73-4986-8ae4-ddb1a4e41c41"],

Cell["\<\
A material balance is done for salt across the system to get the value for \
the mass fraction of salt in the outlet stream, which is the same as the \
retentate mass fraction inside the tank (given the tank is well-mixed):\
\>", "DetailNotes",
 CellChangeTimes->{{3.713613881209961*^9, 3.7136139265635552`*^9}},
 FontSize->12,
 CellID->554243815,ExpressionUUID->"002c3c00-bc9b-497b-8cd0-c86598a1988e"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", 
     StyleBox["R",
      FontSlant->"Plain"]], "=", 
    RowBox[{
     SubscriptBox["x", "out"], "=", 
     FractionBox[
      RowBox[{
       SubsuperscriptBox["F", "in", "\[Prime]"], " ", 
       SubscriptBox["x", "in"]}], 
      RowBox[{
       SubsuperscriptBox["F", "in", "\[Prime]"], "-", 
       RowBox[{
        SuperscriptBox["J", "\[Prime]"], " ", "A"}]}]]}]}], TraditionalForm]],
   "InlineMath",
  InitializationCell->True,
  FontSize->14,ExpressionUUID->"8102a376-60e5-41d3-9374-df20443471e2"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.713613881209961*^9, 3.713613976839431*^9}, {
  3.714131845072875*^9, 3.714131869226448*^9}, {3.71413668557786*^9, 
  3.7141366915410876`*^9}},
 FontSize->12,
 CellID->384954241,ExpressionUUID->"b5ea77a3-b6f3-46c3-8028-47ed4c514abb"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["F", "in", "\[Prime]"], TraditionalForm]], "InlineMath",
  ExpressionUUID->"5c55a376-f231-4e36-bbb0-d90efa13bed7"],
 " is inlet feed (kg/s), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "in"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "b1ac244e-d7f3-434f-953e-5d79f7307557"],
 " is the mole fraction of salt in the inlet stream, and ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]], "InlineMath",ExpressionUUID->
  "2216d348-9713-4578-a2d1-93c7271fd791"],
 " is the area of the membrane (",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    StyleBox["m",
     FontSlant->"Plain"], "2"], TraditionalForm]], "InlineMath",
  ExpressionUUID->"d8a5fc42-cfed-42ea-a771-fbcda2e193eb"],
 ")."
}], "DetailNotes",
 CellChangeTimes->{{3.713613881209961*^9, 3.713614042285174*^9}, {
  3.713700699929822*^9, 3.71370071419102*^9}},
 FontSize->12,
 CellID->151161612,ExpressionUUID->"5d5b50bf-573a-411e-9f2d-d34a96cc0b23"],

Cell["\<\
To find how much of the feed exits as the permeate, an overall mass balance \
is done: \
\>", "DetailNotes",
 CellChangeTimes->{{3.7136134599878683`*^9, 3.7136134947008543`*^9}, {
  3.7136140561219654`*^9, 3.7136140937741194`*^9}, {3.713700902149337*^9, 
