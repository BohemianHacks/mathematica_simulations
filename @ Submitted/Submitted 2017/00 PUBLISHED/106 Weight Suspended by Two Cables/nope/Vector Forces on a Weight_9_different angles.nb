 CellID->83018487,ExpressionUUID->"1a4de5bd-67ed-45a6-8996-2394f3ae49be"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Sum]", 
     SubscriptBox["F", "x"]}], "=", 
    RowBox[{"0", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        SubscriptBox["T", "1"]}], " ", "cos", " ", 
       SubscriptBox["\[Theta]", "1"]}], "+", 
      RowBox[{
       SubscriptBox["T", "2"], " ", "cos", " ", 
       SubscriptBox["\[Theta]", "2"]}]}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "1286e7ea-60b0-490e-a644-e81d6cd35158"],
