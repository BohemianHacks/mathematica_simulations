Cell["", "DetailsSection",ExpressionUUID->"8dafa324-e344-4e51-8515-6939bc6b4575"],

Cell[TextData[{
 "A force balance is done in the ",
 Cell[BoxData[
  FormBox["y", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "3d6899c0-9c4f-48c3-9346-0d00674be225"],
 " direction:"
}], "DetailNotes",
 CellChangeTimes->{{3.7126669167659817`*^9, 3.7126669289376774`*^9}},
 CellID->535004560,ExpressionUUID->"c195f559-4905-4f0f-a463-a2798a08e2be"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Sum]", 
     SubscriptBox["F", "y"]}], "=", 
    RowBox[{"0", "=", 
     RowBox[{
      RowBox[{"2", " ", "T", " ", "cos", " ", 
       RowBox[{"(", 
        RowBox[{"\[Theta]", "/", "2"}], ")"}]}], "-", 
      RowBox[{"m", " ", "g"}]}]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"48d5cd30-6cbb-4d8d-8b97-f9de8b259a4f"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.7126669167659817`*^9, 3.712666950702923*^9}, {
  3.7137078652939897`*^9, 3.7137078739030237`*^9}},
 CellID->551713347,ExpressionUUID->"4e4db872-392d-49d7-a46b-10db2f8a72b9"],

Cell[TextData[{
 "rearranging to solve for ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "a94f268b-be1c-4bce-b4b7-5b2479cb3762"],
 ":"
}], "DetailNotes",
 InitializationCell->True,
 CellChangeTimes->{{3.7126669167659817`*^9, 3.7126669624135923`*^9}},
 CellID->80680719,ExpressionUUID->"b321a7cb-9f1d-4a5f-9e40-839e827f5ed2"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"T", "=", 
    FractionBox[
     RowBox[{"m", " ", "g"}], 
     RowBox[{"2", " ", "cos", " ", 
      RowBox[{"(", 
       RowBox[{"\[Theta]", "/", "2"}], ")"}]}]]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"6155a533-a45d-4558-b2d7-3bb7974ecab7"],
 ","
}], "DetailNotes",
 InitializationCell->True,
 CellChangeTimes->{{3.7126669167659817`*^9, 3.7126669722411547`*^9}, {
  3.7137078782722473`*^9, 3.713707881045668*^9}},
 CellID->70904923,ExpressionUUID->"e5b6fed1-a812-4e0b-b5c1-7b8bc7bf85f0"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath",ExpressionUUID->
  "68d56644-8683-4296-8a21-162b9e57c7e4"],
 " is the force of tension (N), ",
 Cell[BoxData[
  FormBox["m", TraditionalForm]], "InlineMath",ExpressionUUID->
  "98cb798c-3b61-4740-84cc-16fb9d3f7cfd"],
 " is the mass of the weight pulling down (kg), ",
 Cell[BoxData[
  FormBox[
   RowBox[{"g", "=", 
    RowBox[{"9.8", " ", 
     RowBox[{
      StyleBox["m",
       FontSlant->"Plain"], "/", 
      SuperscriptBox[
       StyleBox["s",
        FontSlant->"Plain"], "2"]}]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"57850e51-9740-449a-b0bc-5c8aa92df0fb"],
 " is acceleration due to gravity, and ",
 Cell[BoxData[
  FormBox["\[Theta]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "d60aa593-7425-438d-bb8e-f7465aa8d874"],
 " is angle between the two cables (degrees)."
}], "DetailNotes",
 InitializationCell->True,
 CellChangeTimes->{{3.7126669167659817`*^9, 3.7126670228020463`*^9}, {
  3.7137078887248993`*^9, 3.71370790453218*^9}},
 CellID->58437359,ExpressionUUID->"13f44668-939f-4a29-8900-f154d2d47cb5"]
}, Open  ]],

