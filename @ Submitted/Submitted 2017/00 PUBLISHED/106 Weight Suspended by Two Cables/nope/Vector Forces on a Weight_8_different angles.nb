      RowBox[{
       SubscriptBox["T", "1"], " ", "sin", " ", 
       SubscriptBox["\[Theta]", "1"]}], "+", 
      RowBox[{
       SubscriptBox["T", "2"], " ", "sin", " ", 
       SubscriptBox["\[Theta]", "2"]}], "-", 
