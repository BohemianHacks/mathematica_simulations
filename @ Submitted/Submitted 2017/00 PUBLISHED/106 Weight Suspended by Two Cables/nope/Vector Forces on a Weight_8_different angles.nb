  FormBox[
   RowBox[{
    RowBox[{"\[Sum]", 
     SubscriptBox["F", "x"]}], "=", 
    RowBox[{"0", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        SubscriptBox["T", "1"]}], " ", "cos", " ", 
       SubscriptBox["\[Theta]", "1"]}], "+", 
