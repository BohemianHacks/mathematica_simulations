Cell[TextData[{
 "A force balance is done in the ",
 Cell[BoxData[
  FormBox["y", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "3d6899c0-9c4f-48c3-9346-0d00674be225"],
 " direction:"
}], "DetailNotes",
