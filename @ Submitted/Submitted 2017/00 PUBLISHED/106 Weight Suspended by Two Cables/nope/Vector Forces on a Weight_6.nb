Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Sum]", 
     SubscriptBox["F", "y"]}], "=", 
    RowBox[{"0", "=", 
     RowBox[{
      RowBox[{"2", " ", "T", " ", "sin", " ", 
       SubscriptBox["\[Theta]", "2"]}], "-", 
      RowBox[{"m", " ", "g"}]}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "48d5cd30-6cbb-4d8d-8b97-f9de8b259a4f"],
 ","
}], "DetailNotes",
