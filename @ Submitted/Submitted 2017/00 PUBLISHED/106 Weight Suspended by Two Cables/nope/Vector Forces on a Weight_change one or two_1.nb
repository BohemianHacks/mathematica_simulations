Cell[TextData[{
 "Force balances in the ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "52b537c3-49a3-4d7a-94b0-b0a9b8a1af3a"],
 " and ",
 Cell[BoxData[
  FormBox["y", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "69b84183-0cef-4bbc-a776-3a6449d95410"],
 " directions are done to calculate the tension forces in each cable:"
}], "DetailNotes",
 CellChangeTimes->{{3.714142135601408*^9, 3.7141421601867323`*^9}},
 CellID->709269628,ExpressionUUID->"6bdfc32c-1750-408a-8668-b0af9a81009b"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Sum]", 
     SubscriptBox["F", "y"]}], "=", 
    RowBox[{"0", "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["T", "1"], " ", "sin", " ", 
       SubscriptBox["\[Theta]", "1"]}], "+", 
      RowBox[{
       SubscriptBox["T", "2"], " ", "sin", " ", 
       SubscriptBox["\[Theta]", "2"]}], "-", 
      RowBox[{"m", " ", "g"}]}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "834551f2-aadc-421d-8946-4cb99dba4b63"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.714142135601408*^9, 3.7141421773848124`*^9}},
 CellID->83018487,ExpressionUUID->"e9c0a9e9-a3a5-40d2-81fb-e0379409578f"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Sum]", 
     SubscriptBox["F", "x"]}], "=", 
    RowBox[{"0", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        SubscriptBox["T", "1"]}], " ", "cos", " ", 
       SubscriptBox["\[Theta]", "1"]}], "+", 
      RowBox[{
       SubscriptBox["T", "2"], " ", "cos", " ", 
       SubscriptBox["\[Theta]", "2"]}]}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "1286e7ea-60b0-490e-a644-e81d6cd35158"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.714142135601408*^9, 3.714142193139888*^9}},
 CellID->316251320,ExpressionUUID->"200c6334-3fe0-4380-b0a1-912839ae760e"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["T", "1"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "cd8d1abd-4e3b-467a-b6fa-948784d917a4"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["T", "2"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "f80bac44-0026-40a0-8a08-521f7594be16"],
 " are the tensions in the left and right cables (N), ",
 Cell[BoxData[
  FormBox["\[Theta]", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "4b40ccc3-f486-4ef1-a9de-88780c061cc0"],
 " is the angle (degrees), ",
 Cell[BoxData[
  FormBox["m", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "114a1dc2-022a-4a85-93e0-3dfe76969d23"],
 " is the mass of the ball (kg), and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"g", "=", 
    RowBox[{"9.8", " ", 
     RowBox[{
      StyleBox["m",
       FontSlant->"Plain"], "/", 
      SuperscriptBox[
       StyleBox["s",
        FontSlant->"Plain"], "2"]}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "9dcdd8f0-6259-48da-b64e-cc5662d35e3f"],
 " is acceleration due to gravity."
}], "DetailNotes",
 CellChangeTimes->{{3.714142135601408*^9, 3.7141422561780257`*^9}},
