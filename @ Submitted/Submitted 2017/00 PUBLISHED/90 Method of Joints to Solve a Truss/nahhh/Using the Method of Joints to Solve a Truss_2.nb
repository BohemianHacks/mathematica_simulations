  FormBox[
   RowBox[{
    RowBox[{"\[Sum]", 
     SubscriptBox["M", 
      StyleBox["A",
       FontSlant->"Plain"]]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"w", " ", 
       RowBox[{"(", 
        RowBox[{"-", 
         SubscriptBox["F", "y"]}], ")"}]}], "+", 
      RowBox[{"2", " ", "w", " ", 
       SubscriptBox["R", 
        StyleBox["B",
         FontSlant->"Plain"]]}]}], "=", "0"}]}], TraditionalForm]], 
  "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "31e47955-a386-4f5e-9aec-2e559ecc7402"],
