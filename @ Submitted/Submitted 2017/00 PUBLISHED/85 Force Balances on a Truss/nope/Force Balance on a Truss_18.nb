      SubscriptBox["R", 
       StyleBox["A",
        FontSlant->"Plain"]], "+", 
      SubscriptBox["R", 
       StyleBox["G",
        FontSlant->"Plain"]], "-", 
      SubscriptBox["F", 
       StyleBox["D",
        FontSlant->"Plain"]], "-", 
      SubscriptBox["F", 
