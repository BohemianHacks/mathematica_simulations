 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", "i"], "=", 
    RowBox[{
     SubscriptBox["x", "i"], " ", 
     FractionBox[
      RowBox[{"P", " ", "V"}], 
      RowBox[{"R", " ", "T"}]]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.692559347807947*^9, 3.692559469540082*^9}, {
  3.692559592865903*^9, 3.692559592866023*^9}, {3.692560310273551*^9, 
  3.692560380457252*^9}, {3.693933578327785*^9, 3.693933623363289*^9}},
 CellID->129131042],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", "total"], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["x", 
