Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["r", TraditionalForm]], "InlineMath",ExpressionUUID->
  "74a6c739-d94d-4501-9a27-21ef96d458e1"],
 " is the radius of the catalyst particle (m), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["C", 
    StyleBox["A",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",ExpressionUUID->
  "cf665ff2-10e2-4497-832c-9116a91e2ade"],
 " is the concentration in the catalyst (",
 Cell[BoxData[
  FormBox[
   RowBox[{"mol", "/", 
    SuperscriptBox[
     StyleBox["m",
      FontSlant->"Plain"], "3"]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"913730b6-6b69-4cd8-a94a-2ab032a9db80"],
 "), ",
 Cell[BoxData[
  FormBox["k", TraditionalForm]], "InlineMath",ExpressionUUID->
  "76f0c4cb-675b-48b3-85a7-4fb8920233c2"],
 " is the first-order rate constant (1/s), ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["D", 
     StyleBox["e",
      FontSlant->"Plain"]], "=", 
    RowBox[{
     SubscriptBox["D", "AB"], " ", "\[Phi]", " ", 
     RowBox[{"\[Sigma]", "/", "\[Tau]"}]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"0d981637-1ccf-4edc-95a7-210a78101a25"],
 " is the effective diffusivity (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     StyleBox["m",
      FontSlant->"Plain"], "2"], "/", 
    StyleBox["s",
     FontSlant->"Plain"]}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "ff73b08d-004e-4c7f-9e64-b0372f113585"],
 "), ",
 Cell[BoxData[
  FormBox["\[Phi]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "9c6cdde5-43d1-4391-ba0c-0e4590235251"],
 " is the pellet porosity, ",
 Cell[BoxData[
  FormBox["\[Sigma]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "3062baba-6195-4fb7-bbfb-754635ffed72"],
 " is the constriction factor, ",
 Cell[BoxData[
  FormBox["\[Tau]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "7610d956-275b-4b30-ab9a-39dd6d2bd78b"],
 " is tortuosity, and ",
 Cell[BoxData[
  FormBox["\[Phi]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "8f129ad9-28fb-4788-90fa-a07ebb3b31e8"],
 ", ",
 Cell[BoxData[
  FormBox["\[Sigma]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "dcd33888-d502-4344-8011-36564dd801e3"],
 ", and ",
 Cell[BoxData[
  FormBox["\[Tau]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "d250452f-2d0f-4f98-b3cc-c3474496d523"],
 " are set based of typical values for these variables and are unitless."
}], "DetailNotes",
