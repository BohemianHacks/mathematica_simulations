Cell[TextData[{
 "Once the maximum beam deflection ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", 
    RowBox[{"(", "x", ")"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"b2d52199-2191-476b-9f48-2bccebcfc7e5"],
