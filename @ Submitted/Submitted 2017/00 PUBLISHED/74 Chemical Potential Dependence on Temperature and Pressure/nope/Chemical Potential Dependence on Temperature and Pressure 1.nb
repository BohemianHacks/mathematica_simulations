  FormBox[
   RowBox[{
    RowBox[{"\[DifferentialD]", "G"}], "=", 
    RowBox[{
     RowBox[{"V", " ", 
      RowBox[{"\[DifferentialD]", "P"}]}], "-", 
     RowBox[{"S", " ", 
      RowBox[{"\[DifferentialD]", "T"}]}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6925480549640656`*^9, 3.692548118282198*^9}},
 CellID->77248943],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]], "InlineMath"],
 " is volume, ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath"],
 " is pressure, ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]], "InlineMath"],
 " is entropy, and ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath"],
 " is temperature. For a single component system ",
 Cell[BoxData[
  FormBox[
   RowBox[{"G", "=", "\[Mu]"}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.6925480549640656`*^9, 3.6925481398915167`*^9}, {
