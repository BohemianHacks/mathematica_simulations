Cell["", "DetailsSection"],

Cell[TextData[{
 "The lumped capacitance method is used to approximate the temperature \
through a plane wall and an infinite cylinder. This method is a good \
approximation for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Bi", "<", "0.1"}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ":"
}], "DetailNotes",
 CellChangeTimes->{{3.6895337140752935`*^9, 3.6895337165032983`*^9}, {
  3.689533750166*^9, 3.6895338314026284`*^9}},
 CellID->252561104],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"Bi", "=", 
    FractionBox[
     RowBox[{"h", " ", 
      SubscriptBox["L", 
       StyleBox["c",
        FontSlant->"Plain"]]}], "k"]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6895337140752935`*^9, 3.6895337165032983`*^9}, {
  3.689533750166*^9, 3.6895338602629766`*^9}, {3.689533962467992*^9, 
  3.689533962467992*^9}},
 CellID->209110383],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["L", 
     StyleBox["c",
      FontSlant->"Plain"]], "=", "L"}], TraditionalForm]], "InlineMath"],
 " for a plane wall,"
}], "DetailNotes",
 CellChangeTimes->{{3.6895337140752935`*^9, 3.6895337165032983`*^9}, {
  3.689533750166*^9, 3.6895338602629766`*^9}, {3.689533963174821*^9, 
  3.689533973732315*^9}},
 CellID->87979071],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["L", 
     StyleBox["c",
      FontSlant->"Plain"]], "=", 
    RowBox[{
     SubscriptBox["r", "0"], "/", "2"}]}], TraditionalForm]], "InlineMath"],
 " for an infinite cylinder,"
}], "DetailNotes",
 CellChangeTimes->{{3.6895337140752935`*^9, 3.6895337165032983`*^9}, {
  3.689533750166*^9, 3.6895338602629766`*^9}, {3.689533963174821*^9, 
  3.6895339895321*^9}},
 CellID->650364824],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["Bi", TraditionalForm]], "InlineMath"],
 " is the Biot number, ",
 Cell[BoxData[
  FormBox["h", TraditionalForm]], "InlineMath"],
 " is the heat transfer coefficient (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["W",
     FontSlant->"Plain"], "/", 
    RowBox[{"[", 
     RowBox[{
      SuperscriptBox[
       StyleBox["m",
        FontSlant->"Plain"], "2"], " ", 
      StyleBox["K",
       FontSlant->"Plain"]}], "]"}]}], TraditionalForm]], "InlineMath"],
 "), ",
 Cell[BoxData[
  FormBox["k", TraditionalForm]], "InlineMath"],
 " is thermal conductivity (W/[m K]), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["L", 
    StyleBox["c",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " is the characteristic length (m), ",
 Cell[BoxData[
  FormBox["L", TraditionalForm]], "InlineMath"],
 " is length of the wall (m), and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["r", "0"], TraditionalForm]], "InlineMath"],
 " is the radius of the cylinder (m)."
}], "DetailNotes",
 CellChangeTimes->{{3.6895337140752935`*^9, 3.6895337165032983`*^9}, {
  3.689533750166*^9, 3.6895338815444837`*^9}, {3.689533917236661*^9, 
