Cell[TextData[{
 "The saturation pressure of water ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["P", "sat"], TraditionalForm]], "InlineMath"],
 " is calculated using the Antoine equation:"
}], "DetailNotes",
 CellChangeTimes->{{3.6350142851474843`*^9, 3.6350144001610627`*^9}, {
  3.6350151607215643`*^9, 3.6350151630696983`*^9}, {3.635097086908617*^9, 
  3.6350971147582097`*^9}, {3.635700526995995*^9, 3.635700531018021*^9}},
 CellID->144216579],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["log", "10"], "(", 
     SuperscriptBox["P", "sat"], ")"}], "=", 
    RowBox[{"A", "-", 
     FractionBox["B", 
      RowBox[{"T", "+", "C"}]]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6350142851474843`*^9, 3.6350143957838125`*^9}, {
   3.6350144710691185`*^9, 3.635014553895856*^9}, {3.6350151646397886`*^9, 
   3.635015165136817*^9}, 3.635700539234776*^9},
 CellID->260260269],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]], "InlineMath"],
 ", ",
 Cell[BoxData[
  FormBox["B", TraditionalForm]], "InlineMath"],
 ", and ",
 Cell[BoxData[
  FormBox["C", TraditionalForm]], "InlineMath"],
 " are Antoine constants, ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath"],
 " is temperature (\[Degree]C), and ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["P", "sat"], TraditionalForm]], "InlineMath"],
 " is in units of kPa."
}], "DetailNotes",
 CellChangeTimes->{{3.635700623152769*^9, 3.635700738204966*^9}, {
  3.635703982698825*^9, 3.6357039833688636`*^9}},
 CellID->1751483263],

Cell[TextData[{
 "For pure water, the fugacity ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]], "InlineMath"],
 " is equal to the vapor pressure of the pure component,"
}], "DetailNotes",
 CellChangeTimes->{{3.635015370398557*^9, 3.6350154017913523`*^9}, {
  3.635015499990969*^9, 3.6350155029111366`*^9}, {3.635097124987795*^9, 
  3.6350971278349576`*^9}, {3.635700548817603*^9, 3.635700553049429*^9}, {
  3.635700794901005*^9, 3.635700814488224*^9}},
 CellID->1578886081],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{"f", "=", 
   RowBox[{
    SuperscriptBox["P", "sat"], "(", "T", ")"}]}], 
  TraditionalForm]], "InlineMath"]], "DetailNotes",
 CellChangeTimes->{{3.635015370398557*^9, 3.6350154301649756`*^9}},
 CellID->591053136],

Cell["and for water with dissolved salt:", "DetailNotes",
 CellChangeTimes->{{3.635015370398557*^9, 3.635015454384361*^9}, {
  3.6350971608538465`*^9, 3.6350971667631845`*^9}},
 CellID->1718189377],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["f", "^"], "=", 
    RowBox[{
     RowBox[{"x", " ", "f"}], "=", 
     RowBox[{"x", " ", 
      RowBox[{
       SuperscriptBox["P", "sat"], "(", "T", ")"}]}]}]}], TraditionalForm]], 
  "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.635015370398557*^9, 3.635015486156178*^9}, {
   3.635097168834303*^9, 3.6350971688383026`*^9}, 3.6356842498989887`*^9},
 CellID->396258030],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]], "InlineMath"],
 " is the mole fraction of water and ",
