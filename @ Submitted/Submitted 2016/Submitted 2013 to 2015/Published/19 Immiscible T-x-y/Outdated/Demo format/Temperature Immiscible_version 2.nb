Cell[CellGroupData[{

Cell["", "DetailsSection"],

Cell[TextData[{
 "Since the two liquids are immiscible, they each exert their full vapor \
pressure (",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["P", "i", "sat"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ") so the total pressure (",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ") is the sum of these two vapor pressures:"
}], "DetailNotes",
 CellChangeTimes->{{3.6310143271944623`*^9, 3.631014375286213*^9}, {
  3.631014503152527*^9, 3.6310145322631917`*^9}},
 CellID->648874900],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{"P", "=", 
   RowBox[{
    RowBox[{"\[Sum]", 
     SubsuperscriptBox["P", "i", "sat"]}], "=", 
    RowBox[{
     SubsuperscriptBox["P", "1", "sat"], "+", 
     SubsuperscriptBox["P", "2", "sat"]}]}]}], TraditionalForm]], "InlineMath",
 FormatType->"TraditionalForm"]], "DetailNotes",
 CellChangeTimes->{{3.6310143271944623`*^9, 3.6310143979425087`*^9}, {
  3.631014549334168*^9, 3.6310145594997497`*^9}},
 CellID->517921390],

Cell["\<\
The Antoine equation is used to calculate the vapor pressures,\
\>", "DetailNotes",
 CellChangeTimes->{{3.6310143271944623`*^9, 3.631014434032573*^9}},
 CellID->111549307],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["log", "10"], "(", 
   RowBox[{
    SubsuperscriptBox["P", "i", "sat"], "=", 
    RowBox[{
     SubscriptBox["A", "i"], "-", 
     FractionBox[
      SubscriptBox["B", "i"], 
      RowBox[{
       SubscriptBox["C", "i"], "+", "T"}]]}]}], ")"}], 
  TraditionalForm]], "InlineMath",
 FormatType->"TraditionalForm"]], "DetailNotes",
 CellChangeTimes->{{3.6310143271944623`*^9, 3.631014493196957*^9}},
 CellID->416369337],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "i"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["B", "i"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ", and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["C", "i"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " are Antoine constants, and ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is temperature (\[Degree]C)."
}], "DetailNotes",
 CellChangeTimes->{{3.6310143271944623`*^9, 3.6310144956020947`*^9}, {
  3.631014566729163*^9, 3.6310146259615507`*^9}, {3.6310151140004654`*^9, 
  3.6310151140054655`*^9}},
 CellID->100471408],

Cell["\<\
The dew-point curves for the two liquids can be found from solving the \
equations below for temperature,\
\>", "DetailNotes",
 CellChangeTimes->{{3.6310143271944623`*^9, 3.6310144956020947`*^9}, {
  3.631014566729163*^9, 3.6310146259615507`*^9}, {3.6310151140004654`*^9, 
  3.631015160611131*^9}, {3.6310152546795115`*^9, 3.631015289077479*^9}, {
  3.6310153334370165`*^9, 3.6310153362191753`*^9}},
 CellID->30140316],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    RowBox[{
     SubsuperscriptBox["P", "1", "sat"], "(", "T", ")"}], "P"], "=", 
   SubscriptBox["y", "1"]}], TraditionalForm]], "InlineMath",
 FormatType->"TraditionalForm"]], "DetailNotes",
 CellChangeTimes->{{3.6310143271944623`*^9, 3.6310144956020947`*^9}, {
  3.631014566729163*^9, 3.6310146259615507`*^9}, {3.6310151140004654`*^9, 
  3.6310151733938627`*^9}, {3.631015291441614*^9, 3.63101534136547*^9}},
 CellID->23954436],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"1", "-", 
    FractionBox[
     RowBox[{
      SubsuperscriptBox["P", "2", "sat"], "(", "T", ")"}], "P"]}], "=", 
   SubscriptBox["y", "2"]}], TraditionalForm]], "InlineMath",
 FormatType->"TraditionalForm"]], "DetailNotes",
 CellChangeTimes->{{3.6310143271944623`*^9, 3.6310144956020947`*^9}, {
  3.631014566729163*^9, 3.6310146259615507`*^9}, {3.6310151140004654`*^9, 
  3.6310151733938627`*^9}, {3.631015291441614*^9, 3.6310153669819345`*^9}},
 CellID->39321073],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["y", "1"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["y", "2"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " are the vapor mole fractions of benzene-(",
 Cell[BoxData[
  FormBox["1", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ") and water-(",
 Cell[BoxData[
  FormBox["2", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 "). "
}], "DetailNotes",
 CellChangeTimes->{{3.6310143271944623`*^9, 3.6310144956020947`*^9}, {
  3.631014566729163*^9, 3.6310146259615507`*^9}, {3.6310151140004654`*^9, 
  3.6310151733938627`*^9}, {3.631015291441614*^9, 3.6310154336087456`*^9}},
 CellID->54808661],

Cell["\<\
The boiling point is simply the temperature at which the two dew-point curves \
intersect. \
\>", "DetailNotes",
 CellChangeTimes->{{3.6310143271944623`*^9, 3.6310144956020947`*^9}, {
  3.631014566729163*^9, 3.6310146259615507`*^9}, {3.6310151140004654`*^9, 
  3.6310151733938627`*^9}, {3.631015291441614*^9, 3.63101548473567*^9}},
 CellID->5203932]
}, Open  ]],

