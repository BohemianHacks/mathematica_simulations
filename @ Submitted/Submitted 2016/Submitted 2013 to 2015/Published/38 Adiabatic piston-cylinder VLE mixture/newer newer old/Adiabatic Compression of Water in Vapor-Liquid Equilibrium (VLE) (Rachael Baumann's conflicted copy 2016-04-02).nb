 CellID->86337293],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      SubsuperscriptBox["S", "i", "ig"], "-", 
      SubsuperscriptBox["S", 
       StyleBox["R",
        FontSlant->"Plain"], "ig"]}], ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["Cp", 
       StyleBox["A",
        FontSlant->"Plain"]], " ", "ln", " ", 
      FractionBox[
       SubscriptBox["T", "i"], 
       SubscriptBox["T", 
        StyleBox["R",
         FontSlant->"Plain"]]]}], "+", 
     RowBox[{
      SubscriptBox["Cp", 
       StyleBox["B",
        FontSlant->"Plain"]], " ", 
