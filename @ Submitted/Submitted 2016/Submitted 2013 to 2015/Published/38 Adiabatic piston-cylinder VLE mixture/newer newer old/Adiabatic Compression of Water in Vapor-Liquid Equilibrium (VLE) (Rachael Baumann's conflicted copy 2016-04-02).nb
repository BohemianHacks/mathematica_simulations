Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["S", "i"], TraditionalForm]], "InlineMath"],
 " is entropy (J/mol/K), the superscript ",
 Cell[BoxData[
  FormBox["ig", TraditionalForm]], "InlineMath"],
 " represents an ideal gas, the subscript ",
 Cell[BoxData[
  FormBox[
   StyleBox["R",
    FontSlant->"Plain"], TraditionalForm]], "InlineMath"],
 " refers to the reference state, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     SubscriptBox["S", "i"], "-", 
     SubsuperscriptBox["S", "i", "ig"]}], ")"}], TraditionalForm]], 
  "InlineMath"],
 " is the entropy departure function for a real gas, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["S", "R", "ig"], "=", 
    RowBox[{
     RowBox[{"-", "123"}], " ", 
     RowBox[{
      RowBox[{"J", "/", "mol"}], "/", "K"}]}]}], TraditionalForm]], 
  "InlineMath"],
 " is the ideal gas entropy at the reference state, and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "=", 
    RowBox[{"(", 
     RowBox[{"1", ",", "2"}], ")"}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 "."
}], "DetailNotes",
