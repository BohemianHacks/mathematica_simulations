      RowBox[{"(", 
       RowBox[{
        SubsuperscriptBox["T", "i", "2"], "-", 
        SubsuperscriptBox["T", 
         StyleBox["R",
          FontSlant->"Plain"], "2"]}], ")"}]}], "+", 
     RowBox[{
      FractionBox["1", "3"], " ", 
      SubscriptBox["Cp", 
       StyleBox["D",
