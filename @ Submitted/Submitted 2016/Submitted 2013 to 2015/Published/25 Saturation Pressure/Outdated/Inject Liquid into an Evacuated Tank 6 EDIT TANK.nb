Cell[CellGroupData[{

Cell["", "DetailsSection"],

Cell[TextData[{
 "When only vapor is present, the pressure (",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath"],
 ") is calculated using the ideal gas law:"
}], "DetailNotes",
 CellChangeTimes->{{3.6376096784961863`*^9, 3.637609702656568*^9}, {
  3.6376098113037825`*^9, 3.63760981739013*^9}},
 CellID->87697152],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    FractionBox[
     RowBox[{"n", " ", "R", " ", "T"}], "V"]}], TraditionalForm]], 
  "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6376096784961863`*^9, 3.6376097218876677`*^9}},
 CellID->40402104],

Cell[TextData[{
 "when the componet inected is in vapor-liquid equilibrium the the total \
pressure is equal to the saturation pressure (",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["P", "sat"], TraditionalForm]], "InlineMath"],
 "):"
}], "DetailNotes",
 CellChangeTimes->{{3.6376096784961863`*^9, 3.637609758868783*^9}, {
  3.637609894643549*^9, 3.6376098996128335`*^9}},
 CellID->96556927],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     SuperscriptBox["P", "sat"], "=", 
     SuperscriptBox["10", 
      RowBox[{"(", 
       RowBox[{"A", "-", 
        FractionBox["B", 
         RowBox[{"T", "+", "C"}]]}], ")"}]]}]}], TraditionalForm]], 
  "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6376096784961863`*^9, 3.6376098003631563`*^9}},
 CellID->428189557],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath"],
 " is moles, ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]], "InlineMath"],
 " is the ideal gas constant (L bar/mol/K), ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath"],
 " is temperature (K), ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]], "InlineMath"],
 " is volume (L), and ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]], "InlineMath"],
 ", ",
 Cell[BoxData[
  FormBox["B", TraditionalForm]], "InlineMath"],
 ", and ",
 Cell[BoxData[
  FormBox["C", TraditionalForm]], "InlineMath"],
 " are Antoine constants."
}], "DetailNotes",
 CellChangeTimes->{{3.6376096784961863`*^9, 3.6376099345798335`*^9}},
 CellID->417532811]
}, Open  ]],

