Cell["", "DetailsSection"],

Cell[TextData[{
 "When only vapor is present, the pressure (",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath"],
 ") is calculated using the ideal gas law:"
}], "DetailNotes",
 CellChangeTimes->{{3.6376096784961863`*^9, 3.637609702656568*^9}, {
  3.6376098113037825`*^9, 3.63760981739013*^9}, {3.638197648417094*^9, 
  3.638197662806917*^9}, {3.638271692075653*^9, 3.6382717061143684`*^9}},
 CellID->87697152],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{"P", "=", 
   FractionBox[
    RowBox[{"n", " ", "R", " ", "T"}], "V"]}], 
  TraditionalForm]], "InlineMath"]], "DetailNotes",
 CellChangeTimes->{{3.6376096784961863`*^9, 3.6376097218876677`*^9}, 
   3.63827171509754*^9},
 CellID->40402104],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath"],
 " is moles, ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]], "InlineMath"],
 " is the ideal gas constant (L bar/mol/K), ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath"],
 " is temperature (K), and ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]], "InlineMath"],
 " is volume (L). "
}], "DetailNotes",
 CellChangeTimes->{{3.6376096784961863`*^9, 3.637609758868783*^9}, {
  3.637609894643549*^9, 3.6376098996128335`*^9}, {3.638197700912097*^9, 
  3.6381977074664717`*^9}, {3.63827171873871*^9, 3.638271830224533*^9}, {
  3.638283821144185*^9, 3.6382838218722267`*^9}},
 CellID->96556927],

Cell[TextData[{
 "When enough of the component is injected into the tank, both liquid and \
vapor are present in equilibrium, and the pressure is equal to the saturation \
pressure (",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["P", "sat"], TraditionalForm]], "InlineMath"],
 ") at the temperature of the tank:"
}], "DetailNotes",
 CellChangeTimes->{{3.6376096784961863`*^9, 3.637609758868783*^9}, {
  3.637609894643549*^9, 3.6376098996128335`*^9}, {3.638197700912097*^9, 
  3.6381977074664717`*^9}, {3.63827171873871*^9, 3.638271840229529*^9}, {
  3.638283836859084*^9, 3.6382838393962293`*^9}},
 CellID->408665561],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     SuperscriptBox["P", "sat"], "=", 
     SuperscriptBox["10", 
      RowBox[{"(", 
       RowBox[{"A", "-", 
        FractionBox["B", 
         RowBox[{"T", "+", "C"}]]}], ")"}]]}]}], TraditionalForm]], 
  "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6376096784961863`*^9, 3.6376098003631563`*^9}},
 CellID->428189557],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]], "InlineMath"],
 ", ",
 Cell[BoxData[
  FormBox["B", TraditionalForm]], "InlineMath"],
 ", and ",
 Cell[BoxData[
  FormBox["C", TraditionalForm]], "InlineMath"],
 " are Antoine constants."
}], "DetailNotes",
 CellChangeTimes->{{3.6376096784961863`*^9, 3.6376099345798335`*^9}, 
   3.638271819669467*^9, {3.6382718518230257`*^9, 3.6382718553310614`*^9}},
