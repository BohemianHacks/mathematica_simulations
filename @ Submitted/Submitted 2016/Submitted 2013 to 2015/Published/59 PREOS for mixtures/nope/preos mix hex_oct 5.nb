  FormBox[
   RowBox[{"b", "=", 
    RowBox[{"0.0778", " ", 
     FractionBox[
      RowBox[{"R", " ", "Tc"}], "Pc"]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6594726538979187`*^9, 3.6594728543953867`*^9}, {
  3.659472972172123*^9, 3.659473015377594*^9}, {3.6594730474734297`*^9, 
