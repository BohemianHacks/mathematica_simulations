Cell[CellGroupData[{

Cell["", "DetailsSection"],

Cell[TextData[{
 "The Peng-Robinson equation of state for mixtures is used to plot pressure ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath"],
 " versus volume ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]], "InlineMath"],
 ":"
}], "DetailNotes",
 CellChangeTimes->{{3.6594726538979187`*^9, 3.659472702149678*^9}, 
   3.659474271050414*^9},
 CellID->46145746],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     FractionBox[
      RowBox[{"R", " ", "T"}], 
      RowBox[{"V", "-", "bm"}]], "-", 
     FractionBox["am", 
      RowBox[{
       SuperscriptBox["V", "2"], "+", 
       RowBox[{"2", " ", "V", " ", "bm"}], "-", 
       SuperscriptBox["bm", "2"]}]]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6594726538979187`*^9, 3.6594727759298983`*^9}},
 CellID->586567212],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]], "InlineMath"],
 " is the gas constant (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["cm", "3"], " ", 
    RowBox[{
     RowBox[{"MPa", "/", "mol"}], "/", "K"}]}], TraditionalForm]], 
  "InlineMath"],
 "), ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath"],
 " is temperature (K), ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath"],
 " is in MPa, and ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]], "InlineMath"],
 " is in ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["cm", "3"], "/", "mol"}], TraditionalForm]], "InlineMath"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.6594726538979187`*^9, 3.6594728543993864`*^9}, 
   3.6594742810009837`*^9},
 CellID->294065433],

Cell[TextData[{
 Cell[BoxData[
  FormBox["am", TraditionalForm]], "InlineMath"],
 " is the attraction parameter and ",
 Cell[BoxData[
  FormBox["bm", TraditionalForm]], "InlineMath"],
 " is the repulsion parameter for the mixture:"
}], "DetailNotes",
 CellChangeTimes->{{3.6594726538979187`*^9, 3.6594728543953867`*^9}, {
  3.659472972172123*^9, 3.65947298261672*^9}, {3.659473085719617*^9, 
  3.65947309346106*^9}},
 CellID->338268364],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"am", "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"i", "=", "1"}], "2"], 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"j", "=", "1"}], "2"], 
      RowBox[{
       SubscriptBox["z", "i"], " ", 
       SubscriptBox["z", "j"], " ", 
       RowBox[{"(", 
        RowBox[{"1", "-", 
         SubscriptBox["k", "ij"]}], ")"}], " ", 
       SqrtBox[
        RowBox[{
         SubscriptBox["a", "i"], " ", 
         SubscriptBox["a", "j"]}]]}]}]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6594726538979187`*^9, 3.6594728543953867`*^9}, {
  3.659472972172123*^9, 3.659473015377594*^9}, {3.6594730474734297`*^9, 
  3.659473072595867*^9}},
 CellID->393070321],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"bm", "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"i", "=", "1"}], "2"], 
     RowBox[{
      SubscriptBox["z", "i"], " ", 
      SubscriptBox["b", "i"]}]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6594726538979187`*^9, 3.6594728543953867`*^9}, {
  3.659472972172123*^9, 3.659473015377594*^9}, {3.6594730474734297`*^9, 
  3.659473122448718*^9}},
 CellID->658830919],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["z", TraditionalForm]], "InlineMath"],
 " is the component mole fraction, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["k", "ij"], TraditionalForm]], "InlineMath"],
 " is the binary interaction parameter, and ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox["b", TraditionalForm]], "InlineMath"],
 " are the attraction and repulsion parameters for a pure component."
}], "DetailNotes",
 CellChangeTimes->{{3.6594726538979187`*^9, 3.6594728543953867`*^9}, {
  3.659472972172123*^9, 3.659473015377594*^9}, {3.6594730474734297`*^9, 
  3.659473215405035*^9}},
 CellID->234946049],

Cell["The binary interaction parameter is calculated [1]:", "DetailNotes",
 CellChangeTimes->{{3.6594726538979187`*^9, 3.6594728543953867`*^9}, {
   3.659472972172123*^9, 3.659473015377594*^9}, {3.6594730474734297`*^9, 
   3.659473264782859*^9}, 3.6594733656796303`*^9},
 CellID->147258644],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["k", "ij"], "=", 
    RowBox[{"1", "=", 
     RowBox[{
      RowBox[{
       FractionBox["1", "2"], " ", 
       FractionBox[
        SubscriptBox["b", "2"], 
        SubscriptBox["b", "1"]], " ", 
       SqrtBox[
        FractionBox[
         SubscriptBox["a", "1"], 
         SubscriptBox["a", "2"]]]}], "-", 
      RowBox[{
       FractionBox["1", "2"], " ", 
       FractionBox[
        SubscriptBox["b", "1"], 
        SubscriptBox["b", "2"]], " ", 
       SqrtBox[
        FractionBox[
         SubscriptBox["a", "2"], 
         SubscriptBox["a", "1"]]]}], "+", 
      RowBox[{
       FractionBox["1", "2"], " ", 
       FractionBox[
        RowBox[{
         SubscriptBox["b", "2"], " ", "R", " ", "T"}], 
        SqrtBox[
         RowBox[{
          SubscriptBox["a", "1"], " ", 
          SubscriptBox["a", "2"]}]]], " ", 
       FractionBox[
        SubscriptBox["\[Theta]", "1"], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"T", "/", 
           SubscriptBox["Tc", "1"]}], ")"}], 
         SubscriptBox["\[Theta]", "2"]]]}]}]}]}], TraditionalForm]], 
  "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6594726538979187`*^9, 3.6594728543953867`*^9}, {
  3.659472972172123*^9, 3.659473015377594*^9}, {3.6594730474734297`*^9, 
  3.659473346134512*^9}},
 CellID->8820022],

Cell[TextData[{
 " for an alkane/alkane mixture ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Theta]", "1"], "=", "0.22806"}], TraditionalForm]], 
  "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Theta]", "2"], "=", "0.18772"}], TraditionalForm]], 
  "InlineMath"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.6594726538979187`*^9, 3.6594728543953867`*^9}, {
  3.659472972172123*^9, 3.659473015377594*^9}, {3.6594730474734297`*^9, 
  3.6594733895339947`*^9}, {3.6594737183057995`*^9, 3.659473718309799*^9}},
 CellID->665083523],

Cell["\<\
The attraction and repulsion parameters for a pure component are:\
\>", "DetailNotes",
 CellChangeTimes->{{3.6594726538979187`*^9, 3.6594728543953867`*^9}, {
  3.659472972172123*^9, 3.659473015377594*^9}, {3.6594730474734297`*^9, 
  3.6594733895339947`*^9}, {3.6594737183057995`*^9, 3.6594737403380594`*^9}},
 CellID->4595339],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"a", "=", 
    RowBox[{"0.457", " ", 
     FractionBox[
      RowBox[{
       SuperscriptBox["R", "2"], " ", 
       SuperscriptBox["Tc", "2"]}], "Pc"], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{"\[Kappa]", " ", 
