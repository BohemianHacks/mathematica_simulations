Cell[TextData[{
 "The feed (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["F", "\[Bullet]"], "=", 
    RowBox[{"1000", " ", 
     RowBox[{"kg", "/", 
      StyleBox["h",
       FontSlant->"Plain"]}]}]}], TraditionalForm]], "InlineMath"],
