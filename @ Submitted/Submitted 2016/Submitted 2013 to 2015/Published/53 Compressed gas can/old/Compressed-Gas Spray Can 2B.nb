Cell[CellGroupData[{

Cell["", "DetailsSection"],

Cell[TextData[{
 "The total volume of the can (",
 Cell[BoxData[
  FormBox[
   SubscriptBox["V", "can"], TraditionalForm]], "InlineMath"],
 ") is 0.375 L, and the initial volume of liquid in the can (",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["V", "0", 
    StyleBox["L",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 ") is:"
}], "DetailNotes",
 CellChangeTimes->{{3.65159188151482*^9, 3.6515920583499346`*^9}, {
  3.651592160686788*^9, 3.651592194768738*^9}, {3.651867580310506*^9, 
  3.651867591908169*^9}, {3.6519492301605434`*^9, 3.6519492843555317`*^9}, {
  3.651949315203174*^9, 3.6519493256221237`*^9}, {3.6519493670185227`*^9, 
  3.651949400107519*^9}},
 CellID->291613031],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SubsuperscriptBox["V", "0", 
    StyleBox["L",
     FontSlant->"Plain"]], "=", 
   RowBox[{"f", " ", 
    SubscriptBox["V", "can"]}]}], 
  TraditionalForm]], "InlineMath"]], "DetailNotes",
 CellChangeTimes->{{3.65159188151482*^9, 3.6515920583499346`*^9}, {
  3.651592160686788*^9, 3.651592215592929*^9}},
 CellID->879942446],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["V", "0", 
    StyleBox["L",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " is in L, and ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]], "InlineMath"],
 " is the initial volume fraction of liquid."
}], "DetailNotes",
 CellChangeTimes->{{3.65159188151482*^9, 3.6515920583499346`*^9}, {
  3.651592160686788*^9, 3.6515922538781185`*^9}, {3.651867565671668*^9, 
  3.6518675701029215`*^9}, {3.6520133577091217`*^9, 3.6520133581331453`*^9}},
 CellID->794859098],

Cell[TextData[{
 "Initially all contents are at room temperature (300 K) and the pressure \
inside the can is the saturation pressure (",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["P", "sat"], TraditionalForm]], "InlineMath"],
 ") at 300 K.  The Antoine equation is used to calculate ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["P", "sat"], TraditionalForm]], "InlineMath"],
 ":"
}], "DetailNotes",
 CellChangeTimes->{{3.65159188151482*^9, 3.6515920793541365`*^9}},
 CellID->47002862],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SuperscriptBox["P", "sat"], "=", 
   RowBox[{"10", "^", 
    RowBox[{"(", 
     RowBox[{"A", "-", 
      FractionBox["B", 
       RowBox[{"T", "+", "C"}]]}], ")"}]}]}], 
  TraditionalForm]], "InlineMath"]], "DetailNotes",
 CellChangeTimes->{{3.65159188151482*^9, 3.6515920959450855`*^9}},
 CellID->166088824],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["P", "sat"], TraditionalForm]], "InlineMath"],
 " is in bar, ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath"],
 " is temperature (K), and ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]], "InlineMath"],
 ", ",
 Cell[BoxData[
  FormBox["B", TraditionalForm]], "InlineMath"],
 ", and ",
 Cell[BoxData[
  FormBox["C", TraditionalForm]], "InlineMath"],
 " are Antoine constants."
}], "DetailNotes",
 CellChangeTimes->{{3.65159188151482*^9, 3.651592133605239*^9}},
 CellID->257919867],

Cell[TextData[{
 "The total moles (",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath"],
 ") is equal to the the liquid moles (",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["n", 
    StyleBox["L",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 ") plus the vapor moles (",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["n", 
    StyleBox["V",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 "):"
}], "DetailNotes",
 CellChangeTimes->{{3.65159188151482*^9, 3.6515921336012387`*^9}, {
  3.651592303682967*^9, 3.6515925119328785`*^9}, {3.651593179697072*^9, 
  3.651593207906686*^9}},
 CellID->41841307],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{"n", "=", 
   RowBox[{
    SuperscriptBox["n", 
     StyleBox["L",
      FontSlant->"Plain"]], "+", 
    SuperscriptBox["n", 
     StyleBox["V",
      FontSlant->"Plain"]]}]}], 
  TraditionalForm]], "InlineMath"]], "DetailNotes",
 CellChangeTimes->{{3.65159188151482*^9, 3.6515921336012387`*^9}, {
  3.651592303682967*^9, 3.65159251808123*^9}},
 CellID->250525888],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SuperscriptBox["n", 
    StyleBox["L",
     FontSlant->"Plain"]], "=", 
   RowBox[{
    SuperscriptBox["\[Rho]", 
     StyleBox["L",
      FontSlant->"Plain"]], " ", 
    SuperscriptBox["V", 
     StyleBox["L",
      FontSlant->"Plain"]]}]}], 
  TraditionalForm]], "InlineMath"]], "DetailNotes",
 CellChangeTimes->{{3.65159188151482*^9, 3.6515921336012387`*^9}, {
  3.651592303682967*^9, 3.6515925417095814`*^9}},
 CellID->563793043],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["n", 
     StyleBox["V",
      FontSlant->"Plain"]], "=", 
    FractionBox[
     RowBox[{
      SuperscriptBox["V", 
       StyleBox["V",
        FontSlant->"Plain"]], " ", 
      SuperscriptBox["P", "sat"]}], 
     RowBox[{"R", " ", "T"}]]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.65159188151482*^9, 3.6515921336012387`*^9}, {
  3.651592303682967*^9, 3.651592568890136*^9}},
 CellID->850406981],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[Rho]", 
    StyleBox["L",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " is the liquid molar density (mol/L), and ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]], "InlineMath"],
 " is the ideal gas constant (L bar/mol/K)."
}], "DetailNotes",
 CellChangeTimes->{{3.65159188151482*^9, 3.6515921336012387`*^9}, {
  3.651592303682967*^9, 3.651592627109466*^9}, {3.6515936319309387`*^9, 
  3.6515936699921155`*^9}},
 CellID->196677009],

Cell["\<\
The liquid volume is found by rearranging the equation for total moles:\
\>", "DetailNotes",
 CellChangeTimes->{{3.65159188151482*^9, 3.6515921336012387`*^9}, {
   3.651592303682967*^9, 3.6515926868978853`*^9}, 3.6518479767232447`*^9},
 CellID->232008480],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SuperscriptBox["V", 
    StyleBox["L",
     FontSlant->"Plain"]], "=", 
   FractionBox[
    RowBox[{"n", "-", 
     RowBox[{
      SubscriptBox["V", "can"], " ", 
      RowBox[{
       SuperscriptBox["P", "sat"], "/", 
       RowBox[{"(", 
        RowBox[{"R", " ", "T"}], ")"}]}]}]}], 
    RowBox[{
     SuperscriptBox["\[Rho]", 
      StyleBox["L",
       FontSlant->"Plain"]], "-", 
     RowBox[{
      SuperscriptBox["P", "sat"], "/", 
      RowBox[{"(", 
       RowBox[{"R", " ", "T"}], ")"}]}]}]]}], 
  TraditionalForm]], "InlineMath"]], "DetailNotes",
 CellChangeTimes->{{3.6515926748101945`*^9, 3.651592727218192*^9}, 
   3.6519511908510685`*^9},
 CellID->320680653],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SuperscriptBox["V", 
    StyleBox["V",
     FontSlant->"Plain"]], "=", 
   RowBox[{
    SubscriptBox["V", "can"], "-", 
    SuperscriptBox["V", 
     StyleBox["L",
      FontSlant->"Plain"]]}]}], 
  TraditionalForm]], "InlineMath"]], "DetailNotes",
 CellChangeTimes->{{3.6515926748101945`*^9, 3.651592737567784*^9}, {
  3.6520134301612654`*^9, 3.6520134301922674`*^9}},
 CellID->302141301],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["V", 
    StyleBox["L",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["V", 
    StyleBox["V",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " are the liquid and vapor volumes, respectively (L)."
}], "DetailNotes",
 CellChangeTimes->{{3.6515926748101945`*^9, 3.6515927697176228`*^9}, {
  3.6520134622791023`*^9, 3.652013463169153*^9}},
 CellID->628499920],

Cell["From an unsteady state mole balance:", "DetailNotes",
 CellChangeTimes->{{3.65159188151482*^9, 3.6515921336012387`*^9}, {
   3.651592303682967*^9, 3.6515923229340677`*^9}, 3.6515932173602266`*^9},
 CellID->78302062],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    RowBox[{"\[DifferentialD]", "n"}], 
    RowBox[{"\[DifferentialD]", "t"}]], "=", 
   RowBox[{
    RowBox[{"-", "\[Alpha]"}], " ", 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["P", "sat"], "-", "1"}], ")"}]}]}], 
  TraditionalForm]], "InlineMath"]], "DetailNotes",
 CellChangeTimes->{{3.65159188151482*^9, 3.6515921336012387`*^9}, {
  3.651592303682967*^9, 3.6515923521887417`*^9}, {3.6515932346712165`*^9, 
  3.6515932346752167`*^9}},
 CellID->1029651035],

Cell[TextData[{
 "at ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "=", "0"}], TraditionalForm]], "InlineMath"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "=", 
    SubscriptBox["n", 
     RowBox[{"T", "=", "300"}]]}], TraditionalForm]], "InlineMath"]
}], "DetailNotes",
 CellChangeTimes->{{3.65159188151482*^9, 3.6515921336012387`*^9}, {
  3.651592303682967*^9, 3.6515923521887417`*^9}, {3.6515932346712165`*^9, 
  3.6515932661030145`*^9}},
 CellID->239660188],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["t", TraditionalForm]], "InlineMath"],
 " is time (s), ",
 Cell[BoxData[
  FormBox["\[Alpha]", TraditionalForm]], "InlineMath"],
 " is a constant (mol/bar/s), and the air pressure outside of the can is 1 \
bar."
}], "DetailNotes",
 CellChangeTimes->{{3.65159188151482*^9, 3.6515921336012387`*^9}, {
   3.651592303682967*^9, 3.6515924353084955`*^9}, {3.651593221081439*^9, 
   3.6515932217894793`*^9}, 3.651847979525405*^9},
 CellID->217837831],

Cell["From the energy balance:", "DetailNotes",
 CellChangeTimes->{{3.651593277624673*^9, 3.6515932886263027`*^9}},
 CellID->71908871],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    RowBox[{"\[DifferentialD]", "T"}], 
    RowBox[{"\[DifferentialD]", "t"}]], "=", 
   RowBox[{"-", 
    FractionBox[
     RowBox[{"\[Alpha]", " ", 
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["P", "sat"], "-", "1"}], ")"}], " ", "\[CapitalDelta]",
       "\[InvisibleSpace]", 
      SuperscriptBox["H", "vap"]}], 
     RowBox[{"n", " ", "Cp"}]]}]}], 
  TraditionalForm]], "InlineMath"]], "DetailNotes",
 CellChangeTimes->{{3.651593277624673*^9, 3.6515933291606207`*^9}, {
  3.651593391930211*^9, 3.6515933919332113`*^9}},
 CellID->436390520],

Cell[TextData[{
 "at ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "=", "0"}], TraditionalForm]], "InlineMath"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{"T", "=", 
    RowBox[{"300", " ", "K"}]}], TraditionalForm]], "InlineMath"]
}], "DetailNotes",
 CellChangeTimes->{{3.651593277624673*^9, 3.6515933291606207`*^9}, {
  3.651593391930211*^9, 3.6515934060050163`*^9}},
 CellID->635436400],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", 
    SuperscriptBox["H", "vap"]}], TraditionalForm]], "InlineMath"],
 " is the heat of vaporization (kJ/mol), and ",
 Cell[BoxData[
  FormBox["Cp", TraditionalForm]], "InlineMath"],
 " is the liquid heat capacity (kJ/mol/K)."
}], "DetailNotes",
 CellChangeTimes->{{3.651593277624673*^9, 3.651593387910981*^9}},
 CellID->163297344]
}, Open  ]],

