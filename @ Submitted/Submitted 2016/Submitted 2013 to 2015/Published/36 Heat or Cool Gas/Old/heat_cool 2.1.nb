Cell[CellGroupData[{

Cell["", "DetailsSection"],

Cell[TextData[{
 "The final temperatures (",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ") of the constant pressure and constant volume systems are found using:"
}], "DetailNotes",
 CellChangeTimes->{{3.6422647881897087`*^9, 3.642264853382437*^9}},
 CellID->110900824],

Cell["\[CapitalDelta]U = Q+W", "DetailNotes",
 CellChangeTimes->{{3.6422647881897087`*^9, 3.6422648417957745`*^9}, {
  3.642286436740859*^9, 3.6422864644149113`*^9}, {3.642286594441619*^9, 
  3.6422865963986473`*^9}, {3.6422869548617744`*^9, 3.642286999661549*^9}},
 CellID->598315815],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", "U"}], TraditionalForm]], 
  "InlineMath"],
 " is the change in internal energy (kJ/mol)"
}], "DetailNotes",
 CellChangeTimes->{{3.6422647881897087`*^9, 3.6422648417957745`*^9}, {
  3.642286436740859*^9, 3.6422864644149113`*^9}, {3.642286594441619*^9, 
  3.6422865963986473`*^9}, {3.6422869548617744`*^9, 3.6422869814297547`*^9}},
 CellID->223637760],

Cell["For the constant-volume process, W = 0.", "DetailNotes",
 CellChangeTimes->{{3.6422647881897087`*^9, 3.6422648417957745`*^9}, {
  3.642286436740859*^9, 3.6422864820022697`*^9}},
 CellID->173536423],

Cell[TextData[{
 "For the constant pressure process, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"W", "=", " ", 
    RowBox[{
     RowBox[{"-", "P"}], " ", "\[CapitalDelta]", "\[InvisibleSpace]", "V"}]}],
    TraditionalForm]], "InlineMath"],
 " = RDT, so DU = Q - PDV = Q - RDT"
}], "DetailNotes",
 CellChangeTimes->{{3.6422647881897087`*^9, 3.6422648417957745`*^9}, {
  3.642286436740859*^9, 3.6422865561711874`*^9}, {3.642287043590764*^9, 
  3.6422870522429256`*^9}},
 CellID->38227106],

Cell["and thus DH = Q", "DetailNotes",
 CellChangeTimes->{{3.6422647881897087`*^9, 3.6422648417957745`*^9}, {
  3.642286436740859*^9, 3.6422865709136868`*^9}, {3.6422869855271864`*^9, 
  3.6422869855271864`*^9}},
 CellID->571812156],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", "H"}], TraditionalForm]], 
  "InlineMath"],
 " is the enthalpy change (kJ/mol), ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath"],
 " is pressure (Pa), and ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]], "InlineMath"],
 " is in ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["m", "3"], "/", "mol"}], TraditionalForm]], "InlineMath"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.6422647881897087`*^9, 3.6422648417957745`*^9}, {
  3.642286436740859*^9, 3.6422865709136868`*^9}, {3.6422869855271864`*^9, 
  3.642286986596773*^9}, {3.6422870806977463`*^9, 3.6422870815450544`*^9}, {
  3.642287119675495*^9, 3.642287143293457*^9}},
 CellID->535874043],

Cell["For both processes,  DU = CvDT  and DH = CpDT", "DetailNotes",
 CellChangeTimes->{{3.6422647881897087`*^9, 3.6422649930264244`*^9}, {
  3.642286624641512*^9, 3.6422866276757727`*^9}},
 CellID->809237414],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"Cp", "=", 
    RowBox[{"7", " ", 
     RowBox[{"R", "/", "2"}]}]}], TraditionalForm]], "InlineMath"],
 "  and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Cv", "=", 
    RowBox[{"5", " ", 
     RowBox[{"R", "/", "2"}]}]}], TraditionalForm]], "InlineMath"],
 " "
}], "DetailNotes",
 CellChangeTimes->{{3.6422647881897087`*^9, 3.642265019594944*^9}, {
  3.64228665527147*^9, 3.6422866653902626`*^9}},
 CellID->81304728],

Cell[TextData[{
 Cell[BoxData[
  FormBox["Q", TraditionalForm]], "InlineMath"],
 " is heat added or removed (kJ/mol), and ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]], "InlineMath"],
 " is the ideal gas constant."
}], "DetailNotes",
 CellChangeTimes->{{3.6422647881897087`*^9, 3.6422650497996716`*^9}, {
  3.6422866920765505`*^9, 3.642286703662302*^9}},
 CellID->749066],

Cell["", "DetailNotes",
 CellChangeTimes->{{3.6422647881897087`*^9, 3.642265049793671*^9}, {
  3.6422650818395042`*^9, 3.6422650818435044`*^9}},
 CellID->95155056],

Cell["\<\
The ideal gas law is used to calculate the final volume for the \
constant-pressure process and the final pressure for the constant-volume \
process\
\>", "DetailNotes",
