Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["K", 
     StyleBox["e",
      FontSlant->"Plain"]], "=", 
    RowBox[{
     SubscriptBox["K", 
      StyleBox["m",
       FontSlant->"Plain"]], " ", 
     RowBox[{"exp", "[", 
      RowBox[{
       FractionBox["\[CapitalDelta]H", "R"], " ", 
       RowBox[{"(", 
        RowBox[{
         SubsuperscriptBox["T", 
          StyleBox["m",
           FontSlant->"Plain"], 
          RowBox[{"-", "1"}]], "-", 
         SuperscriptBox["T", 
          RowBox[{"-", "1"}]]}], ")"}]}], "]"}]}]}], TraditionalForm]], 
  "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
