Cell[CellGroupData[{

Cell["", "DetailsSection"],

Cell[TextData[{
 "The total pressure (",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath"],
 ") inside the autoclave is the sum of the saturation pressure of water (",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["P", "w", "sat"], TraditionalForm]], "InlineMath"],
 ") and the pressure of air (",
 Cell[BoxData[
  FormBox[
   SubscriptBox["P", "a"], TraditionalForm]], "InlineMath"],
 "):"
}], "RelatedLinks",
 CellChangeTimes->{{3.6470980483121567`*^9, 3.6470980945698023`*^9}, {
  3.6470981348911085`*^9, 3.647098160008545*^9}},
 CellID->455919848],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     SubsuperscriptBox["P", "w", "sat"], "+", 
     SubscriptBox["P", "a"]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "RelatedLinks",
 CellChangeTimes->{{3.6470980483121567`*^9, 3.64709812861475*^9}},
 CellID->10348285],

Cell["\<\
where the saturation pressure is calculated using the Antoine equation:\
\>", "RelatedLinks",
 CellChangeTimes->{{3.6470980483121567`*^9, 3.647098131166896*^9}, {
  3.6470981641257806`*^9, 3.647098178815621*^9}},
 CellID->407372020],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["P", "w", "sat"], "=", 
    RowBox[{"10", "^", 
     RowBox[{"(", 
      RowBox[{"A", "-", 
       FractionBox["B", 
        RowBox[{"T", "+", "C"}]]}], ")"}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "RelatedLinks",
 CellChangeTimes->{{3.6470982291254983`*^9, 3.6470982533588843`*^9}},
 CellID->112584854],

Cell[TextData[{
 "and ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]], "InlineMath"],
 ", ",
 Cell[BoxData[
  FormBox["B", TraditionalForm]], "InlineMath"],
 ", and ",
 Cell[BoxData[
  FormBox["C", TraditionalForm]], "InlineMath"],
 " are Antoine constants. The pressure of air is calculated from the ideal \
gas law:"
}], "RelatedLinks",
 CellChangeTimes->{{3.6470982291254983`*^9, 3.6470982696738176`*^9}, {
  3.647169814429951*^9, 3.6471698242198877`*^9}, {3.6471780190927534`*^9, 
  3.6471780193707695`*^9}},
 CellID->379084695],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["P", "a"], "=", 
    RowBox[{
     SubscriptBox["n", "a"], " ", "R", " ", 
     RowBox[{"T", "/", 
      SubscriptBox["V", "a"]}]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "RelatedLinks",
 CellChangeTimes->{{3.6470982291254983`*^9, 3.6470982942292223`*^9}, {
  3.6470983379977255`*^9, 3.647098338883776*^9}},
 CellID->110273158],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["n", "a"], TraditionalForm]], "InlineMath"],
 " is moles of air, ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]], "InlineMath"],
 " is the ideal gas constant, ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath"],
 " is temperature (K), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["V", "a"], TraditionalForm]], "InlineMath"],
 " is the volume of air (L), and pressures are in bar."
}], "RelatedLinks",
 CellChangeTimes->{{3.6470982291254983`*^9, 3.647098411281917*^9}, {
  3.6471698105251794`*^9, 3.6471698392125196`*^9}},
 CellID->65422811],

Cell[TextData[{
 "The volume of the autoclave is 1 L, so the volume of air is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["V", "a"], "=", 
    RowBox[{"1", "-", 
     SubscriptBox["V", "w"]}]}], TraditionalForm]], "InlineMath"],
 ", where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["V", "w"], TraditionalForm]], "InlineMath"],
 " is the volume of liquid water (L)."
}], "RelatedLinks",
 CellChangeTimes->{{3.6470982291254983`*^9, 3.6470984737774916`*^9}, {
  3.6470985096105413`*^9, 3.6470985096155415`*^9}, {3.647169852220741*^9, 
  3.6471698654130955`*^9}},
 CellID->38457464],

Cell[TextData[{
 "The air contains only oxygen (",
 Cell[BoxData[
  FormBox[
   SubscriptBox["n", 
    SubscriptBox["O", "2"]], TraditionalForm]], "InlineMath"],
 ") and nitrogen (",
 Cell[BoxData[
  FormBox[
   SubscriptBox["n", 
    SubscriptBox["N", "2"]], TraditionalForm]], "InlineMath"],
 ")  so ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["n", "a"], "=", 
    RowBox[{
     SubscriptBox["n", 
      SubscriptBox["O", "2"]], "+", 
     SubscriptBox["n", 
      SubscriptBox["N", "2"]]}]}], TraditionalForm]], "InlineMath"],
 "."
}], "RelatedLinks",
 CellChangeTimes->{{3.6470982291254983`*^9, 3.6470984737774916`*^9}, {
  3.6470985096105413`*^9, 3.6470986678935947`*^9}, {3.6471698691217194`*^9, 
  3.6471699213967786`*^9}},
 CellID->44118346],

Cell[TextData[{
 "The solubilities of oxygen and nitrogen (",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", 
    SubscriptBox["O", "2"]], TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", 
    SubscriptBox["N", "2"]], TraditionalForm]], "InlineMath"],
 ") are used to calculate the moles dissolved (",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["n", "i", "L"], TraditionalForm]], "InlineMath"],
 "):"
}], "RelatedLinks",
 CellChangeTimes->{{3.6470982291254983`*^9, 3.6470984737774916`*^9}, {
   3.6470985096105413`*^9, 3.6470986379138794`*^9}, {3.647100560429841*^9, 
   3.647100637897272*^9}, {3.647100825827021*^9, 3.6471008367076435`*^9}, {
   3.6471699371729636`*^9, 3.647169938212716*^9}, 3.6471781311691637`*^9},
 CellID->304741118],

Cell[TextData[{
 "oxygen: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["n", 
     SubscriptBox["O", "2"], "L"], "=", 
    RowBox[{"0.21", " ", 
     SubscriptBox["V", "w"], " ", 
     RowBox[{"P", "/", 
      SubscriptBox["c", 
       SubscriptBox["O", "2"]]}]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "RelatedLinks",
 CellChangeTimes->{{3.6470982291254983`*^9, 3.6470984737774916`*^9}, {
  3.6470985096105413`*^9, 3.6470986379138794`*^9}, {3.647100560429841*^9, 
  3.64710074573244*^9}},
 CellID->388870751],

Cell[TextData[{
 "nitrogen: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["n", 
     SubscriptBox["N", "2"], "L"], "=", 
    RowBox[{"0.79", " ", 
     SubscriptBox["V", "w"], " ", 
     RowBox[{"P", "/", 
      SubscriptBox["c", 
       SubscriptBox["N", "2"]]}]}]}], TraditionalForm]], "InlineMath"],
 "."
}], "RelatedLinks",
 CellChangeTimes->{{3.6470982291254983`*^9, 3.6470984737774916`*^9}, {
  3.6470985096105413`*^9, 3.6470986379138794`*^9}, {3.647100560429841*^9, 
  3.6471007790073433`*^9}, {3.6471008429710016`*^9, 3.6471008466572123`*^9}},
 CellID->116153824],

Cell[TextData[{
 "A mass balance calculates the moles in the gas phase (",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["n", "i", "V"], TraditionalForm]], "InlineMath"],
 "):"
}], "RelatedLinks",
 CellChangeTimes->{{3.6470982291254983`*^9, 3.6470984737774916`*^9}, {
  3.6470985096105413`*^9, 3.6470986379138794`*^9}, {3.647100560429841*^9, 
  3.6471007790073433`*^9}, {3.6471008429710016`*^9, 3.6471008466532125`*^9}, {
  3.647100881041179*^9, 3.6471009302929964`*^9}, {3.6471699607977123`*^9, 
  3.647169968477155*^9}},
 CellID->31986291],

Cell[TextData[{
 "oxygen: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["n", 
     SubscriptBox["O", "2"], "V"], "=", 
    RowBox[{
     RowBox[{"0.21", " ", 
      SubscriptBox["n", "a"]}], "-", 
     SubsuperscriptBox["n", 
      SubscriptBox["O", "2"], "L"]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "RelatedLinks",
 CellChangeTimes->{{3.6470982291254983`*^9, 3.6470984737774916`*^9}, {
  3.6470985096105413`*^9, 3.6470986379138794`*^9}, {3.647100560429841*^9, 
  3.6471007790073433`*^9}, {3.6471008429710016`*^9, 3.6471008466532125`*^9}, {
  3.647100881041179*^9, 3.6471009461389027`*^9}, {3.6471009882883134`*^9, 
  3.6471010256714516`*^9}},
 CellID->60310487],

Cell[TextData[{
 "nitrogen: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["n", 
     SubscriptBox["N", "2"], "V"], "=", 
    RowBox[{
     RowBox[{"0.79", " ", 
      SubscriptBox["n", "a"]}], "-", 
