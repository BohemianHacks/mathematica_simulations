Cell[TextData[{
 "This system is modeled by Raoult's law. The saturation pressures ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["P", "i", "sat"], TraditionalForm]], "InlineMath"],
 " (bar) of ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath"],
 "-hexane and ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath"],
 "-octane are calculated from the Antoine equation, where the Antoine \
coefficients ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "i"], TraditionalForm]], "InlineMath"],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["B", "i"], TraditionalForm]], "InlineMath"],
 ", and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["C", "i"], TraditionalForm]], "InlineMath"],
 " are constants and ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath"],
 " (\[Degree]C) is temperature:"
}], "DetailNotes",
