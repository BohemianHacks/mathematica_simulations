Cell[CellGroupData[{

Cell["", "DetailsSection"],

Cell["\<\
Isotherms and isobars were solved using the van der Waals equation of state:\
\>", "DetailNotes",
 CellChangeTimes->{{3.6189412868763404`*^9, 3.618941329194761*^9}},
 CellID->60133961],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"P", " ", "=", 
    RowBox[{
     FractionBox[
      RowBox[{" ", 
       RowBox[{"R", " ", "T"}]}], 
      RowBox[{"V", "-", "b"}]], "-", 
     FractionBox["a", 
      SuperscriptBox["V", "2"]]}]}], TraditionalForm]], "InlineMath"],
 ";"
}], "DetailNotes",
 CellChangeTimes->{{3.6189412868763404`*^9, 3.618941362465664*^9}, {
  3.619470201275498*^9, 3.619470207146143*^9}},
 CellID->64309552],

Cell["rearranged:", "DetailNotes",
 CellChangeTimes->{{3.6189412868763404`*^9, 3.618941377244509*^9}},
 CellID->385298819],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"T", " ", "=", " ", 
    RowBox[{
     FractionBox["1", "R"], " ", 
     RowBox[{"(", 
      RowBox[{"P", "+", 
       FractionBox["a", 
        SuperscriptBox["V", "2"]]}], ")"}], " ", 
     RowBox[{"(", 
      RowBox[{"V", "-", "b"}], ")"}]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6189412868763404`*^9, 3.618941406072158*^9}, 
   3.61947021314244*^9},
 CellID->106567780],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"a", "=", 
    RowBox[{
     FractionBox["27", "64"], " ", 
     FractionBox[
      RowBox[{
       SuperscriptBox["R", "2"], " ", 
       SuperscriptBox[
        SubscriptBox["T", 
         StyleBox["c",
          FontSlant->"Plain"]], "2"]}], 
      SubscriptBox["P", 
       StyleBox["c",
        FontSlant->"Plain"]]]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6189412868763404`*^9, 3.618941417189794*^9}, {
   3.618941454049902*^9, 3.6189414731799965`*^9}, 3.619470214222437*^9, {
   3.619544874021208*^9, 3.619544893729476*^9}},
 CellID->17241382],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"b", " ", "=", " ", 
    FractionBox[
     RowBox[{"R", " ", 
      SubscriptBox["T", 
       StyleBox["c",
        FontSlant->"Plain"]]}], 
     RowBox[{"8", " ", 
      SubscriptBox["P", 
       StyleBox["c",
        FontSlant->"Plain"]]}]]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6189412868763404`*^9, 3.618941417189794*^9}, {
   3.618941454049902*^9, 3.6189414866037645`*^9}, {3.6189415419609303`*^9, 
   3.6189415419649305`*^9}, 3.619470215437434*^9, {3.619544907280245*^9, 
   3.619544913903812*^9}},
 CellID->301082822],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath"],
 " is pressure (MPa), ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]], "InlineMath"],
 " is the ideal gas constant (8.314 ",
 Cell[BoxData[
  FormBox[
   RowBox[{"MPa", " ", 
    RowBox[{
     RowBox[{
      SuperscriptBox["cm", "3"], "/", "mol"}], "/", "K"}]}], 
   TraditionalForm]], "InlineMath"],
 "), ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath"],
 " is temperature (K), ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]], "InlineMath"],
 " is molar volume (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["cm", "3"], "/", "mol"}], TraditionalForm]], "InlineMath"],
 "), ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox["b", TraditionalForm]], "InlineMath"],
 " are van der Waals constants, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["T", 
    StyleBox["c",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " is the critical temperature of water (K), and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["P", 
    StyleBox["c",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " is the critical pressure (MPa)."
}], "DetailNotes",
 CellChangeTimes->{{3.6189412868763404`*^9, 3.618941417189794*^9}, {
  3.618941454049902*^9, 3.6189414866037645`*^9}, {3.6189415419609303`*^9, 
  3.6189417219042225`*^9}, {3.619544918787484*^9, 3.619544924293387*^9}, {
  3.619547791093883*^9, 3.6195478000728083`*^9}, {3.6500500279227247`*^9, 
  3.650050027926725*^9}, {3.650050303767502*^9, 3.6500503355403194`*^9}},
