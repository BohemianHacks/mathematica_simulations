 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{"\[Rho]", " ", "g", " ", "h"}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.593113582552314*^9, 3.593113582553001*^9}, 
   3.6071067979339333`*^9, {3.609785707970625*^9, 3.6097857227341013`*^9}, {
   3.609870920728387*^9, 3.609870929870865*^9}},
 CellID->136840064],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath"],
 " is the hydrostatic gage pressure (kPa), ",
 Cell[BoxData[
  FormBox["\[Rho]", TraditionalForm]], "InlineMath"],
