Cell[CellGroupData[{

Cell["", "DetailsSection"],

Cell["\<\
The heat flux for conductive heat transfer through the skin is equal to the \
convective heat transfer of the moving air:\
\>", "DetailNotes",
 CellChangeTimes->{{3.68452150622486*^9, 3.684521547033166*^9}, {
  3.684682314530661*^9, 3.684682314953862*^9}},
 CellID->13577667],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["q", "cond", "\[DoublePrime]"], "=", 
    RowBox[{
     FractionBox["k", "L"], " ", 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["T", 
        RowBox[{
         StyleBox["s",
          FontSlant->"Plain"], ",", "1"}]], "-", 
       SubscriptBox["T", 
        RowBox[{
         StyleBox["s",
          FontSlant->"Plain"], ",", "2"}]]}], ")"}]}]}], TraditionalForm]], 
  "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.68452150622486*^9, 3.6845215751084304`*^9}},
 CellID->273038575],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["q", "conv", "\[DoublePrime]"], "=", 
    RowBox[{"h", " ", 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["T", 
        RowBox[{
         StyleBox["s",
          FontSlant->"Plain"], ",", "2"}]], "-", 
       SubscriptBox["T", "\[Infinity]"]}], ")"}]}]}], TraditionalForm]], 
  "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.68452150622486*^9, 3.684521599051119*^9}},
 CellID->605750818],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["q", "\[DoublePrime]"], TraditionalForm]], "InlineMath"],
 " is heat flux (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["W",
     FontSlant->"Plain"], "/", 
    SuperscriptBox[
     StyleBox["m",
      FontSlant->"Plain"], "2"]}], TraditionalForm]], "InlineMath"],
 "), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["T", 
    RowBox[{
     StyleBox["s",
      FontSlant->"Plain"], ",", "1"}]], TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["T", 
    RowBox[{
     StyleBox["s",
      FontSlant->"Plain"], ",", "2"}]], TraditionalForm]], "InlineMath"],
 " are the inner and outer surface temperatures (\[Degree]C), ",
 Cell[BoxData[
  FormBox["k", TraditionalForm]], "InlineMath"],
 " is the thermal conductivity of fatty tissue (W/[m K]), ",
 Cell[BoxData[
  FormBox["L", TraditionalForm]], "InlineMath"],
 " is skin thickness (mm), ",
 Cell[BoxData[
  FormBox["h", TraditionalForm]], "InlineMath"],
 " is the convective heat transfer coefficient of air (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["W",
     FontSlant->"Plain"], "/", 
    RowBox[{"[", 
     RowBox[{
      SuperscriptBox[
       StyleBox["m",
        FontSlant->"Plain"], "2"], " ", 
      StyleBox["K",
       FontSlant->"Plain"]}], "]"}]}], TraditionalForm]], "InlineMath"],
 "), and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["T", "\[Infinity]"], TraditionalForm]], "InlineMath"],
 " is air temperature (\[Degree]C)."
}], "DetailNotes",
 CellChangeTimes->{{3.68452150622486*^9, 3.6845216102035394`*^9}, {
  3.684521645070407*^9, 3.684521775481864*^9}, {3.684682336812518*^9, 
  3.6846823683237753`*^9}},
 CellID->610040891],

Cell[TextData[{
 "For a calm day ",
 Cell[BoxData[
  FormBox[
   RowBox[{"h", "=", 
    RowBox[{"25", " ", 
     RowBox[{
      StyleBox["W",
       FontSlant->"Plain"], "/", 
      RowBox[{"[", 
       RowBox[{
        SuperscriptBox[
         StyleBox["m",
          FontSlant->"Plain"], "2"], " ", 
        StyleBox["K",
         FontSlant->"Plain"]}], "]"}]}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ", and for a windy day the convective heat transfer coefficient is a \
function of wind speed:"
}], "DetailNotes",
 CellChangeTimes->{{3.68452150622486*^9, 3.6845216102035394`*^9}, {
  3.684521645070407*^9, 3.684521775481864*^9}, {3.684682336812518*^9, 
  3.6846824884990673`*^9}, {3.6846825725962763`*^9, 3.684682591183914*^9}},
 CellID->237188451],

Cell[TextData[{
 Cell[BoxData[
