Cell[TextData[{
 "Two first-order, liquid-phase reactions ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["A",
     FontSlant->"Plain"], "\[Rule]", 
    RowBox[{
     StyleBox["B",
      FontSlant->"Plain"], "\[Rule]", 
     StyleBox["C",
      FontSlant->"Plain"]}]}], TraditionalForm]], "InlineMath"],
