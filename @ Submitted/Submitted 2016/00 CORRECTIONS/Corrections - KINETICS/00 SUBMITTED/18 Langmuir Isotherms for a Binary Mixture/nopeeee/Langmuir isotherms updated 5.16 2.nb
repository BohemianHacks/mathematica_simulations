 " are:"
}], "DetailNotes",
 CellChangeTimes->{{3.673014169017062*^9, 3.6730142193040185`*^9}},
 CellID->348236420],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Theta]", 
     StyleBox["A",
      FontSlant->"Plain"]], "=", 
    FractionBox[
     RowBox[{
      SubscriptBox["K", 
       StyleBox["A",
        FontSlant->"Plain"]], " ", 
      SubscriptBox["P", 
       StyleBox["A",
        FontSlant->"Plain"]]}], 
     RowBox[{"1", "+", 
      RowBox[{
       SubscriptBox["K", 
        StyleBox["A",
         FontSlant->"Plain"]], " ", 
       SubscriptBox["P", 
        StyleBox["A",
         FontSlant->"Plain"]]}], "+", 
      RowBox[{
       SubscriptBox["K", 
        StyleBox["B",
         FontSlant->"Plain"]], " ", 
       SubscriptBox["P", 
        StyleBox["B",
         FontSlant->"Plain"]]}]}]]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.673014169017062*^9, 3.67301426475636*^9}, {
  3.673014574973109*^9, 3.673014581174729*^9}},
 CellID->275036712],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Theta]", 
     StyleBox["B",
      FontSlant->"Plain"]], "=", 
    FractionBox[
     RowBox[{
      SubscriptBox["K", 
       StyleBox["B",
        FontSlant->"Plain"]], " ", 
      SubscriptBox["P", 
       StyleBox["B",
        FontSlant->"Plain"]]}], 
     RowBox[{"\[Alpha]", " ", 
      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{
         SubscriptBox["K", 
          StyleBox["A",
           FontSlant->"Plain"]], " ", 
         SubscriptBox["P", 
          StyleBox["A",
           FontSlant->"Plain"]]}], " ", "+", 
        RowBox[{
         SubscriptBox["K", 
          StyleBox["B",
           FontSlant->"Plain"]], " ", 
         SubscriptBox["P", 
          StyleBox["B",
           FontSlant->"Plain"]]}]}], ")"}]}]]}], TraditionalForm]], 
  "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.673014169017062*^9, 3.673014293429912*^9}, {
  3.6730145838797817`*^9, 3.673014589499339*^9}},
 CellID->141886429],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["K", "i"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is the adsorption equilibrium constant for component ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "=", 
    RowBox[{"(", 
     RowBox[{
      StyleBox["A",
       FontSlant->"Plain"], ",", 
      StyleBox["B",
       FontSlant->"Plain"]}], ")"}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " (1/bar), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["P", "i"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is the partial pressure (bar), and ",
 Cell[BoxData[
  FormBox["\[Alpha]", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is the saturation coverage of ",
 Cell[BoxData[
  FormBox[
   StyleBox["B",
    FontSlant->"Plain"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " to ",
 Cell[BoxData[
  FormBox[
   StyleBox["A",
    FontSlant->"Plain"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.673014169017062*^9, 3.673014439432248*^9}, {
  3.6730145460351005`*^9, 3.6730145460351005`*^9}, {3.6730145926209035`*^9, 
  3.673014592880904*^9}, {3.67301486522606*^9, 3.67301487307368*^9}},
 CellID->125931885],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["K", "i"], "=", 
    RowBox[{"k", " ", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{
        SubscriptBox["\[Lambda]", "i"], "/", "R"}], "/", "T"}]]}]}], 
   TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.673014169017062*^9, 3.673014439432248*^9}, {
  3.6730145460351005`*^9, 3.6730145634694295`*^9}, {3.6730145963569827`*^9, 
  3.6730146102221937`*^9}},
 CellID->355773725],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["k", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is a pre-exponential factor (1/bar), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Lambda]", "i"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is the heat of adsorption (kJ/mol), ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is the ideal gas constant (kJ/mol/K), and ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is temperature (K)."
}], "DetailNotes",
 CellChangeTimes->{{3.673014169017062*^9, 3.673014439432248*^9}, {
  3.6730145460351005`*^9, 3.6730145634694295`*^9}, {3.6730145963569827`*^9, 
  3.673014684409938*^9}},
 CellID->663424178],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["P", 
     StyleBox["A",
      FontSlant->"Plain"]], "=", 
    FractionBox["P", 
     RowBox[{"1", "+", "r"}]]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.673014169017062*^9, 3.6730144686057277`*^9}},
 CellID->75131326],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["P", 
     StyleBox["B",
      FontSlant->"Plain"]], "=", 
    RowBox[{
     RowBox[{"\[Alpha]", " ", 
      SubscriptBox["P", 
       StyleBox["A",
        FontSlant->"Plain"]]}], "=", 
     RowBox[{"\[Alpha]", " ", 
      FractionBox["P", 
       RowBox[{"1", "+", "\[Alpha]"}]]}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.673014169017062*^9, 3.6730144830299997`*^9}},
 CellID->466013499],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is the total pressure ",
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     SubscriptBox["P", 
      StyleBox["A",
       FontSlant->"Plain"]], "+", 
     SubscriptBox["P", 
      StyleBox["B",
       FontSlant->"Plain"]]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " (bar), and ",
 Cell[BoxData[
  FormBox["r", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is the ration of partial pressures."
}], "DetailNotes",
