  3.667850910084923*^9, 3.6678509100889235`*^9}},
 CellID->344783614],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["C", "1"], "=", 
    RowBox[{"1", "+", 
     RowBox[{
      SubscriptBox["K", 
       StyleBox["A",
        FontSlant->"Plain"]], " ", 
      SubscriptBox["P", 
       StyleBox["A",
        FontSlant->"Plain"]]}], "+", 
     RowBox[{
      SubscriptBox["K", 
       StyleBox["B",
        FontSlant->"Plain"]], " ", 
      SubscriptBox["P", 
       StyleBox["B",
        FontSlant->"Plain"]]}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
