Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["C", "2"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       SubscriptBox["K", 
        StyleBox["A",
         FontSlant->"Plain"]], " ", 
       SubscriptBox["P", 
        StyleBox["A",
         FontSlant->"Plain"]], " ", 
