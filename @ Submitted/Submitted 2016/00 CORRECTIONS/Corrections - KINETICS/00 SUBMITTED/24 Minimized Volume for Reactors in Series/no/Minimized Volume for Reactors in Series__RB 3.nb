    FractionBox[
     RowBox[{"k", " ", 
      SubscriptBox["C", 
       RowBox[{
        StyleBox["A",
         FontSlant->"Plain"], ",", "0"}]], " ", 
      RowBox[{"(", 
       RowBox[{"1", "-", "x"}], ")"}]}], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{
         SubscriptBox["K", 
          StyleBox["A",
