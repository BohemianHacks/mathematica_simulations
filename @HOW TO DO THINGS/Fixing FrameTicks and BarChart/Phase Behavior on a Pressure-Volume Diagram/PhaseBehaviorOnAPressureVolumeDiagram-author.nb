Cell["\<\
At a given pressure and volume, the temperature is calculated from the Peng\
\[Dash]Robinson equation of state:\
\>", "DetailNotes",
 CellChangeTimes->{{3.616248610307424*^9, 3.616248706028899*^9}, 
   3.6195453299453487`*^9, {3.67371414526272*^9, 3.6737141452647204`*^9}, {
   3.6737141838153954`*^9, 3.673714184698903*^9}, {3.673714322882474*^9, 
   3.6737143783557496`*^9}, {3.6737869921697187`*^9, 3.673787000167119*^9}, {
   3.6960125869877043`*^9, 3.696012588086254*^9}},
 CellID->24376353,ExpressionUUID->"adae034b-a007-403b-8298-1ec59e620120"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     FractionBox[
      RowBox[{"R", " ", "T"}], 
      RowBox[{"V", "-", "b"}]], "-", 
     FractionBox["a", 
      RowBox[{
       SuperscriptBox["V", "2"], "+", 
       RowBox[{"2", " ", "V", " ", "b"}], "-", 
       SuperscriptBox["b", "2"]}]]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"3be99127-b84c-4663-9843-504ba4684f2f"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.616248610307424*^9, 3.616248706028899*^9}, 
   3.6195453299453487`*^9, {3.67371414526272*^9, 3.673714177299322*^9}},
 CellID->532484372,ExpressionUUID->"c6411bf5-20ba-49b0-bcc8-80f99ff9ea95"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"a", "=", 
    RowBox[{"0.457", " ", 
     FractionBox[
      RowBox[{
       SuperscriptBox["R", "2"], " ", 
       SubsuperscriptBox["T", 
        StyleBox["c",
         FontSlant->"Plain"], "2"]}], 
      SubscriptBox["P", 
       StyleBox["c",
        FontSlant->"Plain"]]], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{"\[Kappa]", " ", 
         RowBox[{"(", 
          RowBox[{"1", "-", 
           SqrtBox[
            RowBox[{"T", "/", 
             SubscriptBox["T", 
              StyleBox["c",
               FontSlant->"Plain"]]}]]}], ")"}]}]}], ")"}], "2"]}]}], 
   TraditionalForm]], "InlineMath",ExpressionUUID->
  "937ff6fa-c186-47aa-beb8-4a0dd971cc41"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.616248610307424*^9, 3.616248758815918*^9}, 
   3.619545331362939*^9, {3.6745846954004498`*^9, 3.6745847180140977`*^9}, {
   3.679248504326655*^9, 3.6792485225768414`*^9}},
 CellID->664680889,ExpressionUUID->"ab29c698-59d5-4059-8de4-80b3561ccd3a"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"b", "=", 
    RowBox[{"0.078", " ", 
     FractionBox[
      RowBox[{"R", " ", 
       SubscriptBox["T", 
        StyleBox["c",
         FontSlant->"Plain"]]}], 
      SubscriptBox["P", 
       StyleBox["c",
        FontSlant->"Plain"]]]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"a0f08768-cdf2-4773-a152-955f9392ec59"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.616248610307424*^9, 3.616248820758461*^9}, 
   3.619545334974325*^9, {3.6745847225666356`*^9, 3.674584723435154*^9}},
 CellID->26190243,ExpressionUUID->"46ad9c08-6dd6-40c8-9efa-7066ccb67bb4"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Kappa]", "=", 
    RowBox[{"0.375", "+", 
     RowBox[{"1.54", " ", "\[Omega]"}], "-", 
     RowBox[{"0.270", " ", 
      SuperscriptBox["\[Omega]", "2"]}]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"6b0f462c-411d-4173-9397-04d8c6d802f0"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.616248610307424*^9, 3.616248994470397*^9}, {
   3.616249033669639*^9, 3.6162491794079747`*^9}, 3.617711882442876*^9, {
   3.618000391606111*^9, 3.6180004717424*^9}, {3.618000540355756*^9, 
   3.6180005980917673`*^9}, 3.618000687936089*^9, {3.618057646509813*^9, 
   3.618057653382206*^9}, {3.6181549441057434`*^9, 3.618154956695464*^9}, {
   3.6187741220203915`*^9, 3.6187741299028425`*^9}, 3.6195452292981377`*^9, {
   3.6195453471243896`*^9, 3.619545348046767*^9}, {3.619548428143958*^9, 
   3.619548433470006*^9}, {3.6198014085263743`*^9, 3.6198014111823745`*^9}, {
   3.630760238324813*^9, 3.630760239374873*^9}, {3.665336538118905*^9, 
   3.6653365426564307`*^9}},
 CellID->326283920,ExpressionUUID->"86e01990-cc25-4246-8460-57cdf6b7bd7c"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath",ExpressionUUID->
  "5f112a3b-bd3b-4361-8109-7e832d1a3d1e"],
 " is pressure (MPa), ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]], "InlineMath",ExpressionUUID->
  "4382a122-bc73-4f0e-971b-d1674b79519e"],
 " is the ideal gas constant (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"8.314", " ", "[", 
     RowBox[{
      SuperscriptBox["cm", "3"], " ", "MPa"}], "]"}], "/", 
    RowBox[{"[", 
     RowBox[{"mol", " ", "K"}], "]"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"3295f425-49a2-48bd-9dd9-6825ce4cc5e0"],
 "), ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath",ExpressionUUID->
  "95cf96ba-f679-4d4d-a4e3-7763b080a58b"],
 " is temperature (K), ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]], "InlineMath",ExpressionUUID->
  "f878e289-551e-458c-bade-68374a33526b"],
 " is the molar volume (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["cm", "3"], "/", "mol"}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"0dd1badd-ef04-495c-b611-6dac2dfa3edb"],
 "), ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]], "InlineMath",ExpressionUUID->
  "88aaa35f-904f-487b-a775-9174b0a853bb"],
 " and ",
 Cell[BoxData[
  FormBox["b", TraditionalForm]], "InlineMath",ExpressionUUID->
  "b0fbb401-43ec-444c-aa92-209f4ad74380"],
 " are Peng\[Dash]Robinson constants, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["P", 
    StyleBox["c",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",ExpressionUUID->
  "a46e2ae3-4739-452d-877b-76f38ff8c395"],
 " is the critical pressure (MPa), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["T", 
    StyleBox["c",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",ExpressionUUID->
  "c4eaed4c-3c22-43e4-bf14-888dafdd7361"],
 " is the critical temperature (K), and ",
 Cell[BoxData[
  FormBox["\[Omega]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "f847023e-a4b2-4200-b360-dceca929d163"],
 " is the acentric factor. "
}], "DetailNotes",
 CellChangeTimes->{{3.6738010377016993`*^9, 3.6738010377016993`*^9}, 
   3.6775903970201635`*^9, {3.677768724001688*^9, 3.6777687380045867`*^9}},
 CellID->84767626,ExpressionUUID->"76327766-4cb7-488e-9391-288abc412d27"],

