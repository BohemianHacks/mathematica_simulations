   3.682702980646694*^9, 3.682703025104867*^9}, {3.6827030591399927`*^9, 
   3.6827030591400347`*^9}, 3.6827040130658207`*^9, {3.682711122025771*^9, 
   3.682711136936945*^9}},
 CellID->720826832,ExpressionUUID->"49a22957-865e-4e05-9e74-a599ce7af789"]
