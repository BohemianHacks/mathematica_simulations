      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{"\[Tau]", " ", 
         SubscriptBox["k", 
          StyleBox["f",
           FontSlant->"Plain"]], " ", 
         SuperscriptBox["e", 
          RowBox[{"-", 
           FractionBox[
            SubscriptBox["E", 
             StyleBox["f",
              FontSlant->"Plain"]], 
            RowBox[{"R", " ", "T"}]]}]]}], "+", 
        RowBox[{"\[Tau]", " ", 
