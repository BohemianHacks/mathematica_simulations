Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["N", 
    StyleBox["A",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " are moles of ",
 Cell[BoxData[
  FormBox[
   StyleBox["A",
    FontSlant->"Plain"], TraditionalForm]], "InlineMath"],
 " (mol), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["C", 
    StyleBox["A",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " is the concentration of ",
 Cell[BoxData[
  FormBox[
   StyleBox["A",
    FontSlant->"Plain"], TraditionalForm]], "InlineMath"],
 " (",
