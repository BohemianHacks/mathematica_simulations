 CellID->9756732,ExpressionUUID->"a0fa4b1c-a362-4de6-ad5d-c1e7a66df2c1"],

Cell[TextData[{
 "The extent of reaction is found by setting the equilibrium constant ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["K", "eq"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "eb622b2d-ecef-4d47-8e20-0813cf56be24"],
 " equal to the equilibrium rate constant ",
 Cell[BoxData[
  FormBox["K", TraditionalForm]], "InlineMath",ExpressionUUID->
  "47fe77fa-5654-42fe-a941-0db04f447dfa"],
 " and solving for ",
 Cell[BoxData[
  FormBox["\[Zeta]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "7bff79db-1e81-46e8-9edf-dd0dd4ee72c6"],
 ":"
}], "DetailNotes",
 CellChangeTimes->{{3.6751880296622343`*^9, 3.675188064126915*^9}, {
  3.675188132615466*^9, 3.6751882627485633`*^9}, {3.675188308580863*^9, 
  3.6751883531823626`*^9}, {3.6751884079154797`*^9, 3.675188519277799*^9}, {
  3.6751885927842364`*^9, 3.67518861350309*^9}, {3.6751886596086073`*^9, 
  3.6751887261327667`*^9}, {3.6780377352494907`*^9, 3.678037740508792*^9}},
 CellID->20687285,ExpressionUUID->"a93198f1-438a-43fb-82be-520611159bb5"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["K", "eq"], "=", 
    SuperscriptBox["e", 
     RowBox[{"-", 
      FractionBox[
       RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", "G"}], 
       RowBox[{"R", " ", "T"}]]}]]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"68af1e72-7244-4d10-afa9-3b76ffc3cfc7"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6751880296622343`*^9, 3.675188064126915*^9}, {
  3.675188132615466*^9, 3.6751882627485633`*^9}, {3.675188308580863*^9, 
  3.6751883531823626`*^9}, {3.6751884079154797`*^9, 3.675188519277799*^9}, {
  3.6751885927842364`*^9, 3.67518861350309*^9}, {3.6751886596086073`*^9, 
  3.675188756677439*^9}, {3.6780377440379934`*^9, 3.6780377444560175`*^9}, {
  3.679408352295026*^9, 3.679408353121827*^9}, {3.693680858614808*^9, 
  3.693680858614808*^9}},
 CellID->261178778,ExpressionUUID->"e9e566c1-5691-4aa5-9bc0-8208e24be9da"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"K", "=", 
    RowBox[{
     RowBox[{"\[Product]", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["y", "i"], " ", "P"}], ")"}], 
       RowBox[{"-", 
        SubscriptBox["v", "i"]}]]}], "=", 
     RowBox[{
      FractionBox[
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["y", 
           SubscriptBox["NH", "3"]], " ", "P"}], ")"}], "2"], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["y", 
           SubscriptBox["N", "2"]], " ", "P"}], ")"}], " ", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           SubscriptBox["y", 
            SubscriptBox["H", "2"]], " ", "P"}], ")"}], "3"]}]], "=", 
      RowBox[{
       FractionBox[
        SuperscriptBox[
         RowBox[{"(", 
          SubscriptBox["y", 
           SubscriptBox["NH", "3"]], ")"}], "2"], 
        RowBox[{
         SubscriptBox["y", 
          SubscriptBox[
           StyleBox["N",
            FontSlant->"Plain"], "2"]], " ", 
         SuperscriptBox[
          RowBox[{"(", 
           SubscriptBox["y", 
            SubscriptBox[
             StyleBox["H",
              FontSlant->"Plain"], "2"]], ")"}], "3"]}]], " ", 
       FractionBox["1", 
        SuperscriptBox["P", "2"]]}]}]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"acae5190-33d3-4653-8632-370d41322e41"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6751880296622343`*^9, 3.675188064126915*^9}, {
  3.675188132615466*^9, 3.6751882627485633`*^9}, {3.675188308580863*^9, 
  3.6751883531823626`*^9}, {3.6751884079154797`*^9, 3.675188519277799*^9}, {
  3.6751885927842364`*^9, 3.67518861350309*^9}, {3.6751886596086073`*^9, 
  3.6751888496760178`*^9}, {3.6780377473431826`*^9, 3.678037820323357*^9}, {
  3.679408444839507*^9, 3.6794084448707075`*^9}},
 CellID->141294820,ExpressionUUID->"1afea4f9-bf71-45ba-b3f6-5d66cb7933bc"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", "G"}], "=", 
    RowBox[{
     RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", 
      SubscriptBox["H", 
       StyleBox["r",
        FontSlant->"Plain"]]}], "-", 
     RowBox[{"T", " ", "\[CapitalDelta]", "\[InvisibleSpace]", 
      SubscriptBox["S", 
       StyleBox["r",
        FontSlant->"Plain"]]}]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"114d413a-d522-4c57-a17e-4217f7576bd7"],
 " is Gibbs free energy (J/mol), ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", 
    SubscriptBox["H", 
     StyleBox["r",
      FontSlant->"Plain"]]}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "57517263-5ff2-4ab2-8e39-c933d597433f"],
 " is the heat of reaction (J/mol), ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", 
    SubscriptBox["S", 
     StyleBox["r",
      FontSlant->"Plain"]]}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "fc233c27-5116-40a8-b776-08e603274160"],
 " is the entropy change of reaction (J/(mol K)), ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath",ExpressionUUID->
  "6b8769ee-6e04-4878-8674-911c38529233"],
 " is temperature (K), ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]], "InlineMath",ExpressionUUID->
  "b7597360-9f13-45bf-aa72-5812f3089769"],
 " is the ideal gas constant (J/(mol K)) and ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath",ExpressionUUID->
  "f18a364a-dbcf-471a-bc9e-852ade3592de"],
 " is pressure (bar)."
}], "DetailNotes",
 CellChangeTimes->{{3.6751891651641335`*^9, 3.6751891651641335`*^9}, 
   3.6780378548913345`*^9, {3.679408524360426*^9, 3.6794085246724267`*^9}, {
   3.693933467515719*^9, 3.693933481599729*^9}},
 CellID->312480064,ExpressionUUID->"ce738d0f-f0ba-49a7-8670-b5d0a2912542"],

