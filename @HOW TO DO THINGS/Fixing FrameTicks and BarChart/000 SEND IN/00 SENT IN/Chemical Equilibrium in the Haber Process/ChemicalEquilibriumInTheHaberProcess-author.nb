Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", "G"}], "=", 
    RowBox[{
     RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", 
      SubscriptBox["H", 
       StyleBox["r",
