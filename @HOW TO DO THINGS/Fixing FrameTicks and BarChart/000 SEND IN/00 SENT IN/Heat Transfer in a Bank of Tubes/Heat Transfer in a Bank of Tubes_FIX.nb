Cell[TextData[{
 "The heat transfer rate per unit length of tube is calculated from the ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["h", "_"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "f7f306e5-8795-488f-ad50-1762d9cbc168"],
 " found from the Nusselt correlation:"
}], "DetailNotes",
 CellChangeTimes->{{3.6818304090161247`*^9, 3.6818304328327847`*^9}, {
  3.6818307184464417`*^9, 3.6818307565561447`*^9}, {3.6823458361289883`*^9, 
  3.6823458461604424`*^9}},
 CellID->91857037,ExpressionUUID->"bbb6c254-e49d-4f63-b465-6c7b097611d1"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"q", "=", 
    RowBox[{"N", " ", 
     OverscriptBox["h", "_"], " ", "\[Pi]", " ", "d", " ", "\[CapitalDelta]", 
     "\[InvisibleSpace]", 
     SubscriptBox["T", "lm"]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"dfdc1e4d-e2fe-4631-8f5e-e295a00c6c77"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6818304090161247`*^9, 3.6818304328327847`*^9}, {
  3.6818307184464417`*^9, 3.6818307802566037`*^9}},
 CellID->785174280,ExpressionUUID->"c024883b-f944-48ca-b2e2-5f72988e969d"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", 
     SubscriptBox["T", "lm"]}], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["T", 
          StyleBox["s",
           FontSlant->"Plain"]], "-", 
         SubscriptBox["T", "in"]}], ")"}], "-", 
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["T", 
          StyleBox["s",
           FontSlant->"Plain"]], "-", 
         SubscriptBox["T", "out"]}], ")"}]}], 
      RowBox[{"ln", " ", 
       RowBox[{"(", 
        FractionBox[
         RowBox[{
          SubscriptBox["T", 
           StyleBox["s",
            FontSlant->"Plain"]], "-", 
          SubscriptBox["T", "in"]}], 
         RowBox[{
          SubscriptBox["T", 
           StyleBox["s",
            FontSlant->"Plain"]], "-", 
          SubscriptBox["T", "out"]}]], ")"}]}]], "=", 
     FractionBox[
      RowBox[{
       SubscriptBox["T", "out"], "-", 
       SubscriptBox["T", "in"]}], 
      RowBox[{"ln", " ", 
       RowBox[{"(", 
        FractionBox[
         RowBox[{
          SubscriptBox["T", 
           StyleBox["s",
            FontSlant->"Plain"]], "-", 
          SubscriptBox["T", "in"]}], 
         RowBox[{
          SubscriptBox["T", 
           StyleBox["s",
            FontSlant->"Plain"]], "-", 
          SubscriptBox["T", "out"]}]], ")"}]}]]}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"1ddc8c32-ae79-43cd-a828-072e68ce9e86"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6818304090161247`*^9, 3.6818304328327847`*^9}, {
  3.6818307184464417`*^9, 3.681830878592422*^9}},
 CellID->1578442,ExpressionUUID->"eb673397-5557-4d9f-963e-f864a06e1d06"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["T", "out"], "=", 
    RowBox[{
     SubscriptBox["T", 
      StyleBox["s",
       FontSlant->"Plain"]], "-", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["T", 
         StyleBox["s",
          FontSlant->"Plain"]], "-", 
        SubscriptBox["T", "in"]}], ")"}], " ", 
      SuperscriptBox["e", 
       RowBox[{"-", 
        FractionBox[
         RowBox[{"\[Pi]", " ", "d", " ", "N", " ", 
          OverscriptBox["h", "_"]}], 
         RowBox[{"\[Rho]", " ", "u", " ", 
          SubscriptBox["N", 
           StyleBox["T",
            FontSlant->"Plain"]], " ", 
          SubscriptBox["S", 
           StyleBox["T",
            FontSlant->"Plain"]], " ", "Cp"}]]}]]}]}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"5b84c34d-dc84-4093-9116-2aa5fff92fdd"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6818304090161247`*^9, 3.6818304328327847`*^9}, {
  3.6818307184464417`*^9, 3.681830934393759*^9}},
 CellID->26478917,ExpressionUUID->"bf848f6b-eb2f-4479-bd8d-40dc19ca5425"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["q", TraditionalForm]], "InlineMath",ExpressionUUID->
  "5672b511-d0c4-4ec3-802c-0c80d2d2d5b1"],
 " is in W/m, ",
 Cell[BoxData[
  FormBox["N", TraditionalForm]], "InlineMath",ExpressionUUID->
  "6ab3b614-b0a9-4c78-9f45-d33351836045"],
 " is the total number of tubes in the bank, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", 
    SubscriptBox["T", "lm"]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"c4f0c743-020a-429a-99b6-1450effcd408"],
 " is the log-mean temperature difference (\[Degree]C), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["T", 
    StyleBox["s",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",ExpressionUUID->
  "92aa177f-85ee-45c1-a584-9baf2670ac3e"],
 " is the tube surface temperature (\[Degree]C), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["T", "in"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "8572146b-df77-4b8c-a759-4d7cb52858d2"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["T", "out"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "e8bda149-e248-4e09-8cd3-31c673056306"],
 " are the temperatures at the inlet and outlet (\[Degree]C), ",
 Cell[BoxData[
  FormBox["\[Rho]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "9acb72f7-b057-4bdb-b986-686a80a5c69c"],
 " is air density (",
 Cell[BoxData[
  FormBox[
   RowBox[{"kg", "/", 
    SuperscriptBox[
     StyleBox["m",
      FontSlant->"Plain"], "3"]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"46ab107f-b4a8-4e0c-a417-69b407fd7ea4"],
 "), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["N", 
    StyleBox["T",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",ExpressionUUID->
  "292a4cdd-2c08-4d83-9db7-cfdb5d4a1819"],
 " is the number of tubes in each row and ",
 Cell[BoxData[
  FormBox["Cp", TraditionalForm]], "InlineMath",ExpressionUUID->
  "367dd210-fa8b-4003-86c4-665d21e66694"],
 " is heat capacity (J/[kg K])."
}], "DetailNotes",
 CellChangeTimes->{{3.6818304090161247`*^9, 3.6818304328327847`*^9}, {
   3.6818307184464417`*^9, 3.6818310821036987`*^9}, {3.6818311230314765`*^9, 
   3.681831140319126*^9}, {3.6818312324993343`*^9, 3.6818312420817556`*^9}, 
