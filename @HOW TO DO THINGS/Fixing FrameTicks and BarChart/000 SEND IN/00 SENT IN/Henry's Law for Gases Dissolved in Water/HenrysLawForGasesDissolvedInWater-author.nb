Cell["", "DetailsSection",ExpressionUUID->"427e6b57-9c02-41a6-b8e9-a3cefa9b665d"],

Cell[TextData[{
 "Henry's constant ",
 Cell[BoxData[
  FormBox["H", TraditionalForm]], "InlineMath",ExpressionUUID->
  "26eeae7e-d4df-4074-9370-c2c915ded5eb"],
 " varies with temperature, as given by:"
}], "DetailNotes",
 CellChangeTimes->{{3.6562514794420843`*^9, 3.6562514985301757`*^9}, 
   3.6562525954489155`*^9, {3.6570411748452797`*^9, 3.657041227152233*^9}},
 CellID->66032182,ExpressionUUID->"05b96ebf-83b3-4dfd-84df-35bbe3a1e54a"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"H", "=", 
    RowBox[{"1", "/", 
     RowBox[{"exp", "(", 
      RowBox[{"A", "+", 
       FractionBox["B", "T"], "+", 
       RowBox[{"C", " ", "ln", " ", "T"}], "+", 
       RowBox[{"D", " ", "T"}]}], ")"}]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"cc80d724-e561-4367-a769-0ef03ec575b3"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6562514794420843`*^9, 3.656251571649358*^9}, {
  3.657036781095166*^9, 3.6570367817032757`*^9}, {3.6800355932393937`*^9, 
  3.6800355981710024`*^9}, {3.680045908757216*^9, 3.6800459112139673`*^9}},
 CellID->2878623,ExpressionUUID->"c03c09f2-c74e-4367-9f22-36cd3a1d87d6"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["H", TraditionalForm]], "InlineMath",ExpressionUUID->
  "1379db1a-e41f-4c1a-8ef9-d71c0f727a2b"],
 " is in bar; ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]], "InlineMath",ExpressionUUID->
  "ac229e56-259c-41f5-9549-05c37521266d"],
 ", ",
 Cell[BoxData[
  FormBox["B", TraditionalForm]], "InlineMath",ExpressionUUID->
  "74e12756-f111-4b96-84e1-b7c08f5a3a4d"],
 ", ",
 Cell[BoxData[
  FormBox["C", TraditionalForm]], "InlineMath",ExpressionUUID->
  "8e328667-ecae-4792-b3ac-378d4654d7f1"],
 " and ",
 Cell[BoxData[
  FormBox["D", TraditionalForm]], "InlineMath",ExpressionUUID->
  "5487f3a6-49a7-4bca-8e40-bc18bbd7df71"],
 " are constants specific to each species; and ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath",ExpressionUUID->
  "c0f7c8e5-1671-4b21-bf8b-1c6e767396ec"],
 " is temperature (K)."
}], "DetailNotes",
 CellChangeTimes->{{3.6562514794420843`*^9, 3.656251615988894*^9}, 
   3.657041243312027*^9, {3.657285910693221*^9, 3.657285917835351*^9}, 
   3.680035603293027*^9, 3.6816639292156973`*^9},
 CellID->160356554,ExpressionUUID->"82bf4d38-a081-441b-88d5-63b7edc33a6b"],

Cell[TextData[{
 "Henry's law is used to calculate the mole fraction ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]], "InlineMath",ExpressionUUID->
  "47907e5e-1120-40bc-9898-2797500506db"],
 " of a species dissolved in water:"
}], "DetailNotes",
 CellChangeTimes->{{3.6562514794420843`*^9, 3.656251615984894*^9}, {
   3.6562516637146235`*^9, 3.656251705420009*^9}, 3.6562525982200747`*^9, {
   3.6570368195722847`*^9, 3.657036822861354*^9}},
 CellID->99917254,ExpressionUUID->"4a565b73-e325-4ee2-a52f-ade77c55445d"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", " ", "H"}], "=", "P"}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"e7ccff33-5b11-4feb-8219-00de5a23426f"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6562514794420843`*^9, 3.656251615984894*^9}, {
  3.6562516637146235`*^9, 3.656251697597562*^9}},
 CellID->37884586,ExpressionUUID->"2beb1c0b-62a6-4707-8f17-fdea41078ebb"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath",ExpressionUUID->
  "e4bb4a96-2e9b-4f29-97ce-96e30ec9f619"],
 " is the partial pressure (bar)."
}], "DetailNotes",
 CellChangeTimes->{{3.6562514794420843`*^9, 3.656251615984894*^9}, {
