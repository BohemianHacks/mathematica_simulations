Cell["", "DetailsSection",ExpressionUUID->"f4a75887-84a8-4f87-9ba5-e2335f48c812"],

Cell["\<\
The volume and density are calculated from the Peng-Robinson equation of \
state:\
\>", "DetailNotes",
 CellChangeTimes->{{3.676150902487013*^9, 3.6761509249721804`*^9}, 
   3.6761512252907863`*^9},
 CellID->113927855,ExpressionUUID->"3191178c-c8b1-4724-b463-15d817fdf50d"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     FractionBox[
      RowBox[{"R", " ", "T"}], 
      RowBox[{"V", "-", "b"}]], "-", 
     FractionBox["a", 
      RowBox[{
       SuperscriptBox["V", "2"], "+", 
       RowBox[{"2", " ", "V", " ", "b"}], "-", 
       SuperscriptBox["b", "2"]}]]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"46ad6753-3539-46b2-9549-08a6e4992b74"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.676150902487013*^9, 3.676150953615452*^9}},
 CellID->5796550,ExpressionUUID->"35a880c1-8953-4745-90a9-08a52dc2d9bf"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"a", "=", 
    RowBox[{"0.457", " ", 
     FractionBox[
      RowBox[{
       SuperscriptBox["R", "2"], " ", 
       SubsuperscriptBox["T", "c", "2"]}], 
      SubscriptBox["P", "c"]], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{"\[Kappa]", " ", 
         RowBox[{"(", 
          RowBox[{"1", "-", 
           SqrtBox[
            RowBox[{"T", "/", 
             SubscriptBox["T", "c"]}]]}], ")"}]}]}], ")"}], "2"]}]}], 
   TraditionalForm]], "InlineMath",ExpressionUUID->
  "d561920c-7602-4534-853d-c4921a395d4e"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.676150902487013*^9, 3.6761509891302276`*^9}, {
  3.676151040877079*^9, 3.6761510555916805`*^9}},
 CellID->548693058,ExpressionUUID->"761882e0-859f-430c-a6df-4b1b67b6e6a4"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"b", "=", 
    RowBox[{"0.0778", " ", 
     FractionBox[
      RowBox[{"R", " ", 
       SubscriptBox["T", "c"]}], 
      SubscriptBox["P", "c"]]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"146c2b9d-f1a1-481e-a2fd-a3f8530d4874"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.676150902487013*^9, 3.676151001022301*^9}, {
  3.67615105254916*^9, 3.6761510583336926`*^9}},
 CellID->1248694,ExpressionUUID->"9f8236a2-0fc1-4efb-be5f-844585689d28"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Kappa]", "=", 
    RowBox[{"0.375", "+", 
     RowBox[{"1.54", " ", "\[Omega]"}], "-", 
     RowBox[{"0.27", " ", 
      SuperscriptBox["\[Omega]", "2"]}]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"2b9bfa76-887c-4427-8c4c-5e48ba4482d2"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.676150902487013*^9, 3.6761510201744304`*^9}},
 CellID->147558564,ExpressionUUID->"284ce559-8728-4147-a086-8a813178ffc0"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath",ExpressionUUID->
  "db69f538-c191-4392-a281-7f8143881414"],
 " is pressure (MPa), ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath",ExpressionUUID->
  "60312e56-b42a-4dd1-a3a3-cea82fb3e5a2"],
 " is temperature (K), the subscript ",
 Cell[BoxData[
  FormBox["c", TraditionalForm]], "InlineMath",ExpressionUUID->
  "e9e13dc1-9de9-467a-afb3-7e922cc5d115"],
 " represents the critical property, ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]], "InlineMath",ExpressionUUID->
  "6ae53129-1163-4c3f-824b-a0557c3c72f0"],
 " is the molar volume (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["cm", "3"], "/", "mol"}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"89161d0b-5530-4b6f-b5a7-4dd209361131"],
 "), ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]], "InlineMath",ExpressionUUID->
  "0e7e7bee-a845-498b-8806-d984a98bcb5e"],
 " is the ideal gas constant, ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]], "InlineMath",ExpressionUUID->
  "2dca20ef-3ea6-48ba-aa12-3213870f4b7a"],
 ", ",
 Cell[BoxData[
  FormBox["b", TraditionalForm]], "InlineMath",ExpressionUUID->
  "d9ecfb99-074d-40c1-ae7e-ff90a92d61a2"],
 ", and ",
 Cell[BoxData[
  FormBox["\[Kappa]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "8d4f912b-95b3-45d8-8b81-faa454ac069a"],
 " are Peng-Robinson constants, and ",
 Cell[BoxData[
  FormBox["\[Omega]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "e7b2e334-be32-4290-b03e-4f817d303195"],
 " is the acentric factor."
}], "DetailNotes",
 CellChangeTimes->{{3.676150902487013*^9, 3.676151192349085*^9}},
 CellID->818126333,ExpressionUUID->"e5bf5809-7d70-4b5c-a206-34552dee1d12"],

Cell[TextData[{
 "The molar density is ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Rho]", "=", 
    RowBox[{"1", "/", "V"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"4de07725-efed-4373-9187-e4b1e046a0c1"],
 " (",
 Cell[BoxData[
  FormBox[
   RowBox[{"mol", "/", 
    SuperscriptBox["cm", "3"]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"ce73c955-8305-4724-9687-d9e621b28b5f"],
 ")."
}], "DetailNotes",
 CellChangeTimes->{{3.6763105575810575`*^9, 3.6763105575966578`*^9}, 
   3.6775904432453933`*^9},
 CellID->534252177,ExpressionUUID->"7b87f29d-cdcf-4c90-b18d-849e305ce8d4"],

