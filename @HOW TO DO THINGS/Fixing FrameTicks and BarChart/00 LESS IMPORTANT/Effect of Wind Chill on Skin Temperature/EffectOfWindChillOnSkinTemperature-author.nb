Cell["\<\
The heat flux for conductive heat transfer through the skin is equal to the \
convective heat transfer of the moving air:\
\>", "DetailNotes",
 CellChangeTimes->{{3.68452150622486*^9, 3.684521547033166*^9}, {
  3.684682314530661*^9, 3.684682314953862*^9}},
 CellID->13577667],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["q", "cond"], "=", 
    RowBox[{
     FractionBox["k", "L"], " ", 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["T", "1"], "-", 
       SubscriptBox["T", "2"]}], ")"}]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.68452150622486*^9, 3.6845215751084304`*^9}, {
  3.685813097609096*^9, 3.685813111209239*^9}, {3.685891682963007*^9, 
  3.6858917135833387`*^9}, {3.6859000577842073`*^9, 3.685900113054315*^9}},
 CellID->273038575],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["q", "conv"], "=", 
    RowBox[{"h", " ", 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["T", "2"], "-", 
       SubscriptBox["T", "\[Infinity]"]}], ")"}]}]}], TraditionalForm]], 
  "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.68452150622486*^9, 3.684521599051119*^9}, {
  3.685813117014516*^9, 3.6858131172280073`*^9}, {3.685891720743092*^9, 
  3.685891725734901*^9}, {3.6859000824875402`*^9, 3.68590008305539*^9}, {
  3.685900117062231*^9, 3.685900119366136*^9}},
 CellID->605750818],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["q", TraditionalForm]], "InlineMath"],
 " is heat flux (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["W",
     FontSlant->"Plain"], "/", 
    SuperscriptBox[
     StyleBox["m",
      FontSlant->"Plain"], "2"]}], TraditionalForm]], "InlineMath"],
 "), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["T", "1"], TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["T", "2"], TraditionalForm]], "InlineMath"],
 " are the inner and outer surface temperatures (\[Degree]C), ",
 Cell[BoxData[
  FormBox["k", TraditionalForm]], "InlineMath"],
 " is the thermal conductivity of fatty tissue (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     StyleBox["W",
      FontSlant->"Plain"], "/", 
     StyleBox["m",
      FontSlant->"Plain"]}], " ", 
    StyleBox["K",
     FontSlant->"Plain"]}], TraditionalForm]], "InlineMath"],
 "), ",
 Cell[BoxData[
  FormBox["L", TraditionalForm]], "InlineMath"],
 " is skin thickness (mm), ",
 Cell[BoxData[
  FormBox["h", TraditionalForm]], "InlineMath"],
 " is the convective heat transfer coefficient of air (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     StyleBox["W",
      FontSlant->"Plain"], "/", 
     SuperscriptBox[
      StyleBox["m",
       FontSlant->"Plain"], "2"]}], " ", 
    StyleBox["K",
     FontSlant->"Plain"]}], TraditionalForm]], "InlineMath"],
 ") and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["T", "\[Infinity]"], TraditionalForm]], "InlineMath"],
 " is air temperature (\[Degree]C)."
}], "DetailNotes",
 CellChangeTimes->{{3.68452150622486*^9, 3.6845216102035394`*^9}, {
  3.684521645070407*^9, 3.684521775481864*^9}, {3.684682336812518*^9, 
  3.6846823683237753`*^9}, {3.685813128097069*^9, 3.685813146184041*^9}, {
  3.685891737504795*^9, 3.685891754415709*^9}, {3.6858990092151012`*^9, 
  3.685899022355846*^9}, {3.685900087711596*^9, 3.685900088809679*^9}, {
  3.685900123366053*^9, 3.685900131110117*^9}, {3.6859677645813227`*^9, 
  3.685967798771493*^9}},
 CellID->610040891],

