Cell["\<\
Isotherms and isobars are solved using the Van der Waals equation of state:\
\>", "DetailNotes",
 CellChangeTimes->{{3.6189412868763404`*^9, 3.618941329194761*^9}, 
   3.6781262861990633`*^9, {3.6785549741972027`*^9, 3.6785549780198565`*^9}},
 CellID->60133961,ExpressionUUID->"0d04b09c-5e14-4e03-aec9-fd656b107d55"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"P", " ", "=", 
    RowBox[{
     FractionBox[
      RowBox[{" ", 
       RowBox[{"R", " ", "T"}]}], 
      RowBox[{"V", "-", "b"}]], "-", 
     FractionBox["a", 
      SuperscriptBox["V", "2"]]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"3aa6b06a-0078-4f5c-b2a8-027b3912da95"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6189412868763404`*^9, 3.618941362465664*^9}, {
  3.619470201275498*^9, 3.619470207146143*^9}, {3.6781262933816433`*^9, 
  3.6781262934816437`*^9}},
 CellID->64309552,ExpressionUUID->"7911641c-d89a-4ded-9225-340dda026fd8"],

Cell["rearranged:", "DetailNotes",
 CellChangeTimes->{{3.6189412868763404`*^9, 3.618941377244509*^9}},
 CellID->385298819,ExpressionUUID->"95ee7f47-ee3d-4767-af2f-4b2f5e5eb984"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"T", " ", "=", " ", 
    RowBox[{
     FractionBox["1", "R"], " ", 
     RowBox[{"(", 
      RowBox[{"P", "+", 
       FractionBox["a", 
        SuperscriptBox["V", "2"]]}], ")"}], " ", 
     RowBox[{"(", 
      RowBox[{"V", "-", "b"}], ")"}]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"597d9cb3-0a7b-4026-a26c-7806863146ab"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6189412868763404`*^9, 3.618941406072158*^9}, 
   3.61947021314244*^9},
 CellID->106567780,ExpressionUUID->"afcad344-6caa-43b1-b414-6f670a780999"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"a", "=", 
    RowBox[{
     FractionBox["27", "64"], " ", 
     FractionBox[
      RowBox[{
       SuperscriptBox["R", "2"], " ", 
       SuperscriptBox[
        SubscriptBox["T", 
         StyleBox["c",
          FontSlant->"Plain"]], "2"]}], 
      SubscriptBox["P", 
       StyleBox["c",
        FontSlant->"Plain"]]]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"9b2dd935-9b4a-4864-9daa-e5d7fb68752f"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6189412868763404`*^9, 3.618941417189794*^9}, {
   3.618941454049902*^9, 3.6189414731799965`*^9}, 3.619470214222437*^9, {
   3.619544874021208*^9, 3.619544893729476*^9}},
 CellID->17241382,ExpressionUUID->"e92f91df-161e-4aff-80ba-3eda07895647"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"b", " ", "=", " ", 
    FractionBox[
     RowBox[{"R", " ", 
      SubscriptBox["T", 
       StyleBox["c",
        FontSlant->"Plain"]]}], 
     RowBox[{"8", " ", 
      SubscriptBox["P", 
       StyleBox["c",
        FontSlant->"Plain"]]}]]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"ac420ad0-7b21-416c-9010-b59f5c193ddd"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6189412868763404`*^9, 3.618941417189794*^9}, {
   3.618941454049902*^9, 3.6189414866037645`*^9}, {3.6189415419609303`*^9, 
   3.6189415419649305`*^9}, 3.619470215437434*^9, {3.619544907280245*^9, 
   3.619544913903812*^9}},
 CellID->301082822,ExpressionUUID->"beaa85bb-abba-44f1-98f1-0493def6fb76"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath",ExpressionUUID->
  "c2aad2ec-2952-451c-acf6-b6981de4dc56"],
 " is pressure (MPa), ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]], "InlineMath",ExpressionUUID->
  "da796266-8661-44e7-9192-1e201be3febc"],
 " is the ideal gas constant (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"[", 
     RowBox[{"MPa", " ", 
      SuperscriptBox["cm", "3"]}], "]"}], "/", 
    RowBox[{"[", 
     RowBox[{"mol", " ", "K"}], "]"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"4b12493e-bb0c-48af-b3a8-2566c89bca30"],
 "), ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath",ExpressionUUID->
  "46ab5b2d-61f7-42dc-b500-289048ff463f"],
 " is temperature (K), ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]], "InlineMath",ExpressionUUID->
  "eb93ba57-ed73-4e91-aab6-e8c955bc8b82"],
 " is molar volume (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["cm", "3"], "/", "mol"}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"e199d839-8b5a-4360-b402-5855e6fff966"],
 "), ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]], "InlineMath",ExpressionUUID->
  "1f7349ab-3266-478f-8284-746eb78c4e75"],
 " and ",
 Cell[BoxData[
  FormBox["b", TraditionalForm]], "InlineMath",ExpressionUUID->
  "a9249b32-e74f-4e56-9db5-aa04f5e013f0"],
 " are van der Waals constants, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["T", 
    StyleBox["c",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",ExpressionUUID->
  "97487a1d-6200-48b7-a8c7-20711fb552ef"],
 " is the critical temperature of water (K), and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["P", 
    StyleBox["c",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",ExpressionUUID->
  "0aacc1e0-3cc9-4194-89bd-97d6b9e76d38"],
 " is the critical pressure (MPa)."
}], "DetailNotes",
