  FormBox[
   SubscriptBox["y", 
    StyleBox["A",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",ExpressionUUID->
  "bc33ede5-b979-4e91-995f-56f87c60c5ca"],
