Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["F", "i"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "d8743ca7-61ae-45e2-ba85-dd071023d23d"],
 " is the molar flow rate of component ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", "=", 
     StyleBox["A",
      FontSlant->"Plain"]}], ",", 
    StyleBox["B",
     FontSlant->"Plain"], ",", 
    StyleBox["C",
     FontSlant->"Plain"], ",", 
    StyleBox["D",
     FontSlant->"Plain"]}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "7d53f9bb-ca32-4e75-a9f5-6254dc2d4492"],
 " (mol/min), ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", 
     StyleBox["T",
      FontSlant->"Plain"]], "=", 
    RowBox[{"\[Sum]", 
     SubscriptBox["F", "i"]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"57471219-b8c6-4d00-8468-651ad30495c8"],
 " is the total molar flow rate (mol/min), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["F", 
    RowBox[{
     StyleBox["T",
      FontSlant->"Plain"], ",", "0"}]], TraditionalForm]], "InlineMath",
  ExpressionUUID->"5a7a5e7d-104a-4189-b6ed-7b7d40e0e959"],
 " is the initial total molar flow rate (mol/min), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["F", 
    RowBox[{
     StyleBox["A",
      FontSlant->"Plain"], ",", "0"}]], TraditionalForm]], "InlineMath",
  ExpressionUUID->"f4bf4ad3-0933-4a5c-bd71-9b34c1278f06"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["F", 
    RowBox[{
     StyleBox["B",
      FontSlant->"Plain"], ",", "0"}]], TraditionalForm]], "InlineMath",
  ExpressionUUID->"9e6eab37-5eaf-42fc-8d56-84d9f2b80dea"],
 " are the initial molar flow rates of reactants ",
 Cell[BoxData[
  FormBox[
   StyleBox["A",
    FontSlant->"Plain"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "b2aafe86-9a6b-40c0-83b6-7ec87cd7360d"],
 " and ",
 Cell[BoxData[
  FormBox[
   StyleBox["B",
    FontSlant->"Plain"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "5e7ec429-710e-4da3-8d42-8d4489e0fc7e"],
 " (mol/min), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", 
    StyleBox["x",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",ExpressionUUID->
  "3ea9c9b2-86e4-4447-b91f-206309292bae"],
 " is the reactor cross-sectional area (",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    StyleBox["m",
     FontSlant->"Plain"], "2"], TraditionalForm]], "InlineMath",
  ExpressionUUID->"3eb4cbe1-cbc9-4e89-aa1e-dbe51ba2df51"],
 "), ",
 Cell[BoxData[
  FormBox["\[Delta]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "ac50dd26-c1d5-41bd-aed7-5f7afc17a7f8"],
 " is the reactor diameter (m), ",
 Cell[BoxData[
  FormBox["v", TraditionalForm]], "InlineMath",ExpressionUUID->
  "429379c6-18fb-4105-bcd7-081a07001625"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["v", "0"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "3aec2bcb-b1fb-486c-8ff9-f28435d7f779"],
 " are the volumetric flow rates down the reactor and initially (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     StyleBox["m",
      FontSlant->"Plain"], "3"], "/", "min"}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"571c4483-f889-4eaf-8abb-bbcb39d0f1b1"],
 "), ",
 Cell[BoxData[
  FormBox["r", TraditionalForm]], "InlineMath",ExpressionUUID->
  "d303121e-c5f3-439f-9acd-0d1b82bbac73"],
 " is rate of reaction (",
 Cell[BoxData[
  FormBox[
   RowBox[{"mol", "/", 
    RowBox[{"[", 
     RowBox[{
      SuperscriptBox[
       StyleBox["m",
        FontSlant->"Plain"], "3"], 
      StyleBox["s",
       FontSlant->"Plain"]}], "]"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"c5237f83-9718-4d6e-9b63-c3cee5c7b889"],
 "), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["r", "perm"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "41998dea-ebd6-4332-bc1b-c22f747cdcb1"],
 " is the rate ",
 Cell[BoxData[
  FormBox[
   StyleBox["D",
    FontSlant->"Plain"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "e687ea7f-53da-4636-8a2a-ed026f9e3cc9"],
 " permeating (mol/min), ",
 Cell[BoxData[
  FormBox["k", TraditionalForm]], "InlineMath",ExpressionUUID->
  "033169ab-b5f3-4090-8f25-77e365408e53"],
 " is the reaction rate constant (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["m", "3"], "/", 
    RowBox[{"[", 
     RowBox[{"mol", " ", 
      StyleBox["s",
       FontSlant->"Plain"]}], "]"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"fda89859-8b86-47f5-adcc-a7291637fda4"],
 "), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["K", "eq"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "c182f257-c232-41ce-a867-110372bd8a12"],
 " is the equilibrium constant (unitless), and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["k", 
    StyleBox["m",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",ExpressionUUID->
  "0dff153b-02e3-48ea-b37c-6f7f38074dbe"],
 " is the membrane permeance, based on concentration difference (",
 Cell[BoxData[
  FormBox[
   RowBox[{"mol", "/", 
    RowBox[{"[", 
     RowBox[{
      SuperscriptBox[
       StyleBox["m",
        FontSlant->"Plain"], "2"], 
      StyleBox["s",
       FontSlant->"Plain"]}], "]"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"5f34e9f7-29d9-48cc-adc9-959f65b762de"],
 ")."
}], "DetailNotes",
