      RowBox[{"(*", 
       RowBox[{"J", "/", "mol"}], "*)"}], " ", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"f", " ", "[", "x_", "]"}], ":=", 
