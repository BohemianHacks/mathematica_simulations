Cell[TextData[{
 "Based on the ideal mixture in the P-x-y diagram (",
 StyleBox["Figure 2",
  FontWeight->"Bold",
  FontColor->RGBColor[0., 0.5019607843137255, 0.5019607843137255]],
 "), use the lever rule to calculate the amount of liquid and vapor present \
for an overall molar composition ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["z", "1"], "=", "0.6"}], TraditionalForm]]],
 ". Note that the composition of component 1 in the liquid phase at constant \
pressure is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "1"], "=", "0.33"}], TraditionalForm]]],
 " and in the vapor phase, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", "1"], "=", "0.71"}], TraditionalForm]]],
 ". "
}], "Item2Exercise",
 TextAlignment->Left],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution",
 FontWeight->"Bold"]], "Subsubsection",
 CellDingbat->"\[LightBulb]",
 CellTags->"Solution 1"],

Cell[TextData[{
 "The fraction of liquid present ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    SuperscriptBox["f", "L"], ")"}], TraditionalForm]]],
 " is,\n",
 StyleBox[" ",
  FontSize->10],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["f", "L"], "=", 
    RowBox[{
     FractionBox["L", 
      RowBox[{"L", "+", "V"}]], "=", 
     RowBox[{
      FractionBox[
       RowBox[{
        SubscriptBox["z", "1"], "-", 
        SubscriptBox["y", "1"]}], 
       RowBox[{
        SubscriptBox["x", "1"], "-", 
        SubscriptBox["y", "1"]}]], "=", 
      RowBox[{
       FractionBox[
        RowBox[{"0.6", "-", "0.71"}], 
        RowBox[{"0.33", "-", "0.71"}]], "=", 
       RowBox[{
       "0.29", "   ", "or", "  ", "29", "%", "   ", "liquid"}]}]}]}]}], 
   TraditionalForm]]],
 "\n",
 StyleBox["\t",
  FontSize->10],
 "\nThe fraction of vapor present ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    SuperscriptBox["f", "V"], ")"}], TraditionalForm]]],
 " is,\n",
 StyleBox[" ",
  FontSize->10],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["f", "V"], "=", 
    RowBox[{
     FractionBox["V", 
      RowBox[{"L", "+", "V"}]], "=", 
     RowBox[{
      FractionBox[
       RowBox[{
        SubscriptBox["z", "1"], "-", 
        SubscriptBox["x", "1"]}], 
       RowBox[{
        SubscriptBox["y", "1"], "-", 
        SubscriptBox["x", "1"]}]], "=", 
      RowBox[{
       FractionBox[
        RowBox[{"0.6", "-", "0.33"}], 
        RowBox[{"0.71", "-", "0.33"}]], "=", 
       RowBox[{"0.71", "   ", "or", "  ", "71", "%", "  ", "vapor"}]}]}]}]}], 
   TraditionalForm]]],
 "\[LineSeparator]",
 StyleBox[" ",
  FontSize->10]
}], "Item2Exercise",
 TextAlignment->Left],

