            "p3"}], "}"}], ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"p", "=", "0.733"}], ";", "\[IndentingNewLine]", 
          RowBox[{"(*", 
           RowBox[{
            RowBox[{"Psat1", "=", "1.569"}], ";"}], " ", "*)"}], 
          "\[IndentingNewLine]", 
          RowBox[{"Psat1", "=", "1.536"}], ";", "\[IndentingNewLine]", 
          RowBox[{"Psat2", "=", "0.319"}], ";", " ", "\[IndentingNewLine]", 
