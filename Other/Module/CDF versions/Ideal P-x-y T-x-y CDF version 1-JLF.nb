Cell[BoxData[
 RowBox[{"Framed", "[", 
  RowBox[{
   RowBox[{"Column", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Manipulate", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Module", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
            "p", ",", "Psat1", ",", "Psat2", ",", "Px", ",", "Py", ",", "x1", 
             ",", "y1", ",", "leverx", ",", "levery"}], "}"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"p", "=", "0.7"}], ";", "\[IndentingNewLine]", 
            RowBox[{"Psat1", "=", "1.536"}], ";", " ", "\[IndentingNewLine]", 
            RowBox[{"Psat2", "=", "0.319"}], ";", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"Px", "[", "x_", "]"}], "=", 
             RowBox[{
              RowBox[{"x", "*", "Psat1"}], "+", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{"1", "-", "x"}], ")"}], "*", "Psat2"}]}]}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"Py", "[", "x_", "]"}], "=", 
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{
                FractionBox["x", "Psat1"], "+", 
