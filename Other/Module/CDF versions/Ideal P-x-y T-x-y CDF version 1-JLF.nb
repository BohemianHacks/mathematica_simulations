Cell[TextData[{
 "For the system in ",
 StyleBox["Figure 2",
  FontWeight->"Bold",
  FontColor->RGBColor[0., 0.5019607843137255, 0.5019607843137255]],
 ", use the lever rule to calculate the fraction of liquid and vapor present \
for an overall molar composition ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["z", "1"], "=", "0.6"}], TraditionalForm]]],
 " and a pressure of 0.7 bar. Note that at 0.7 bar, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "1"], "=", "0.31"}], TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", "1"], "=", "0.69"}], TraditionalForm]]],
 "."
}], "Item2Exercise",
 TextAlignment->Left],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution",
 FontWeight->"Bold"]], "Subsubsection",
 CellDingbat->"\[LightBulb]",
 CellTags->"Solution 1"],

Cell[TextData[{
 "The fraction of liquid present is,\n",
 StyleBox[" ",
  FontSize->10],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"L", "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       SubscriptBox["z", "1"], "-", 
       SubscriptBox["y", "1"]}], 
      RowBox[{
       SubscriptBox["x", "1"], "-", 
       SubscriptBox["y", "1"]}]], "=", 
     RowBox[{
      FractionBox[
       RowBox[{"0.6", "-", "0.69"}], 
       RowBox[{"0.31", "-", "0.69"}]], "=", 
      RowBox[{"0.23", "   ", "or", "  ", "23", "%", "   ", "liquid"}]}]}]}], 
   TraditionalForm]]],
 "\n",
 StyleBox["\t",
  FontSize->10],
 "\nThe fraction of vapor present  is,\n",
 StyleBox[" ",
  FontSize->10],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"V", "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       SubscriptBox["z", "1"], "-", 
       SubscriptBox["x", "1"]}], 
      RowBox[{
       SubscriptBox["y", "1"], "-", 
       SubscriptBox["x", "1"]}]], "=", 
     RowBox[{
      FractionBox[
       RowBox[{"0.6", "-", "0.31"}], 
       RowBox[{"0.69", "-", "0.31"}]], "=", 
      RowBox[{"0.77", "   ", "or", "  ", "77", "%", "  ", "vapor"}]}]}]}], 
   TraditionalForm]]],
 "\[LineSeparator]",
 StyleBox[" ",
  FontSize->10]
}], "Item2Exercise",
 TextAlignment->Left],

