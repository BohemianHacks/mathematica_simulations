   3.6219544638407235`*^9, 3.621954463920728*^9}, 3.622476417159371*^9}],

Cell[BoxData[
 FormBox[
  RowBox[{"P", "=", 
   FractionBox["1", 
    RowBox[{
     FractionBox[
      SubscriptBox["y", "1"], 
      SubsuperscriptBox["P", "1", "sat"]], "+", 
     FractionBox[
      RowBox[{"1", "-", 
       SubscriptBox["y", "1"]}], 
      SubsuperscriptBox["P", "2", "sat"]]}]]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.6213559720228696`*^9, 3.621356012255171*^9}, {
