Cell[TextData[{
 "The lever rule is used to determine the relative amounts of liquid and \
vapor in the system. An explanation of the lever rule is shown in this",
 ButtonBox[" Screencast",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://www.youtube.com/watch?v=celOhlEoQ9c"], None},
  ButtonNote->"https://www.youtube.com/watch?v=celOhlEoQ9c"],
 ".\n \nMouseover the plot below to see the points used in the lever rule \
calculations.\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"L", "=", 
    FractionBox[
     RowBox[{
      SubscriptBox["y", "1"], "-", 
      SubscriptBox["z", "1"]}], 
     RowBox[{
      SubscriptBox["y", "1"], "-", 
      SubscriptBox["x", "1"]}]]}], TraditionalForm]]],
 "\tliquid amount present at ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["z", "1"], TraditionalForm]]],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"V", "=", 
    FractionBox[
     RowBox[{
      SubscriptBox["x", "1"], "-", 
      SubscriptBox["z", "1"]}], 
     RowBox[{
      SubscriptBox["x", "1"], "-", 
      SubscriptBox["y", "1"]}]]}], TraditionalForm]]],
 "\tvapor amount present at ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["z", "1"], TraditionalForm]]]
}], "Text",
