Cell[BoxData[
 RowBox[{"Framed", "@", 
  RowBox[{"Column", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"Framed", "@", 
       RowBox[{"Module", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
          "Pe", ",", "Psat1e", ",", "Psat2e", ",", "i", ",", "Txtable", ",", 
           "Tytable", ",", "Txx", ",", "Tyy"}], "}"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"Pe", "=", "0.7"}], ";", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"Psat1e", "[", "Te_", "]"}], "=", 
           RowBox[{"0.00133322368", "*", 
            RowBox[{"10", "^", 
             RowBox[{"(", 
              RowBox[{"6.90565", "-", 
               FractionBox["1211.033", 
                RowBox[{"Te", "+", "220.79"}]]}], ")"}]}]}]}], ";", " ", 
