Cell[TextData[{
 "The Antoine equation is used to get the saturation pressures of each \
component (",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["P", "1", "sat"], TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["P", "2", "sat"], TraditionalForm]]],
 ")."
}], "Text",
