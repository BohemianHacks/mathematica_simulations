Cell[CellGroupData[{

Cell["Concept Test", "Item1Exercise",
 CellChangeTimes->{{3.6234225790685253`*^9, 3.623422581743678*^9}},
 CellTags->"Concept Test 1"],

Cell[TextData[{
 "Based on the ideal mixture in the P-x-y diagram (",
 StyleBox["Figure 2",
  FontWeight->"Bold",
  FontColor->RGBColor[0., 0.5019607843137255, 0.5019607843137255]],
 "), use the lever rule to calculate the amount of liquid and vapor present \
for an overall molar composition ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["z", "1"], "=", "0.6"}], TraditionalForm]]],
 ". Note that the composition of component 1 in the liquid phase at constant \
pressure is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "1"], "=", "0.33"}], TraditionalForm]]],
 " and in the vapor phase, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", "1"], "=", "0.71"}], TraditionalForm]]],
 ". "
}], "Item2Exercise",
 CellChangeTimes->{{3.6213499310153437`*^9, 3.6213501742782574`*^9}, {
   3.621350243440213*^9, 3.621350246436385*^9}, {3.6216235826248894`*^9, 
   3.6216235839119635`*^9}, 3.6224764410647383`*^9, {3.622905537653512*^9, 
   3.6229055599987903`*^9}},
 TextAlignment->Left],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution",
 FontWeight->"Bold"]], "Subsubsection",
 CellDingbat->"\[LightBulb]",
 CellChangeTimes->{{3.621620710993642*^9, 3.6216207129897556`*^9}, {
  3.621621128774537*^9, 3.6216211299166026`*^9}},
 CellTags->"Solution 1"],

Cell[TextData[{
 "The fraction of liquid present ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    SuperscriptBox["f", "L"], ")"}], TraditionalForm]]],
 " is,\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["f", "L"], "=", 
    RowBox[{
     FractionBox["L", 
      RowBox[{"L", "+", "V"}]], "=", 
     RowBox[{
      FractionBox[
       RowBox[{
        SubscriptBox["z", "1"], "-", 
        SubscriptBox["y", "1"]}], 
       RowBox[{
        SubscriptBox["x", "1"], "-", 
        SubscriptBox["y", "1"]}]], "=", 
      RowBox[{
       FractionBox[
        RowBox[{"0.6", "-", "0.71"}], 
        RowBox[{"0.33", "-", "0.71"}]], "=", 
       RowBox[{
       "0.29", "   ", "or", "  ", "29", "%", "   ", "liquid"}]}]}]}]}], 
   TraditionalForm]]],
 "\n\t\nThe fraction of vapor present ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    SuperscriptBox["f", "V"], ")"}], TraditionalForm]]],
 " is,\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["f", "V"], "=", 
    RowBox[{
     FractionBox["V", 
      RowBox[{"L", "+", "V"}]], "=", 
     RowBox[{
      FractionBox[
       RowBox[{
        SubscriptBox["z", "1"], "-", 
        SubscriptBox["x", "1"]}], 
       RowBox[{
        SubscriptBox["y", "1"], "-", 
        SubscriptBox["x", "1"]}]], "=", 
      RowBox[{
       FractionBox[
        RowBox[{"0.6", "-", "0.33"}], 
        RowBox[{"0.71", "-", "0.33"}]], "=", 
       RowBox[{"0.71", "   ", "or", "  ", "71", "%", "  ", "vapor"}]}]}]}]}], 
   TraditionalForm]]],
 "\[LineSeparator]"
}], "Item2Exercise",
 CellChangeTimes->{{3.621620710993642*^9, 3.621620720488185*^9}, {
   3.621782778853834*^9, 3.6217827825110435`*^9}, {3.621783410352954*^9, 
