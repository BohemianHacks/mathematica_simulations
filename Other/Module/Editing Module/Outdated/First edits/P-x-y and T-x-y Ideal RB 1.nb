Cell[TextData[{
 "The fraction of liquid present is,\n",
 StyleBox[" ",
  FontSize->10],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"L", "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       SubscriptBox["z", "1"], "-", 
       SubscriptBox["y", "1"]}], 
      RowBox[{
       SubscriptBox["x", "1"], "-", 
       SubscriptBox["y", "1"]}]], "=", 
     RowBox[{
      FractionBox[
       RowBox[{"0.6", "-", "0.69"}], 
       RowBox[{"0.31", "-", "0.69"}]], "=", 
      RowBox[{"0.23", "   ", "or", "  ", "23", "%", "   ", "liquid"}]}]}]}], 
   TraditionalForm]]],
 "\n",
 StyleBox["\t",
  FontSize->10],
 "\nThe fraction of vapor present  is,\n",
 StyleBox[" ",
  FontSize->10],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"V", "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       SubscriptBox["z", "1"], "-", 
       SubscriptBox["x", "1"]}], 
      RowBox[{
       SubscriptBox["y", "1"], "-", 
       SubscriptBox["x", "1"]}]], "=", 
     RowBox[{
      FractionBox[
       RowBox[{"0.6", "-", "0.31"}], 
       RowBox[{"0.69", "-", "0.31"}]], "=", 
      RowBox[{"0.77", "   ", "or", "  ", "77", "%", "  ", "vapor"}]}]}]}], 
   TraditionalForm]]],
 "\[LineSeparator]",
 StyleBox[" ",
  FontSize->10]
}], "Item2Exercise",
