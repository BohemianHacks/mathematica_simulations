Cell[TextData[{
 "The fraction of liquid  ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    SuperscriptBox["f", "L"], ")"}], TraditionalForm]]],
 " is,\n",
 StyleBox[" ",
  FontSize->10],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["f", "L"], "=", 
    RowBox[{
     FractionBox["L", 
      RowBox[{"L", "+", "V"}]], "=", 
     RowBox[{
      FractionBox[
       RowBox[{
        SubscriptBox["z", "1"], "-", 
        SubscriptBox["y", "1"]}], 
       RowBox[{
        SubscriptBox["x", "1"], "-", 
        SubscriptBox["y", "1"]}]], "=", 
      RowBox[{
       FractionBox[
        RowBox[{"0.6", "-", "0.71"}], 
        RowBox[{"0.33", "-", "0.71"}]], "=", 
       RowBox[{
       "0.29", "   ", "or", "  ", "29", "%", "   ", "liquid"}]}]}]}]}], 
   TraditionalForm]]],
 "\n",
 StyleBox["\t",
  FontSize->10],
 "\nThe fraction of vapor  ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    SuperscriptBox["f", "V"], ")"}], TraditionalForm]]],
 " is,\n",
 StyleBox[" ",
  FontSize->10],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["f", "V"], "=", 
    RowBox[{
     FractionBox["V", 
      RowBox[{"L", "+", "V"}]], "=", 
     RowBox[{
      FractionBox[
       RowBox[{
        SubscriptBox["z", "1"], "-", 
        SubscriptBox["x", "1"]}], 
       RowBox[{
        SubscriptBox["y", "1"], "-", 
        SubscriptBox["x", "1"]}]], "=", 
      RowBox[{
       FractionBox[
        RowBox[{"0.6", "-", "0.33"}], 
        RowBox[{"0.71", "-", "0.33"}]], "=", 
       RowBox[{"0.71", "   ", "or", "  ", "71", "%", "  ", "vapor"}]}]}]}]}], 
   TraditionalForm]]],
 "\[LineSeparator]",
 StyleBox[" ",
  FontSize->10]
}], "Item2Exercise",
