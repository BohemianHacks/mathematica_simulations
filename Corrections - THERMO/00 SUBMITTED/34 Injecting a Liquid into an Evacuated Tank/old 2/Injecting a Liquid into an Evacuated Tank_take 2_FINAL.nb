 Cell[BoxData[
  FormBox[
   SuperscriptBox["V", 
    StyleBox["L",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " and vapor ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["V", 
    StyleBox["V",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " volumes equal the volume of the tank ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]], "InlineMath"],
 ":"
}], "DetailNotes",
 CellChangeTimes->{{3.653670027844483*^9, 3.653670167293459*^9}, {
  3.653670202001444*^9, 3.653670288815409*^9}, {3.6536703419184465`*^9, 
  3.6536704206959524`*^9}, {3.6537638384097223`*^9, 3.65376385881698*^9}, {
  3.68512120010133*^9, 3.6851212038493366`*^9}},
 CellID->52280333],

