    RowBox[{
     SuperscriptBox["n", 
      StyleBox["V",
       FontSlant->"Plain"]], " ", "R", " ", 
     RowBox[{"T", "/", 
      SuperscriptBox["P", "sat"]}]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
