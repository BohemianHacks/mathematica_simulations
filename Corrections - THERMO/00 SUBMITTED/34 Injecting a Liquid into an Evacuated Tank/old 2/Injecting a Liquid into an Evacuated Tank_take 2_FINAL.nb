  FormBox[
   RowBox[{"V", "=", 
    RowBox[{
     SuperscriptBox["V", 
      StyleBox["L",
       FontSlant->"Plain"]], "+", 
     SuperscriptBox["V", 
      StyleBox["V",
       FontSlant->"Plain"]]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
