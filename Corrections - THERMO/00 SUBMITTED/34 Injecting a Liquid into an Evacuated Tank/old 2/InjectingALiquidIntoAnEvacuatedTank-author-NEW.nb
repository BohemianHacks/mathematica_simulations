Cell[CellGroupData[{

Cell["", "DetailsSection"],

Cell["\<\
When propane is in vapor-liquid equilibrium, the pressure in the tank is the \
saturation pressure, which is calculated using the Antoine equation:\
\>", "DetailNotes",
 CellChangeTimes->{{3.653670027844483*^9, 3.653670062777481*^9}, {
  3.6536703538841314`*^9, 3.653670358516396*^9}},
 CellID->486969965],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["P", "sat"], "=", 
    SuperscriptBox["10", 
     RowBox[{"A", "-", 
      RowBox[{"B", "/", 
       RowBox[{"(", 
        RowBox[{"T", "+", "C"}], ")"}]}]}]]}], TraditionalForm]], 
  "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.653670027844483*^9, 3.65367008741389*^9}, 
   3.653763814898385*^9, {3.65376394168322*^9, 3.6537639473293867`*^9}},
 CellID->378425850],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["P", "sat"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is the saturation pressure (bar), ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ", ",
 Cell[BoxData[
  FormBox["B", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ", and ",
 Cell[BoxData[
  FormBox["C", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " are Antoine constants, and ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is temperature (K)."
}], "DetailNotes",
 CellChangeTimes->{{3.653670027844483*^9, 3.65367016730746*^9}},
 CellID->812005259],

Cell[TextData[{
 "When only vapor is present, the ideal gas law is used to calculate the \
pressure ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath"],
 ":"
}], "DetailNotes",
 CellChangeTimes->{{3.653670027844483*^9, 3.653670167293459*^9}, {
  3.653670202001444*^9, 3.6536702212605457`*^9}, {3.6537638237941837`*^9, 
  3.6537638256099987`*^9}},
 CellID->54955516],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{"n", " ", "R", " ", 
     RowBox[{"T", "/", "V"}]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.653670027844483*^9, 3.653670167293459*^9}, {
   3.653670202001444*^9, 3.653670234369295*^9}, 3.6537638295459127`*^9},
 CellID->29619311],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath"],
 " is the total moles (moles of vapor in this case), ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]], "InlineMath"],
 " is the ideal gas constant ([L bar]/[mol K]), and ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]], "InlineMath"],
 " is the total volume (L)."
}], "DetailNotes",
 CellChangeTimes->{{3.653670027844483*^9, 3.653670167293459*^9}, {
  3.653670202001444*^9, 3.65367028883041*^9}, {3.6782148109039416`*^9, 
  3.6782148180959935`*^9}},
 CellID->341171121],

Cell[TextData[{
 "When propane is in VLE, the sum of the liquid ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["V", 
    StyleBox["L",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " and vapor ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["V", 
    StyleBox["V",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " volumes equal the volume of the tank ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]], "InlineMath"],
 ":"
}], "DetailNotes",
 CellChangeTimes->{{3.653670027844483*^9, 3.653670167293459*^9}, {
  3.653670202001444*^9, 3.653670288815409*^9}, {3.6536703419184465`*^9, 
  3.6536704206959524`*^9}, {3.6537638384097223`*^9, 3.65376385881698*^9}},
 CellID->52280333],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"V", "=", 
    RowBox[{
     SuperscriptBox["V", 
      StyleBox["L",
       FontSlant->"Plain"]], "+", 
     SuperscriptBox["V", 
      StyleBox["V",
       FontSlant->"Plain"]]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6537638655684557`*^9, 3.653763866256777*^9}, {
  3.678214829538096*^9, 3.6782148308536015`*^9}},
 CellID->979521570],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["V", 
     StyleBox["L",
      FontSlant->"Plain"]], "=", 
    RowBox[{
     SuperscriptBox["n", 
      StyleBox["L",
       FontSlant->"Plain"]], "/", 
     SuperscriptBox["\[Rho]", 
      StyleBox["L",
       FontSlant->"Plain"]]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.653670027844483*^9, 3.653670167293459*^9}, {
   3.653670202001444*^9, 3.653670288815409*^9}, {3.6536703419184465`*^9, 
   3.6536704632013836`*^9}, 3.653763870753104*^9},
 CellID->84209966],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["V", 
     StyleBox["V",
      FontSlant->"Plain"]], "=", 
    RowBox[{
     SuperscriptBox["n", 
      StyleBox["V",
       FontSlant->"Plain"]], " ", "R", " ", 
     RowBox[{"T", "/", 
      SuperscriptBox["P", "sat"]}]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.653670027844483*^9, 3.653670167293459*^9}, {
   3.653670202001444*^9, 3.653670288815409*^9}, {3.6536703419184465`*^9, 
   3.6536704863957105`*^9}, 3.6537638729284163`*^9},
 CellID->154281602],

Cell[TextData[{
 "and the total moles (moles injected) equals the sum of the liquid ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["n", 
    StyleBox["L",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " and vapor ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["n", 
    StyleBox["V",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " moles:"
}], "DetailNotes",
 CellChangeTimes->{{3.653670027844483*^9, 3.653670167293459*^9}, {
  3.653670202001444*^9, 3.653670288815409*^9}, {3.6536703419184465`*^9, 
  3.6536705495313215`*^9}, {3.6537638775842953`*^9, 3.65376388515208*^9}},
 CellID->37672348],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "=", 
    RowBox[{
     SuperscriptBox["n", 
      StyleBox["L",
       FontSlant->"Plain"]], "+", 
     SuperscriptBox["n", 
      StyleBox["V",
       FontSlant->"Plain"]]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.653670027844483*^9, 3.653670167293459*^9}, {
   3.653670202001444*^9, 3.653670288815409*^9}, {3.6536703419184465`*^9, 
   3.65367054146486*^9}, 3.65376388875183*^9},
 CellID->78212528],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[Rho]", 
    StyleBox["L",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " is the molar density of liquid propane (mol/L)."
}], "DetailNotes",
 CellChangeTimes->{{3.653670027844483*^9, 3.653670167293459*^9}, {
   3.653670202001444*^9, 3.653670288815409*^9}, {3.6536703419184465`*^9, 
   3.6536705889705772`*^9}, 3.653670643454694*^9, {3.653750253513911*^9, 
   3.6537502535259113`*^9}},
 CellID->92236548],

Cell[TextData[{
 "Solve ",
 Cell[BoxData[
  FormBox[
   RowBox[{"V", "=", 
    RowBox[{
     SuperscriptBox["V", 
      StyleBox["L",
       FontSlant->"Plain"]], "+", 
     SuperscriptBox["V", 
      StyleBox["V",
       FontSlant->"Plain"]]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " for ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["n", 
    StyleBox["L",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ":"
}], "DetailNotes",
 CellChangeTimes->{{3.653670027844483*^9, 3.653670167293459*^9}, {
   3.653670202001444*^9, 3.653670288815409*^9}, {3.6536703419184465`*^9, 
   3.6536705889705772`*^9}, 3.653670643454694*^9, {3.653750253513911*^9, 
   3.653750282556572*^9}},
 CellID->156951984],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"V", "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["n", 
       StyleBox["L",
        FontSlant->"Plain"]], "/", 
      SuperscriptBox["\[Rho]", 
       StyleBox["L",
        FontSlant->"Plain"]]}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"n", "-", 
        SuperscriptBox["n", 
         StyleBox["L",
          FontSlant->"Plain"]]}], ")"}], " ", "R", " ", 
      RowBox[{"T", "/", 
       SuperscriptBox["P", "sat"]}]}]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.653670027844483*^9, 3.653670167293459*^9}, {
   3.653670202001444*^9, 3.653670288815409*^9}, {3.6536703419184465`*^9, 
   3.6536705889705772`*^9}, 3.653670643454694*^9, {3.653750253513911*^9, 
   3.6537503258150463`*^9}, 3.65376390039147*^9},
 CellID->80239598],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["n", 
     StyleBox["L",
      FontSlant->"Plain"]], "=", 
    FractionBox[
     RowBox[{
      SuperscriptBox["\[Rho]", 
       StyleBox["L",
        FontSlant->"Plain"]], " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         SuperscriptBox["P", "sat"], " ", "V"}], "-", 
        RowBox[{"n", " ", "R", " ", "T"}]}], ")"}]}], 
     RowBox[{
      SuperscriptBox["P", "sat"], "-", 
      RowBox[{"R", " ", "T", " ", 
       SuperscriptBox["\[Rho]", 
        StyleBox["L",
         FontSlant->"Plain"]]}]}]]}], TraditionalForm]], "InlineMath"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.653670027844483*^9, 3.653670167293459*^9}, {
   3.653670202001444*^9, 3.653670288815409*^9}, {3.6536703419184465`*^9, 
   3.6536705889705772`*^9}, 3.653670643454694*^9, {3.653750253513911*^9, 
