  FormBox[
   RowBox[{
    SubscriptBox["P", 
     StyleBox["y",
      FontSlant->"Plain"]], "=", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       FractionBox["z", 
        SubsuperscriptBox["P", 
         StyleBox["A",
          FontSlant->"Plain"], "sat"]], "+", 
