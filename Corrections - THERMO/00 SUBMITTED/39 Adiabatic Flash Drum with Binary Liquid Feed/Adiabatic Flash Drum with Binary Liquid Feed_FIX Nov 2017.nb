  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", 
       StyleBox["m",
        FontSlant->"Plain"]], " ", 
      SubsuperscriptBox["P", 
       StyleBox["m",
        FontSlant->"Plain"], "sat"]}], "+", 
     RowBox[{
      SubscriptBox["x", 
