Cell[TextData[{
 "Here, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "1"], TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "2"], TraditionalForm]], "InlineMath"],
 " are the liquid mole fractions of ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath"],
 "-hexane and ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath"],
 "-octane. The mole fractions of ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath"],
 "-hexane and ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath"],
 "-octane in the vapor phase are ",
