      RowBox[{
       SubscriptBox["n", 
        RowBox[{"i", ",", "out"}]], " ", 
       SubscriptBox["H", 
        RowBox[{"i", ",", "out"}]]}]}], "-", 
     RowBox[{"\[Sum]", 
      RowBox[{
       SubscriptBox["n", 
        RowBox[{"i", ",", "in"}]], " ", 
       SubscriptBox["H", 
        RowBox[{"i", ",", "in"}]]}]}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
