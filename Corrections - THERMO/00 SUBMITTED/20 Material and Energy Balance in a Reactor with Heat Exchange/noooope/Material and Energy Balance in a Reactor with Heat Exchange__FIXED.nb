  FormBox[
   RowBox[{
    SubscriptBox["H", 
     RowBox[{"i", ",", "out"}]], "=", 
    RowBox[{
     SubscriptBox["H", 
      RowBox[{"i", ",", "in"}]], "+", 
     RowBox[{
      SubscriptBox["Cp", "i"], " ", "\[CapitalDelta]", "\[InvisibleSpace]", 
      "T"}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.674484268361518*^9, 3.6744846660425315`*^9}, {
  3.6744847374052515`*^9, 3.674484896064768*^9}},
 CellID->40838447],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["H", 
    RowBox[{"i", ",", "in"}]], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " and ",
