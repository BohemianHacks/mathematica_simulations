Cell[CellGroupData[{

Cell["", "DetailsSection"],

Cell[TextData[{
 "The reaction ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     StyleBox["A",
      FontSlant->"Plain"], "+", 
     StyleBox["B",
      FontSlant->"Plain"]}], "\[Rule]", 
    StyleBox["C",
     FontSlant->"Plain"]}], TraditionalForm]], "InlineMath"],
 " takes place in a reactor with heat exchange. Reactants ",
 Cell[BoxData[
  FormBox[
   StyleBox["A",
    FontSlant->"Plain"], TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox[
   StyleBox["B",
    FontSlant->"Plain"], TraditionalForm]], "InlineMath"],
 " and inert ",
 Cell[BoxData[
  FormBox[
   StyleBox["I",
    FontSlant->"Plain"], TraditionalForm]], "InlineMath"],
 " enter the reactor. The molar flow rates of each component exiting the \
reactor are calculated using the extent of reaction:"
}], "DetailNotes",
 CellChangeTimes->{{3.674484268361518*^9, 3.6744845755416155`*^9}, 
   3.674487214861476*^9, {3.6746145033785152`*^9, 3.6746145845822477`*^9}},
 CellID->12956179],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["n", 
     RowBox[{"i", ",", "out"}]], "=", 
    RowBox[{"\[Nu]", " ", 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["n", 
        RowBox[{"i", ",", "in"}]], "-", "\[Zeta]"}], ")"}]}]}], 
   TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.674484268361518*^9, 3.6744846153084927`*^9}},
 CellID->133966462],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["n", 
    RowBox[{"i", ",", "out"}]], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["n", 
    RowBox[{"i", ",", "in"}]], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " are the molar flow rates of component ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "=", 
    RowBox[{"(", 
     RowBox[{
      StyleBox["A",
       FontSlant->"Plain"], ",", 
      StyleBox["B",
       FontSlant->"Plain"], ",", 
      StyleBox["C",
       FontSlant->"Plain"], ",", 
      StyleBox["I",
       FontSlant->"Plain"]}], ")"}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " exiting and entering the reactor (mol/h), ",
 Cell[BoxData[
  FormBox["\[Nu]", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is the stoichiometric coefficient, and ",
 Cell[BoxData[
  FormBox["\[Zeta]", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is the extent of reaction (mol/h)."
}], "DetailNotes",
 CellChangeTimes->{{3.674484268361518*^9, 3.6744846660425315`*^9}, {
  3.6744847374052515`*^9, 3.6744847542524214`*^9}, {3.674484933870607*^9, 
  3.6744849530762873`*^9}},
 CellID->48080531],

Cell[TextData[{
 "The overall energy balance to determine the heat added or removed ",
 Cell[BoxData[
  FormBox["Q", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is:"
}], "DetailNotes",
 CellChangeTimes->{{3.674484268361518*^9, 3.6744846660425315`*^9}, {
  3.6744847374052515`*^9, 3.674484793063774*^9}},
 CellID->119079996],

Cell[TextData[{
