Cell[CellGroupData[{

Cell["", "DetailsSection"],

Cell["\<\
The volume and density are calculated from the Peng-Robinson equation of \
state:\
\>", "DetailNotes",
 CellChangeTimes->{{3.676150902487013*^9, 3.6761509249721804`*^9}, 
   3.6761512252907863`*^9},
 CellID->113927855],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     FractionBox[
      RowBox[{"R", " ", "T"}], 
      RowBox[{"V", "-", "b"}]], "-", 
     FractionBox["a", 
      RowBox[{
       SuperscriptBox["V", "2"], "+", 
       RowBox[{"2", " ", "V", " ", "b"}], "-", 
       SuperscriptBox["b", "2"]}]]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.676150902487013*^9, 3.676150953615452*^9}},
 CellID->5796550],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"a", "=", 
    RowBox[{"0.457", " ", 
     FractionBox[
      RowBox[{
       SuperscriptBox["R", "2"], " ", 
       SubsuperscriptBox["T", "c", "2"]}], 
      SubscriptBox["P", "c"]], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{"\[Kappa]", " ", 
         RowBox[{"(", 
          RowBox[{"1", "-", 
           SqrtBox[
            RowBox[{"T", "/", 
             SubscriptBox["T", "c"]}]]}], ")"}]}]}], ")"}], "2"]}]}], 
   TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.676150902487013*^9, 3.6761509891302276`*^9}, {
  3.676151040877079*^9, 3.6761510555916805`*^9}},
 CellID->548693058],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"b", "=", 
    RowBox[{"0.0778", " ", 
     FractionBox[
      RowBox[{"R", " ", 
       SubscriptBox["T", "c"]}], 
      SubscriptBox["P", "c"]]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.676150902487013*^9, 3.676151001022301*^9}, {
  3.67615105254916*^9, 3.6761510583336926`*^9}},
 CellID->1248694],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Kappa]", "=", 
    RowBox[{"0.375", "+", 
     RowBox[{"1.54", " ", "\[Omega]"}], "-", 
     RowBox[{"0.27", " ", 
      SuperscriptBox["\[Omega]", "2"]}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.676150902487013*^9, 3.6761510201744304`*^9}},
 CellID->147558564],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is pressure (MPa), ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is temperature (K), the subscript ",
 Cell[BoxData[
  FormBox["c", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " represents the critical property, ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is the molar volume (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["cm", "3"], "/", "mol"}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 "), ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is the ideal gas constant, ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ", ",
 Cell[BoxData[
  FormBox["b", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ", and ",
 Cell[BoxData[
  FormBox["\[Kappa]", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " are Peng-Robinson constants, and ",
 Cell[BoxData[
  FormBox["\[Omega]", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is the acentric factor."
}], "DetailNotes",
 CellChangeTimes->{{3.676150902487013*^9, 3.676151192349085*^9}},
 CellID->818126333],

Cell[TextData[{
 "The molar density is ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Rho]", "=", 
    RowBox[{"1", "/", "V"}]}], TraditionalForm]], "InlineMath"],
 " (",
 Cell[BoxData[
  FormBox[
   RowBox[{"mol", "/", 
    SuperscriptBox["cm", "3"]}], TraditionalForm]], "InlineMath"],
 ")."
}], "DetailNotes",
 CellChangeTimes->{{3.6763105575810575`*^9, 3.6763105575966578`*^9}, 
   3.6775904432453933`*^9},
 CellID->534252177],

