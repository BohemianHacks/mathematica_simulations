       RowBox[{
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["y", 
           SubscriptBox["N", "2"]], " ", "P"}], ")"}], " ", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           SubscriptBox["y", 
            SubscriptBox["H", "2"]], " ", "P"}], ")"}], "3"]}]], "=", 
      RowBox[{
       FractionBox[
        SuperscriptBox[
         RowBox[{"(", 
          SubscriptBox["y", 
           SubscriptBox["NH", "3"]], ")"}], "2"], 
        RowBox[{
         SubscriptBox["y", 
          SubscriptBox[
           StyleBox["N",
            FontSlant->"Plain"], "2"]], " ", 
         SuperscriptBox[
          RowBox[{"(", 
           SubscriptBox["y", 
            SubscriptBox[
             StyleBox["H",
