Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", "G"}], "=", 
    RowBox[{
     RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", 
      SubscriptBox["H", 
       StyleBox["r",
        FontSlant->"Plain"]]}], "-", 
     RowBox[{"T", " ", "\[CapitalDelta]", "\[InvisibleSpace]", 
      SubscriptBox["S", 
       StyleBox["r",
        FontSlant->"Plain"]]}]}]}], TraditionalForm]], "InlineMath"],
 " is Gibbs free energy (J/mol), ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", 
    SubscriptBox["H", 
     StyleBox["r",
      FontSlant->"Plain"]]}], TraditionalForm]], "InlineMath"],
 " is the heat of reaction (J/mol), ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", 
    SubscriptBox["S", 
     StyleBox["r",
      FontSlant->"Plain"]]}], TraditionalForm]], "InlineMath"],
 " is the entropy change of reaction (J/[mol K]), ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath"],
 " is temperature (K), ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]], "InlineMath"],
 " is the ideal gas constant (J/[mol K]), and ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath"],
 " is pressure (bar)."
}], "DetailNotes",
