Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"K", "=", 
    RowBox[{
     RowBox[{"\[Product]", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["y", "i"], " ", "P"}], ")"}], 
       RowBox[{"-", 
        SubscriptBox["v", "i"]}]]}], "=", 
     RowBox[{
      FractionBox[
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["y", 
