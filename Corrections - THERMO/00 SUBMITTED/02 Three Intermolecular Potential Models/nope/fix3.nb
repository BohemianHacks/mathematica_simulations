Cell["The Lennard-Jones potential is:", "DetailNotes",
 CellChangeTimes->{{3.59162109523731*^9, 3.5916211141913958`*^9}, 
   3.595798078673379*^9, 3.595800251390283*^9},
 CellID->74900062],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"u", "(", "r", ")"}], "=", 
   RowBox[{"4", 
    RowBox[{"\[Epsilon]", " ", "[", " ", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox["\[Sigma]", "r"], ")"}], "12"], "-", 
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox["\[Sigma]", "r"], ")"}], "6"]}], " ", "]"}]}]}], 
  TraditionalForm]], "InlineMath"]], "DetailNotes",
 CellChangeTimes->{{3.59162109523731*^9, 3.591621180968975*^9}, 
