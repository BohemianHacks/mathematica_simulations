Cell[TextData[{
 "The molar enthalpy ",
 Cell[BoxData[
  FormBox["H", TraditionalForm]], "InlineMath"],
 " is:"
}], "DetailNotes",
 CellChangeTimes->{{3.6745981198536572`*^9, 3.6745982130680113`*^9}, 
