 CellID->236651627],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"H", "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", 
       StyleBox["A",
        FontSlant->"Plain"]], " ", 
      SubscriptBox["H", 
       StyleBox["A",
        FontSlant->"Plain"]]}], "+", 
     RowBox[{
      SubscriptBox["x", 
       StyleBox["B",
        FontSlant->"Plain"]], " ", 
      SubscriptBox["H", 
       StyleBox["B",
        FontSlant->"Plain"]]}], "+", 
     RowBox[{"\[Alpha]", " ", 
      SubscriptBox["x", 
       StyleBox["A",
        FontSlant->"Plain"]], " ", 
      SubscriptBox["x", 
       StyleBox["B",
        FontSlant->"Plain"]]}]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6745981198536572`*^9, 3.6745981548218865`*^9}, {
  3.6745982000773907`*^9, 3.6745982424312077`*^9}, {3.674598379194602*^9, 
  3.6745983808881245`*^9}, {3.6756237309323263`*^9, 3.6756237378513412`*^9}},
 CellID->494694633],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["H", "i"], TraditionalForm]], "InlineMath"],
 " is the enthalpy (kJ/mol), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", 
    StyleBox["A",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", 
     StyleBox["B",
      FontSlant->"Plain"]], "=", 
    RowBox[{"1", "-", 
     SubscriptBox["x", 
      StyleBox["A",
       FontSlant->"Plain"]]}]}], TraditionalForm]], "InlineMath"],
 " are the compositions of ",
 Cell[BoxData[
  FormBox[
   StyleBox["A",
    FontSlant->"Plain"], TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox[
   StyleBox["B",
    FontSlant->"Plain"], TraditionalForm]], "InlineMath"],
 ", and ",
 Cell[BoxData[
  FormBox["\[Alpha]", TraditionalForm]], "InlineMath"],
 " is a non-ideal parameter."
}], "DetailNotes",
 CellChangeTimes->{{3.6745981198536572`*^9, 3.6745981548218865`*^9}, {
