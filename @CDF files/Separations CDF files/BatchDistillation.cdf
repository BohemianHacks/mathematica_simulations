mixture ",
 Cell[BoxData[
  FormBox[
   RowBox[{"F", " ", 
    RowBox[{"(", "mol", ")"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"f8cde174-dc20-461a-b18b-bfc65b201ecd"],
