       RowBox[{"g", " ", "cos", " ", "\[Theta]", " ", "\[Beta]"}], 
       RowBox[{"\[Nu]", " ", "\[Alpha]"}]], " ", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["T", 
         StyleBox["s",
          FontSlant->"Plain"]], "-", 
