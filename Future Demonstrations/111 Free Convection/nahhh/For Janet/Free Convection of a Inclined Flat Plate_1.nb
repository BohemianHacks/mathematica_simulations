Cell["the Rayleigh number is:", "DetailNotes",
 CellChangeTimes->{{3.718635393946455*^9, 3.718635543547594*^9}},
 CellID->178196554,ExpressionUUID->"00c9032b-2fce-419c-849b-5360e7082d04"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"Ra", "=", 
    RowBox[{
     RowBox[{"Gr", " ", "Pr"}], "=", 
     RowBox[{
      FractionBox[
       RowBox[{"g", " ", "cos", " ", "\[Theta]", " ", "\[Beta]"}], 
       RowBox[{"\[Nu]", " ", "\[Alpha]"}]], " ", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["T", 
         StyleBox["s",
          FontSlant->"Plain"]], "-", 
        SubscriptBox["T", "\[Infinity]"]}], ")"}], " ", 
      SuperscriptBox["x", "3"]}]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"371e07a7-2656-44d3-b849-94e86c52e28d"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.718635393946455*^9, 3.7186355783055677`*^9}},
 CellID->208715162,ExpressionUUID->"6ad14bac-987f-4aa3-9e5f-7e6475ab628f"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Pr", "=", 
    RowBox[{"\[Nu]", "/", "\[Alpha]"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"e13ea6a4-fd35-4911-a4f1-52d39b0221fa"],
 " is the Prandtl number, ",
 Cell[BoxData[
  FormBox["\[Nu]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "d0f1de61-bc5f-4a3e-af39-9bf84812fb0e"],
 " is the kinematic viscosity (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     StyleBox["m",
      FontSlant->"Plain"], "2"], "/", 
    StyleBox["s",
     FontSlant->"Plain"]}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "53619c2e-a22d-49a2-b306-515038a75244"],
 "), ",
 Cell[BoxData[
  FormBox["\[Alpha]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "5494c4b1-c101-4c2c-8c5b-fd79b5d2ca4c"],
 " is thermal diffusivity (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     StyleBox["m",
      FontSlant->"Plain"], "2"], "/", 
    StyleBox["s",
     FontSlant->"Plain"]}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "ec0fa607-3280-4acf-bbbc-5feb3682f678"],
 "), ",
 Cell[BoxData[
  FormBox["Gr", TraditionalForm]], "InlineMath",ExpressionUUID->
  "e8aa2c87-4327-4ed8-beb4-29107e80902c"],
 " is the Grashof number, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"g", "=", 
    RowBox[{"9.8", " ", 
     RowBox[{
      StyleBox["m",
       FontSlant->"Plain"], "/", 
      SuperscriptBox[
       StyleBox["s",
        FontSlant->"Plain"], "2"]}]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"954efdc9-9563-457e-91cb-38e13953fd97"],
 " is acceleration due to gravity,and ",
 Cell[BoxData[
  FormBox["\[Theta]", TraditionalForm]], "InlineMath",ExpressionUUID->
  "bc6b65c5-fb79-49e5-a36e-a615d50017d1"],
 " is angle (degrees)."
}], "DetailNotes",
 CellChangeTimes->{{3.718635393946455*^9, 3.7186357588637886`*^9}},
 CellID->81462172,ExpressionUUID->"7c7d63c0-7fef-4e9c-bc69-ef7299f45ac5"],

