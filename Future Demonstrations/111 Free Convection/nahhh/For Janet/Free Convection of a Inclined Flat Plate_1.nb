Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"h", "=", 
    FractionBox[
     RowBox[{"Nu", " ", "k"}], "x"]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"718da59f-e125-4f4a-81f0-2c1ccf7915e1"],
 ","
}], "DetailNotes",
