Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"G", "(", "s", ")"}], "=", 
    RowBox[{
     SubscriptBox["K", 
      StyleBox["C",
       FontSlant->"Plain"]], " ", 
     SubscriptBox["K", 
      StyleBox["P",
       FontSlant->"Plain"]], " ", 
     SuperscriptBox["e", 
      RowBox[{
       RowBox[{"-", "\[Theta]"}], " ", "s"}]], " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        SubscriptBox["\[Tau]", 
         StyleBox["I",
          FontSlant->"Plain"]], " ", "s"}], "+", "1"}], ")"}], " ", 
     FractionBox[
      RowBox[{
       RowBox[{
        SubscriptBox["\[Tau]", 
         StyleBox["D",
          FontSlant->"Plain"]], " ", "s"}], "+", "1"}], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SubscriptBox["\[Tau]", 
           StyleBox["P",
            FontSlant->"Plain"]], " ", "s"}], "+", "1"}], ")"}], " ", 
       SubscriptBox["\[Tau]", 
        StyleBox["I",
         FontSlant->"Plain"]], " ", "s"}]]}]}], TraditionalForm]], 
  "InlineMath",
