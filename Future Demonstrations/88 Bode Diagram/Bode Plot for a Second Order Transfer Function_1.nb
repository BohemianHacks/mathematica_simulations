Cell["", "DetailsSection",ExpressionUUID->"fb9ed4d5-8601-4d1c-825b-e5d270e54189"],

Cell["The second-order transfer function plus dead time is:", "DetailNotes",
 CellChangeTimes->{{3.7130175122882423`*^9, 3.713017541586918*^9}, {
  3.713017694498664*^9, 3.713017696789795*^9}},
 CellID->504794705,ExpressionUUID->"c0a3564b-1433-4bfa-a8ae-37dd85dbc226"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"G", "(", "s", ")"}], "=", 
    RowBox[{
     SubscriptBox["K", 
      StyleBox["C",
       FontSlant->"Plain"]], " ", 
     SubscriptBox["K", 
      StyleBox["P",
       FontSlant->"Plain"]], " ", 
     SuperscriptBox["e", 
      RowBox[{
       RowBox[{"-", "\[Theta]"}], " ", "s"}]], " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        SubscriptBox["\[Tau]", 
         StyleBox["I",
          FontSlant->"Plain"]], " ", "s"}], "+", "1"}], ")"}], " ", 
     FractionBox[
      RowBox[{
       RowBox[{
        SubscriptBox["\[Tau]", 
         StyleBox["D",
          FontSlant->"Plain"]], " ", "s"}], "+", "1"}], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SubscriptBox["\[Tau]", 
           StyleBox["P",
            FontSlant->"Plain"]], " ", "s"}], "+", "1"}], ")"}], " ", 
       SubscriptBox["\[Tau]", 
        StyleBox["I",
         FontSlant->"Plain"]], " ", "s"}]]}]}], TraditionalForm]], 
  "InlineMath",
  InitializationCell->True,
  FormatType->"TraditionalForm",ExpressionUUID->
  "56bccc3c-82fe-4add-8b17-c828d0405a83"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.7130175122882423`*^9, 3.7130176108318787`*^9}},
 CellID->178838559,ExpressionUUID->"cdbe000e-fa2f-42cb-9dcb-165b61466516"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["K", 
    StyleBox["C",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "044c5dc3-9e43-402d-84f9-ac53457a1dcd"],
 " is controller gain, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["K", 
    StyleBox["P",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "0daed65c-6a68-46d0-b0df-28ae91160388"],
 " is process gain, ",
 Cell[BoxData[
  FormBox["\[Theta]", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "8a4d7f80-2870-4fe1-82e0-3b7f4186a4ad"],
 " is dead time, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Tau]", 
    StyleBox["I",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "e5fdc0dc-0afc-48e0-87ed-f585cb26a6b8"],
 " is the integral time constant, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Tau]", 
    StyleBox["D",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "e44f851b-97db-456c-89f3-8cbe5ca02f4d"],
 " is derivative time constant, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Tau]", 
    StyleBox["P",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "85b16b2e-b04e-431a-ae23-1cfdf3ac6bd7"],
 " is the process time constant, and ",
 Cell[BoxData[
  FormBox["s", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "9951e67f-356d-4627-a8cc-a86a11e0b365"],
 " is a Laplace domain variable."
}], "DetailNotes",
 CellChangeTimes->{{3.7130175122882423`*^9, 3.713017631818079*^9}, {
