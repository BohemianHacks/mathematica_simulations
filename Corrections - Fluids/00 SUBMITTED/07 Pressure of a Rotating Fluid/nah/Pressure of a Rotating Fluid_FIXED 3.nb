Cell[CellGroupData[{

Cell["", "DetailsSection"],

Cell[TextData[{
 "A fluid in a tank that rotates at a constant angular velocity about an axis \
(in this case about the ",
 Cell[BoxData[
  FormBox["z", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " axis), rotates with the tank as a rigid body. The pressure gradient is \
written in cylindrical coordinates:"
}], "DetailNotes",
 CellChangeTimes->{{3.687275455996868*^9, 3.6872755151045794`*^9}, {
  3.6872755480371647`*^9, 3.6872755626142373`*^9}},
 CellID->123171918],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Del]", "P"}], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"\[PartialD]", "P"}], 
       RowBox[{"\[PartialD]", "r"}]], " ", 
      SubscriptBox[
       OverscriptBox[
        StyleBox["e",
         FontSlant->"Plain"], "\[Wedge]"], "r"]}], "+", 
     RowBox[{
      FractionBox["1", "r"], " ", 
      FractionBox[
       RowBox[{"\[PartialD]", "P"}], 
       RowBox[{"\[PartialD]", "\[Theta]"}]], " ", 
      SubscriptBox[
       OverscriptBox[
        StyleBox["e",
         FontSlant->"Plain"], "\[Wedge]"], "\[Theta]"]}], "+", 
     RowBox[{
      FractionBox[
       RowBox[{"\[PartialD]", "P"}], 
       RowBox[{"\[PartialD]", "z"}]], " ", 
      SubscriptBox[
       OverscriptBox[
        StyleBox["e",
         FontSlant->"Plain"], "\[Wedge]"], "z"]}]}]}], TraditionalForm]], 
  "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.687275455996868*^9, 3.6872755151045794`*^9}, {
  3.6872755480371647`*^9, 3.6872757191112566`*^9}, {3.6872758182123566`*^9, 
  3.6872758466864843`*^9}, {3.687278265948245*^9, 3.6872782662290454`*^9}, {
  3.6872790571815405`*^9, 3.687279061208564*^9}},
 CellID->426185719],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"\[PartialD]", "P"}], 
     RowBox[{"\[PartialD]", "r"}]], "=", 
    RowBox[{"\[Rho]", " ", "r", " ", 
     SuperscriptBox["\[Omega]", "2"]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6872781740763807`*^9, 3.687278211986577*^9}},
 CellID->420100417],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"\[PartialD]", "P"}], 
     RowBox[{"\[PartialD]", "\[Theta]"}]], "=", "0"}], TraditionalForm]], 
  "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6872781740763807`*^9, 3.687278227475051*^9}},
 CellID->811534117],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"\[PartialD]", "P"}], 
     RowBox[{"\[PartialD]", "z"}]], "=", 
    RowBox[{"-", "\[Gamma]"}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.6872781740763807`*^9, 3.6872782439361277`*^9}},
 CellID->455507795],

Cell[TextData[{
 "For this type of rigid body rotation, pressure is a function of ",
 Cell[BoxData[
  FormBox["r", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " and ",
 Cell[BoxData[
  FormBox["z", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ":"
}], "DetailNotes",
 CellChangeTimes->{{3.6872782701370525`*^9, 3.6872782701370525`*^9}, {
  3.6872783251661263`*^9, 3.687278341233034*^9}},
 CellID->358170624],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[DifferentialD]", "P"}], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"\[PartialD]", "P"}], 
       RowBox[{"\[PartialD]", "r"}]], " ", 
      RowBox[{"\[DifferentialD]", "r"}]}], "+", 
     RowBox[{
      FractionBox[
       RowBox[{"\[PartialD]", "P"}], 
       RowBox[{"\[PartialD]", "z"}]], " ", 
      RowBox[{"\[DifferentialD]", "z"}]}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6872782701370525`*^9, 3.6872782701370525`*^9}, {
  3.6872783251661263`*^9, 3.6872783830096407`*^9}},
 CellID->322040285],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[DifferentialD]", "P"}], "=", 
    RowBox[{
     RowBox[{"\[Rho]", " ", "r", " ", 
      SuperscriptBox["\[Omega]", "2"], " ", 
      RowBox[{"\[DifferentialD]", "r"}]}], "-", 
     RowBox[{"\[Gamma]", " ", 
      RowBox[{"\[DifferentialD]", "z"}]}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.6872782701370525`*^9, 3.6872782701370525`*^9}, {
  3.6872783251661263`*^9, 3.6872784068615613`*^9}, {3.6872784615404377`*^9, 
  3.687278461914839*^9}},
 CellID->140496950],

Cell["\<\
Integrating the last equation yields the pressure distribution:\
\>", "DetailNotes",
 CellChangeTimes->{{3.6872782701370525`*^9, 3.6872782701370525`*^9}, {
  3.6872783251661263`*^9, 3.6872784068615613`*^9}, {3.6872784615404377`*^9, 
  3.687278461914839*^9}, {3.6872788493450675`*^9, 3.6872788595633183`*^9}, {
  3.6872789005649195`*^9, 3.6872789226152377`*^9}},
 CellID->111021026],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Integral]", 
     RowBox[{"\[DifferentialD]", "P"}]}], "=", 
    RowBox[{
     RowBox[{"\[Rho]", " ", 
      SuperscriptBox["\[Omega]", "2"], " ", 
      RowBox[{"\[Integral]", 
       RowBox[{"r", " ", 
        RowBox[{"\[DifferentialD]", "r"}]}]}]}], "-", 
     RowBox[{"\[Gamma]", " ", 
      RowBox[{"\[Integral]", 
       RowBox[{"\[DifferentialD]", "z"}]}]}]}]}], TraditionalForm]], 
  "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6872782701370525`*^9, 3.6872782701370525`*^9}, {
  3.6872783251661263`*^9, 3.6872784068615613`*^9}, {3.6872784615404377`*^9, 
  3.687278461914839*^9}, {3.6872788493450675`*^9, 3.6872788595633183`*^9}, {
  3.6872789005649195`*^9, 3.687278954088606*^9}},
 CellID->106402711],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     FractionBox[
      RowBox[{"\[Rho]", " ", 
       SuperscriptBox["\[Omega]", "2"], " ", 
       SuperscriptBox["r", "2"]}], "2"], "-", 
     RowBox[{"\[Gamma]", " ", "z"}], "+", "C"}]}], TraditionalForm]], 
  "InlineMath",
  FormatType->"TraditionalForm"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.6872782701370525`*^9, 3.6872782701370525`*^9}, {
  3.6872783251661263`*^9, 3.6872784068615613`*^9}, {3.6872784615404377`*^9, 
  3.687278461914839*^9}, {3.6872788493450675`*^9, 3.6872788595633183`*^9}, {
  3.6872789005649195`*^9, 3.687278980570732*^9}},
 CellID->452638524],

Cell[TextData[{
 "Where ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is the pressure in the fluid (Pa), ",
 Cell[BoxData[
  FormBox["r", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is the radial distance from the center (m), ",
 Cell[BoxData[
  FormBox["z", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is distance from the tank bottom (m), ",
 Cell[BoxData[
  FormBox["\[Rho]", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is fluid density (",
 Cell[BoxData[
  FormBox[
   RowBox[{"kg", "/", 
    SuperscriptBox[
     StyleBox["m",
      FontSlant->"Plain"], "3"]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 "), ",
 Cell[BoxData[
  FormBox["\[Omega]", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is angular velocity (rad/s), ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Gamma]", "=", 
    RowBox[{"\[Rho]", " ", "g"}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is specific weight (",
 Cell[BoxData[
  FormBox[
   RowBox[{"kg", "/", 
    RowBox[{"[", 
     RowBox[{
      SuperscriptBox[
       StyleBox["m",
        FontSlant->"Plain"], "2"], " ", 
      SuperscriptBox[
       StyleBox["s",
        FontSlant->"Plain"], "2"]}], "]"}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 "), ",
 Cell[BoxData[
  FormBox[
   RowBox[{"g", "=", 
    RowBox[{"9.81", " ", 
     RowBox[{
      StyleBox["m",
       FontSlant->"Plain"], "/", 
      SuperscriptBox[
       StyleBox["s",
        FontSlant->"Plain"], "2"]}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is acceleration due to gravity, ",
 Cell[BoxData[
  FormBox["C", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is a constant estimated as ",
 Cell[BoxData[
  FormBox[
   RowBox[{"C", "=", 
    RowBox[{"\[Gamma]", " ", "h"}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " (",
 Cell[BoxData[
  FormBox[
   RowBox[{"kg", "/", 
    RowBox[{"[", 
     RowBox[{
      StyleBox["m",
       FontSlant->"Plain"], " ", 
      SuperscriptBox[
       StyleBox["s",
        FontSlant->"Plain"], "2"]}], "]"}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 "), where ",
 Cell[BoxData[
  FormBox["h", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is fluid height (m)."
}], "DetailNotes",
 CellChangeTimes->{{3.6872793788195753`*^9, 3.6872794231758537`*^9}, 
   3.687279571067232*^9},
 CellID->471686978]
}, Open  ]],

