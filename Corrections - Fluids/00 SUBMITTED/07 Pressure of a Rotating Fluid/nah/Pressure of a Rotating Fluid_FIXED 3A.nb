Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[DifferentialD]", "P"}], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"\[PartialD]", "P"}], 
       RowBox[{"\[PartialD]", "r"}]], " ", 
      RowBox[{"\[DifferentialD]", "r"}]}], "+", 
     RowBox[{
      FractionBox[
       RowBox[{"\[PartialD]", "P"}], 
       RowBox[{"\[PartialD]", "z"}]], " ", 
      RowBox[{"\[DifferentialD]", "z"}]}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
