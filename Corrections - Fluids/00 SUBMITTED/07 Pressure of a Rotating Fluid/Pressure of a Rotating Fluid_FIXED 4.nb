  3.687622816431323*^9, 3.6876228451576724`*^9}, {3.687622903826971*^9, 
  3.6876229207022824`*^9}, {3.6876229615124807`*^9, 3.6876229727377324`*^9}},
 CellID->750710596],

Cell[TextData[{
 "since ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Gamma]", "=", 
    RowBox[{"\[Rho]", " ", "g"}]}], TraditionalForm]], "InlineMath"],
 ", the equation is simplified to:"
}], "DetailNotes",
 CellChangeTimes->{{3.6872782701370525`*^9, 3.6872782701370525`*^9}, {
  3.6872783251661263`*^9, 3.6872784068615613`*^9}, {3.6872784615404377`*^9, 
  3.687278461914839*^9}, {3.6872788493450675`*^9, 3.6872788595633183`*^9}, {
  3.6872789005649195`*^9, 3.687278980570732*^9}, {3.687622485866288*^9, 
  3.6876224955987525`*^9}, {3.687622595681178*^9, 3.687622775120308*^9}, {
  3.687622816431323*^9, 3.6876228451576724`*^9}, {3.687622903826971*^9, 
  3.6876229417665825`*^9}},
 CellID->41781344],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"z", "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       SuperscriptBox["\[Omega]", "2"], " ", 
       SuperscriptBox["r", "2"]}], 
      RowBox[{"2", " ", "g"}]], "+", "C"}]}], TraditionalForm]], "InlineMath"],
 "."
}], "DetailNotes",
 CellChangeTimes->{{3.6872782701370525`*^9, 3.6872782701370525`*^9}, {
  3.6872783251661263`*^9, 3.6872784068615613`*^9}, {3.6872784615404377`*^9, 
  3.687278461914839*^9}, {3.6872788493450675`*^9, 3.6872788595633183`*^9}, {
  3.6872789005649195`*^9, 3.687278980570732*^9}, {3.687622485866288*^9, 
  3.6876224955987525`*^9}, {3.687622595681178*^9, 3.687622775120308*^9}, {
  3.687622816431323*^9, 3.6876228451576724`*^9}, {3.687622903826971*^9, 
  3.687622975380353*^9}},
 CellID->8488916],

Cell[TextData[{
 "Where ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath"],
 " is the pressure in the fluid (Pa), ",
 Cell[BoxData[
  FormBox["r", TraditionalForm]], "InlineMath"],
 " is the radial distance from the center (m), ",
 Cell[BoxData[
  FormBox["z", TraditionalForm]], "InlineMath"],
 " is distance from the tank bottom (m), ",
 Cell[BoxData[
  FormBox["\[Rho]", TraditionalForm]], "InlineMath"],
 " is fluid density (",
 Cell[BoxData[
  FormBox[
   RowBox[{"kg", "/", 
    SuperscriptBox[
     StyleBox["m",
      FontSlant->"Plain"], "3"]}], TraditionalForm]], "InlineMath"],
 "), ",
 Cell[BoxData[
  FormBox["\[Omega]", TraditionalForm]], "InlineMath"],
 " is angular velocity (rad/s), ",
 Cell[BoxData[
  FormBox["\[Gamma]", TraditionalForm]], "InlineMath"],
 " is specific weight (",
 Cell[BoxData[
  FormBox[
   RowBox[{"kg", "/", 
    RowBox[{"[", 
     RowBox[{
      SuperscriptBox[
       StyleBox["m",
        FontSlant->"Plain"], "2"], " ", 
      SuperscriptBox[
       StyleBox["s",
        FontSlant->"Plain"], "2"]}], "]"}]}], TraditionalForm]], "InlineMath"],
 "), ",
 Cell[BoxData[
  FormBox[
   RowBox[{"g", "=", 
    RowBox[{"9.8", " ", 
     RowBox[{
      StyleBox["m",
       FontSlant->"Plain"], "/", 
      SuperscriptBox[
       StyleBox["s",
        FontSlant->"Plain"], "2"]}]}]}], TraditionalForm]], "InlineMath"],
 " is acceleration due to gravity, ",
 Cell[BoxData[
  FormBox["C", TraditionalForm]], "InlineMath"],
 " is a constant estimated as ",
 Cell[BoxData[
  FormBox[
   RowBox[{"C", "=", 
    RowBox[{"\[Gamma]", " ", "h"}]}], TraditionalForm]], "InlineMath"],
 " (",
 Cell[BoxData[
  FormBox[
   RowBox[{"kg", "/", 
    RowBox[{"[", 
     RowBox[{
      StyleBox["m",
       FontSlant->"Plain"], " ", 
      SuperscriptBox[
       StyleBox["s",
        FontSlant->"Plain"], "2"]}], "]"}]}], TraditionalForm]], "InlineMath"],
 "), where ",
 Cell[BoxData[
  FormBox["h", TraditionalForm]], "InlineMath"],
 " is fluid height (m)."
}], "DetailNotes",
 CellChangeTimes->{{3.6872793788195753`*^9, 3.6872794231758537`*^9}, 
   3.687279571067232*^9, {3.687622583907709*^9, 3.687622583907709*^9}, {
   3.6876231091075273`*^9, 3.6876231091075273`*^9}},
 CellID->471686978],

