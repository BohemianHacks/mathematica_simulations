Cell[CellGroupData[{

Cell["", "DetailsSection"],

Cell["\<\
A force balance is done and the sum of the forces is zero so drag equals lift:\
\>", "DetailNotes",
 CellChangeTimes->{{3.685131747032401*^9, 3.685131779552475*^9}},
 CellID->46322902],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Sum]", 
     SubscriptBox["F", 
      StyleBox["x",
       FontSlant->"Plain"]]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["F", 
       StyleBox["D",
        FontSlant->"Plain"]], "-", 
      RowBox[{
       SubscriptBox["F", 
        StyleBox["T",
         FontSlant->"Plain"]], " ", "cos", " ", "\[Theta]"}]}], "=", "0"}]}], 
   TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.685131747032401*^9, 3.685131847796962*^9}},
 CellID->385431487],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Sum]", 
     SubscriptBox["F", 
      StyleBox["y",
       FontSlant->"Plain"]]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["F", 
       StyleBox["L",
        FontSlant->"Plain"]], "-", 
      RowBox[{"m", " ", "g"}], "+", 
      RowBox[{
       SubscriptBox["F", 
        StyleBox["T",
         FontSlant->"Plain"]], " ", "sin", " ", "\[Theta]"}]}], "=", "0"}]}], 
   TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.685131747032401*^9, 3.685131891965459*^9}, {
  3.68513210634039*^9, 3.6851321065743904`*^9}},
 CellID->380183373],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["F", 
    StyleBox["x",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["F", 
    StyleBox["y",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " are the forces in the the ",
 Cell[BoxData[
  FormBox[
   StyleBox["x",
    FontSlant->"Plain"], TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox[
   StyleBox["y",
    FontSlant->"Plain"], TraditionalForm]], "InlineMath"],
 " directions (N), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["F", 
    StyleBox["D",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " is drag (N), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["F", 
    StyleBox["T",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " is tension (N), ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["F", 
    StyleBox["L",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " is lift (N), ",
 Cell[BoxData[
  FormBox["m", TraditionalForm]], "InlineMath"],
 " is kite mass (kg), and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"g", "=", 
    RowBox[{"9.8", " ", 
     RowBox[{
      StyleBox["m",
       FontSlant->"Plain"], "/", 
      SuperscriptBox[
       StyleBox["s",
        FontSlant->"Plain"], "2"]}]}]}], TraditionalForm]], "InlineMath"],
 " is the gravitational constant."
}], "DetailNotes",
 CellChangeTimes->{{3.685131747032401*^9, 3.6851318791358347`*^9}, {
  3.685131910284292*^9, 3.6851320814677305`*^9}, {3.6851321175834103`*^9, 
  3.6851321175834103`*^9}, {3.6851323184037704`*^9, 3.685132330402407*^9}, {
  3.6869321972686257`*^9, 3.6869322012648544`*^9}},
 CellID->727029487],

Cell["The lift and drag coefficients are:", "DetailNotes",
 CellChangeTimes->{{3.685131747032401*^9, 3.6851318791358347`*^9}, {
  3.685131910284292*^9, 3.6851320814677305`*^9}, {3.6851321175834103`*^9, 
  3.685132137965064*^9}},
 CellID->117791710],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["C", 
     StyleBox["D",
      FontSlant->"Plain"]], "=", 
    FractionBox[
     RowBox[{"2", " ", 
      SubscriptBox["F", 
       StyleBox["D",
        FontSlant->"Plain"]]}], 
     RowBox[{"\[Rho]", " ", 
      SuperscriptBox["u", "2"], " ", "A"}]]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.685131747032401*^9, 3.6851318791358347`*^9}, {
  3.685131910284292*^9, 3.6851320814677305`*^9}, {3.6851321175834103`*^9, 
  3.6851321694575233`*^9}},
 CellID->52526115],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["C", 
     StyleBox["L",
      FontSlant->"Plain"]], "=", 
    FractionBox[
     RowBox[{"2", " ", 
      SubscriptBox["F", 
       StyleBox["L",
        FontSlant->"Plain"]]}], 
     RowBox[{"\[Rho]", " ", 
      SuperscriptBox["u", "2"], " ", "A"}]]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.685131747032401*^9, 3.6851318791358347`*^9}, {
  3.685131910284292*^9, 3.6851320814677305`*^9}, {3.6851321175834103`*^9, 
  3.68513218645485*^9}},
 CellID->6052825],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["\[Rho]", TraditionalForm]], "InlineMath"],
 " is air density (",
 Cell[BoxData[
  FormBox[
   RowBox[{"kg", "/", 
    SuperscriptBox[
     StyleBox["m",
      FontSlant->"Plain"], "3"]}], TraditionalForm]], "InlineMath"],
 "), ",
 Cell[BoxData[
  FormBox["u", TraditionalForm]], "InlineMath"],
 " is wind velocity (m/s), and ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]], "InlineMath"],
 " is the area of one side of the kite (",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    StyleBox["m",
     FontSlant->"Plain"], "2"], TraditionalForm]], "InlineMath"],
 ")."
}], "DetailNotes",
 CellChangeTimes->{{3.685131747032401*^9, 3.6851318791358347`*^9}, {
  3.685131910284292*^9, 3.6851320814677305`*^9}, {3.6851321175834103`*^9, 
  3.685132246347839*^9}, {3.685132288872117*^9, 3.685132295085928*^9}},
 CellID->18640183],

Cell[TextData[{
 "In this Demonstration the coefficients of lift and drag ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["C", 
    StyleBox["L",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["C", 
    StyleBox["D",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " are constant, which allows for the tension ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["F", 
    StyleBox["T",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " in the string to be solved for using the equations above."
}], "DetailNotes",
 CellChangeTimes->{{3.685131747032401*^9, 3.6851318791358347`*^9}, {
   3.685131910284292*^9, 3.6851320814677305`*^9}, {3.6851321175834103`*^9, 
   3.6851322659448743`*^9}, 3.6869339399483013`*^9},
 CellID->38256828]
}, Open  ]],

