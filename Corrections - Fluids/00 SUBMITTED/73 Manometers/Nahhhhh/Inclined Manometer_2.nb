Cell[CellGroupData[{

Cell["", "DetailsSection"],

Cell["\<\
A force balance is done on the manometer to determine the pressure change:\
\>", "DetailNotes",
 CellChangeTimes->{{3.6950510666449556`*^9, 3.6950510968609104`*^9}},
 CellID->25555540],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["P", 
      StyleBox["A",
       FontSlant->"Plain"]], "+", 
     RowBox[{
      SubscriptBox["\[Gamma]", "1"], " ", 
      SubscriptBox["h", "1"]}], "-", 
     RowBox[{
      SubscriptBox["\[Gamma]", "2"], " ", 
      SubscriptBox["h", "2"]}], "-", 
     RowBox[{
      SubscriptBox["\[Gamma]", "3"], " ", 
      SubscriptBox["h", "3"]}]}], "=", 
    SubscriptBox["P", 
     StyleBox["B",
      FontSlant->"Plain"]]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6950510666449556`*^9, 3.695051137305527*^9}},
 CellID->321428071],

Cell[TextData[{
 "the vertical height of fluid ",
 Cell[BoxData[
  FormBox["2", TraditionalForm]], "InlineMath"],
 " is written in terms of the length:"
}], "DetailNotes",
 CellChangeTimes->{{3.6950510666449556`*^9, 3.6950511911675406`*^9}},
 CellID->374735620],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["h", "2"], "=", 
    RowBox[{
     SubscriptBox["L", "2"], " ", "sin", " ", "\[Theta]"}]}], 
   TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6950510666449556`*^9, 3.69505120123239*^9}},
 CellID->100604771],

Cell["the change in pressures is:", "DetailNotes",
 CellChangeTimes->{{3.6950510666449556`*^9, 3.695051213783459*^9}, 
   3.6950516150043864`*^9},
 CellID->339292768],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", "P"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["P", 
       StyleBox["A",
        FontSlant->"Plain"]], "-", 
      SubscriptBox["P", 
       StyleBox["B",
        FontSlant->"Plain"]]}], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[Gamma]", "2"], " ", 
       SubscriptBox["L", "2"], " ", "sin", " ", "\[Theta]"}], "+", 
      RowBox[{
       SubscriptBox["\[Gamma]", "3"], " ", 
       SubscriptBox["h", "3"]}], "-", 
      RowBox[{
       SubscriptBox["\[Gamma]", "1"], " ", 
       SubscriptBox["h", "1"]}]}]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6951341915518374`*^9, 3.6951341929580555`*^9}, {
  3.695134400558427*^9, 3.6951344616753306`*^9}, {3.695134585576743*^9, 
  3.695134585888744*^9}},
 CellID->123594734],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath"],
 " is pressure, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Gamma]", "i"], "=", 
    RowBox[{
     SubscriptBox["SG", "i"], " ", 
     SubscriptBox["\[Rho]", 
      StyleBox["w",
       FontSlant->"Plain"]], " ", "g"}]}], TraditionalForm]], "InlineMath"],
 " is specific weight of fluid ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "=", 
    RowBox[{"(", 
     RowBox[{"1", ",", "2", ",", "3"}], ")"}]}], TraditionalForm]], 
  "InlineMath"],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["SG", "i"], TraditionalForm]], "InlineMath"],
 " is specific gravity, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Rho]", 
    StyleBox["w",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " is the density of water, ",
 Cell[BoxData[
  FormBox["g", TraditionalForm]], "InlineMath"],
 " is acceleration due to gravity, and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["h", "i"], TraditionalForm]], "InlineMath"],
 " is fluid height."
}], "DetailNotes",
 CellChangeTimes->{{3.695134532887277*^9, 3.6951345732196894`*^9}},
 CellID->455402193],

