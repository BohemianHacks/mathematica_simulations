Cell["", "DetailsSection"],

Cell["\<\
A force balance is done on the manometer to determine the pressure change:\
\>", "DetailNotes",
 CellChangeTimes->{{3.6950510666449556`*^9, 3.6950510968609104`*^9}},
 CellID->25555540],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["P", 
     StyleBox["g",
      FontSlant->"Plain"]], "=", 
    RowBox[{
     SubscriptBox["P", "atm"], "+", 
     RowBox[{"\[Gamma]", " ", "h"}]}]}], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6950510666449556`*^9, 3.695051137305527*^9}, {
  3.6972189201022778`*^9, 3.697218954814042*^9}, {3.6972220351948986`*^9, 
  3.697222039456719*^9}},
 CellID->321428071],

Cell["\<\
the vertical height of the fluid is written in terms of the length:\
\>", "DetailNotes",
 CellChangeTimes->{{3.6950510666449556`*^9, 3.6950511911675406`*^9}, {
  3.6972189589034567`*^9, 3.6972189631000795`*^9}},
 CellID->374735620],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"h", "=", 
    RowBox[{"L", " ", "sin", " ", "\[Theta]"}]}], TraditionalForm]], 
  "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6950510666449556`*^9, 3.69505120123239*^9}, {
  3.6972189647386856`*^9, 3.697218969122121*^9}},
 CellID->100604771],

Cell["the change in pressures is:", "DetailNotes",
 CellChangeTimes->{{3.6950510666449556`*^9, 3.695051213783459*^9}, 
   3.6950516150043864`*^9},
 CellID->339292768],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", "P"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["P", 
       StyleBox["g",
        FontSlant->"Plain"]], "-", 
      SubscriptBox["P", "atm"]}], "=", 
     RowBox[{
      RowBox[{"-", "\[Gamma]"}], " ", "L", " ", "sin", " ", "\[Theta]"}]}]}], 
   TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6951341915518374`*^9, 3.6951341929580555`*^9}, {
  3.695134400558427*^9, 3.6951344616753306`*^9}, {3.695134585576743*^9, 
  3.695134585888744*^9}, {3.697218976480765*^9, 3.6972189819569907`*^9}, {
  3.6972220423383274`*^9, 3.6972220511067753`*^9}},
 CellID->123594734],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath"],
 " is pressure, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Gamma]", "=", 
    RowBox[{"SG", " ", "\[Rho]", " ", "g"}]}], TraditionalForm]], 
  "InlineMath"],
 " is specific weight of the manometer fluid, ",
 Cell[BoxData[
  FormBox["SG", TraditionalForm]], "InlineMath"],
 " is specific gravity, ",
 Cell[BoxData[
  FormBox["\[Rho]", TraditionalForm]], "InlineMath"],
 " is the density of the manometer fluid, ",
 Cell[BoxData[
  FormBox["g", TraditionalForm]], "InlineMath"],
 " is acceleration due to gravity, and ",
 Cell[BoxData[
  FormBox["h", TraditionalForm]], "InlineMath"],
 " is fluid height."
}], "DetailNotes",
