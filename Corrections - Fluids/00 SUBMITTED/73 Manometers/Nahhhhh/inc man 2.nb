Cell[CellGroupData[{

Cell["", "DetailsSection"],

Cell["\<\
A force balance is done on the manometer to determine the pressure change:\
\>", "DetailNotes",
 CellChangeTimes->{{3.6950510666449556`*^9, 3.6950510968609104`*^9}},
 CellID->25555540],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["P", 
      StyleBox["A",
       FontSlant->"Plain"]], "+", 
     RowBox[{
      SubscriptBox["\[Gamma]", "1"], " ", 
      SubscriptBox["h", "1"]}], "-", 
     RowBox[{
      SubscriptBox["\[Gamma]", "2"], " ", 
      SubscriptBox["h", "2"]}], "-", 
     RowBox[{
      SubscriptBox["\[Gamma]", "3"], " ", 
      SubscriptBox["h", "3"]}]}], "=", 
    SubscriptBox["P", 
     StyleBox["B",
      FontSlant->"Plain"]]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6950510666449556`*^9, 3.695051137305527*^9}},
 CellID->321428071],

Cell[TextData[{
 "the vertical height of fluid ",
 Cell[BoxData[
  FormBox["2", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is written in terms of the length:"
}], "DetailNotes",
 CellChangeTimes->{{3.6950510666449556`*^9, 3.6950511911675406`*^9}},
 CellID->374735620],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["h", "2"], "=", 
    RowBox[{
     SubscriptBox["L", "2"], " ", "sin", " ", "\[Theta]"}]}], 
   TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6950510666449556`*^9, 3.69505120123239*^9}},
 CellID->100604771],

Cell["the change in pressures is:", "DetailNotes",
 CellChangeTimes->{{3.6950510666449556`*^9, 3.695051213783459*^9}, 
   3.6950516150043864`*^9},
 CellID->339292768],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", "P"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["P", 
       StyleBox["A",
        FontSlant->"Plain"]], "-", 
      SubscriptBox["P", 
       StyleBox["B",
        FontSlant->"Plain"]]}], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[Gamma]", "2"], " ", 
       SubscriptBox["L", "2"], " ", "sin", " ", "\[Theta]"}], "+", 
      RowBox[{
       SubscriptBox["\[Gamma]", "3"], " ", 
       SubscriptBox["h", "3"]}], "-", 
      RowBox[{
       SubscriptBox["\[Gamma]", "1"], " ", 
       SubscriptBox["h", "1"]}]}]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6950510666449556`*^9, 3.6950512701669416`*^9}},
 CellID->449034477],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is pressure, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Gamma]", "i"], "=", 
    RowBox[{
     SubscriptBox["\[Rho]", "i"], " ", "g"}]}], TraditionalForm]], 
  "InlineMath",
  FormatType->"TraditionalForm"],
 " is specific weight of fluid ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "=", 
    RowBox[{"(", 
     RowBox[{"1", ",", "2", ",", "3"}], ")"}]}], TraditionalForm]], 
  "InlineMath",
  FormatType->"TraditionalForm"],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Rho]", "i"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is fluid density, ",
 Cell[BoxData[
  FormBox["g", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is acceleration due to gravity, and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["h", "i"], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm"],
 " is fluid height."
}], "DetailNotes",
 CellChangeTimes->{{3.6950510666449556`*^9, 3.6950512952668867`*^9}, {
  3.6950515034141607`*^9, 3.695051600279512*^9}, {3.6950516383039045`*^9, 
  3.6950516399271135`*^9}},
 CellID->13030594],

