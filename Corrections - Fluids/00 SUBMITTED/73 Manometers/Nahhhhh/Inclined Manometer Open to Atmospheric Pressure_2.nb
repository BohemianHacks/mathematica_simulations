 CellChangeTimes->{{3.6950510666449556`*^9, 3.695051213783459*^9}, 
   3.6950516150043864`*^9},
 CellID->339292768],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[CapitalDelta]", "\[InvisibleSpace]", "P"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["P", 
       StyleBox["g",
        FontSlant->"Plain"]], "-", 
      SubscriptBox["P", "atm"]}], "=", 
     RowBox[{"\[Gamma]", " ", "L", " ", "sin", " ", "\[Theta]"}]}]}], 
   TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6951341915518374`*^9, 3.6951341929580555`*^9}, {
  3.695134400558427*^9, 3.6951344616753306`*^9}, {3.695134585576743*^9, 
  3.695134585888744*^9}, {3.697218976480765*^9, 3.6972189819569907`*^9}, {
  3.6972220423383274`*^9, 3.6972220511067753`*^9}, {3.697293263719906*^9, 
  3.697293263719906*^9}},
 CellID->123594734],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath"],
 " is pressure, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Gamma]", "=", 
    RowBox[{"SG", " ", 
     SubscriptBox["\[Rho]", 
      StyleBox["w",
       FontSlant->"Plain"]], " ", "g"}]}], TraditionalForm]], "InlineMath"],
 " is specific weight, ",
 Cell[BoxData[
  FormBox["SG", TraditionalForm]], "InlineMath"],
 " is the specific gravity of the manometer fluid, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Rho]", 
    StyleBox["w",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath"],
 " is the density of water, ",
 Cell[BoxData[
  FormBox["g", TraditionalForm]], "InlineMath"],
 " is acceleration due to gravity, and ",
 Cell[BoxData[
  FormBox["h", TraditionalForm]], "InlineMath"],
 " is fluid height."
}], "DetailNotes",
 CellChangeTimes->{{3.695134532887277*^9, 3.6951345732196894`*^9}, {
