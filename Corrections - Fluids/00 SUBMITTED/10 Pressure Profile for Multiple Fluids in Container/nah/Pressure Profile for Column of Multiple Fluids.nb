Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath"],
 " is the hydrostatic gage pressure (kPa), ",
 Cell[BoxData[
  FormBox["\[Rho]", TraditionalForm]], "InlineMath"],
 " is the fluid density (",
 Cell[BoxData[
  FormBox[
   RowBox[{"kg", "/", 
    SuperscriptBox[
     StyleBox["m",
      FontSlant->"Plain"], "3"]}], TraditionalForm]], "InlineMath"],
 "), ",
 Cell[BoxData[
  FormBox["g", TraditionalForm]], "InlineMath"],
 " is the gravitational constant (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["9.8",
     FontSlant->"Plain"], 
    StyleBox[" ",
     FontSlant->"Plain"], 
    RowBox[{
     StyleBox["m",
      FontSlant->"Plain"], "/", 
     SuperscriptBox[
      StyleBox["s",
       FontSlant->"Plain"], "2"]}]}], TraditionalForm]], "InlineMath"],
 "), and ",
 Cell[BoxData[
  FormBox["h", TraditionalForm]], "InlineMath"],
 " is the height of the fluid (",
 Cell[BoxData[
  FormBox[
   StyleBox["m",
    FontSlant->"Plain"], TraditionalForm]], "InlineMath"],
 "). "
}], "DetailNotes",
 CellChangeTimes->{{3.593113589529297*^9, 3.593113589530096*^9}, {
