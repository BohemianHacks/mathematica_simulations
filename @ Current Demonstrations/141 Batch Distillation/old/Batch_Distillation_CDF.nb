Cell["DETAILS", "DetailsSection",ExpressionUUID->"cd92d376-b5a9-4e2f-afa7-e392be73c809"],

Cell[TextData[{
 StyleBox["In this batch distillation of a binary mixture, a fixed amount of \
the feed mixture ",
  FontSize->12],
 Cell[BoxData[
  FormBox[
   RowBox[{"F", " ", 
    RowBox[{"(", "mol", ")"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"d3034b42-9377-4b97-8bf1-b462c9dd2c3f"],
 StyleBox[", with initial mole fraction ",
  FontSize->12],
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "F"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "ce076430-f351-41f1-a663-f68b30cffd51"],
 StyleBox[", evaporates into a distillate collection flask (where ",
  FontSize->12],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["D", "i"], " ", 
    RowBox[{"(", "mol", ")"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"25849755-5851-4f7c-9260-5dadc289dfa6"],
 StyleBox[" is the mass of distillate collected into flask ",
  FontSize->12],
 Cell[BoxData[
  FormBox["i", TraditionalForm]], "InlineMath",ExpressionUUID->
  "aeb7b579-75c0-4325-905b-e4a3a69067bd"],
 StyleBox[", and ",
  FontSize->12],
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", 
    RowBox[{"D", ",", " ", "i"}]], TraditionalForm]], "InlineMath",
  ExpressionUUID->"3288643e-79a6-470a-b866-4328fc23d2bf"],
 StyleBox[" is the mole fraction within distillate flask ",
  FontSize->12],
 Cell[BoxData[
  FormBox["i", TraditionalForm]], "InlineMath",ExpressionUUID->
  "072e5ac4-ea3e-4bd7-a28a-155d52e48e29"],
 StyleBox["). This process is repeated to fill several distillate collection \
flasks. The overall mass balance and component balance for the more volatile \
liquid are:\n",
  FontSize->12],
 StyleBox[" ",
  FontSize->4],
 StyleBox["\n",
  FontSize->12],
 Cell[BoxData[
  FormBox[
   RowBox[{"F", " ", "=", " ", 
    RowBox[{
     SubscriptBox["B", "final"], " ", "+", " ", 
     SubscriptBox["D", "total"]}]}], TraditionalForm]], "InlineMath",
  FontSize->14,ExpressionUUID->"f12629fd-f87a-4358-b3c2-d75b17a4c5a8"],
 StyleBox["\t",
  FontSize->14],
 StyleBox["\t\t\t\t\t[1]\n",
  FontSize->12],
 StyleBox[" ",
  FontSize->4],
 StyleBox["\n",
  FontSize->12],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"F", " ", 
     SubscriptBox["x", "F"]}], " ", "=", " ", 
    RowBox[{
     RowBox[{
      SubscriptBox["B", "final"], 
      SubscriptBox["x", 
       RowBox[{"B", ",", " ", "final"}]]}], " ", "+", " ", 
     RowBox[{
      SubscriptBox["D", "total"], 
      SubscriptBox["x", 
       RowBox[{"D", ",", " ", "avg"}]]}]}]}], TraditionalForm]], "InlineMath",
  FontSize->14,ExpressionUUID->"7c3b214b-1d9b-4eb5-a1de-85ed914060ab"],
 StyleBox["\t",
  FontSize->14],
 StyleBox["\t\t\t[2]\n",
  FontSize->12],
 StyleBox[" ",
  FontSize->4],
 StyleBox["\nwhere ",
  FontSize->12],
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", 
    RowBox[{"D", ",", " ", "avg"}]], TraditionalForm]], "InlineMath",
  ExpressionUUID->"969ae487-7b47-40e8-b2fe-5c6003a3a10e"],
 StyleBox[" is the average mole fraction within the distillate collection \
flask(s), and ",
  FontSize->12],
 Cell[BoxData[
  FormBox[
   SubscriptBox["B", "final"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "54e9d5be-0bb3-4ae4-a634-d769f91987a0"],
 StyleBox[" and ",
  FontSize->12],
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", 
    RowBox[{"B", ",", " ", "final"}]], TraditionalForm]], "InlineMath",
  ExpressionUUID->"5c462936-30c0-4906-8de4-d91b2fd6516f"],
 StyleBox[" are the final mass and mole fraction within the bottom (boiler) \
vessel. The value of ",
  FontSize->12],
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", 
    RowBox[{"D", ",", " ", "avg"}]], TraditionalForm]], "InlineMath",
  ExpressionUUID->"33b72cbe-7ea8-4fce-8e8d-e8a2a6481d15"],
 StyleBox[" can be calculated as:\n",
  FontSize->12],
 StyleBox[" ",
  FontSize->4],
 StyleBox["\n",
  FontSize->12],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", 
     RowBox[{"D", ",", " ", "avg"}]], " ", "=", " ", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"i", "=", "1"}], "n",
      LimitsPositioning->False], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{
           SubscriptBox["D", "i"], 
           SubscriptBox["x", 
            RowBox[{"D", ",", " ", "i"}]]}], " ", "+", " ", 
          RowBox[{
           SubscriptBox["D", 
            RowBox[{"i", "+", "1"}]], 
           SubscriptBox["x", 
            RowBox[{"D", ",", " ", 
             RowBox[{"i", "+", "1"}]}]]}]}], " ", "..."}], " ", 
        SubscriptBox["D", "n"], 
        SubscriptBox["x", 
         RowBox[{"D", ",", " ", "n"}]]}], ")"}], "/", 
      SubscriptBox["D", "total"]}]}]}], TraditionalForm]], "InlineMath",
  FontSize->14,ExpressionUUID->"6302812c-e869-4f98-a378-cfbd6ece3b6d"],
 StyleBox["\t",
  FontSize->14],
 StyleBox["[3]\n",
  FontSize->12],
 StyleBox[" ",
  FontSize->4],
 StyleBox["\nWhere the total number of collection stages (equivalent to the \
total number of distillate collection flasks) is ",
  FontSize->12],
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath",ExpressionUUID->
  "c68c3cb6-18d4-4d38-99e1-4ae3a09219e6"],
 StyleBox[". Total distillate is:\n",
  FontSize->12],
 StyleBox[" ",
  FontSize->4],
 StyleBox["\n",
  FontSize->12],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["D", "total"], " ", "=", " ", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"i", "=", "1"}], "n",
         LimitsPositioning->False], 
        SubscriptBox["D", "i"]}], "+", 
       SubscriptBox["D", 
        RowBox[{"i", "+", "1"}]]}], "..."}], " ", 
     SubscriptBox["D", "n"]}]}], TraditionalForm]], "InlineMath",
  FontSize->14,ExpressionUUID->"77116d09-4c17-4442-bb15-0dce6c067ba5"],
 StyleBox["\t",
  FontSize->14],
 StyleBox["\t\t\t\t[4]\n",
  FontSize->12],
 StyleBox[" ",
  FontSize->4],
 StyleBox["\nBecause the saturated vapor is in thermodynamic equilibrium with \
the saturated liquid in the vessel, the composition of the vapor ",
  FontSize->12],
 Cell[BoxData[
  FormBox[
   SubscriptBox["y", "B"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "83d59d9f-915c-45c4-90a8-c830234d4816"],
 StyleBox[" is a function of the composition in the bottom vessel ( ",
  FontSize->12],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", "B"], " ", "=", " ", 
    RowBox[{"f", "(", 
     SubscriptBox["x", "B"], ")"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"bef39100-152e-44fe-ba9f-5e67556ccf13"],
 StyleBox[" ). During evaporation, both compositions change with time, except \
when the composition of the liquid is an azeotrope. Numerical approximations \
like Antoine\[CloseCurlyQuote]s Equation are often used to model ",
  FontSize->12],
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "(", 
    SubscriptBox["x", "B"], ")"}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"358314f5-2f35-414d-b0bf-1898c50659c7"],
 StyleBox[" using experimental data. Other methods, such as assuming constant \
relative volatility, may also be used to approximate this relationship. \
Composition of the next collection flask ",
  FontSize->12],
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", 
    RowBox[{"B", ",", " ", 
     RowBox[{"i", "+", "1"}]}]], TraditionalForm]], "InlineMath",
  ExpressionUUID->"430e6dbc-49a4-4dea-9797-9d5c54343e9b"],
 StyleBox[" can be calculated using integration:\n",
  FontSize->12],
 StyleBox[" ",
  FontSize->4],
 StyleBox["\n",
  FontSize->12],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      SubscriptBox["B", "i"], 
      SubscriptBox["B", 
       RowBox[{"i", "+", "1"}]]], 
     RowBox[{
      FractionBox["1", "B"], 
      StyleBox[
       RowBox[{
        StyleBox["d",
         FontSlant->"Plain"], 
        StyleBox["B",
         FontSlant->"Italic"]}]]}]}], "=", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      SubscriptBox["x", 
       RowBox[{"B", ",", " ", "i"}]], 
      SubscriptBox["x", 
       RowBox[{"B", ",", " ", 
        RowBox[{"i", "+", "1"}]}]]], 
     RowBox[{
      FractionBox["1", 
       RowBox[{
        SubscriptBox["y", "B"], " ", "-", " ", 
        SubscriptBox["x", "B"]}]], 
      SubscriptBox[
       StyleBox[
        RowBox[{
         StyleBox["d",
          FontSlant->"Plain"], "x"}]], "B"]}]}]}], TraditionalForm]], 
  "InlineMath",
  FontSize->14,ExpressionUUID->"9359ec7a-f31c-4052-8e77-67fcf1a4373e"],
 StyleBox["\t\t",
  FontSize->14],
 StyleBox["\t\t\t[5]\n",
  FontSize->12],
 StyleBox[" ",
  FontSize->4],
 StyleBox["\nA mass balance can be used to solve for the distillate \
composition, ",
  FontSize->12],
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", 
    RowBox[{"D", ",", " ", 
     RowBox[{"i", "+", "1"}]}]], TraditionalForm]], "InlineMath",
  ExpressionUUID->"7a8f96a2-89f4-4aac-aeb6-2e0d79e580e4"],
 StyleBox[".\n",
  FontSize->12],
 StyleBox[" ",
  FontSize->4],
 StyleBox["\n",
  FontSize->12],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", 
     RowBox[{"D", ",", " ", 
      RowBox[{"i", "+", "1"}]}]], "=", 
    FractionBox[
     RowBox[{
      RowBox[{
       SubscriptBox["B", "i"], 
       SubscriptBox["x", 
        RowBox[{"B", ",", "i"}]]}], "-", 
      RowBox[{
       SubscriptBox["B", 
        RowBox[{"i", "+", "1"}]], 
       SubscriptBox["x", 
        RowBox[{"B", ",", 
         RowBox[{"i", "+", "1"}]}]]}]}], 
     RowBox[{
      SubscriptBox["B", "i"], "-", 
      SubscriptBox["B", 
       RowBox[{"i", "+", "1"}]]}]]}], TraditionalForm]], "InlineMath",
  FontSize->16,ExpressionUUID->"8172a9ff-09b6-42ba-8cfb-ab8c3ed441a2"],
 StyleBox["\t",
  FontSize->16],
