Cell[TextData[{
 "Finally, compositions ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", 
    RowBox[{"B", ",", " ", 
     RowBox[{"i", "+", "1"}]}]], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
