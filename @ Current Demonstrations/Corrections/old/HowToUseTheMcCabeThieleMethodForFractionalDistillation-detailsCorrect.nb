  RowBox[{"y", "=", 
   RowBox[{
    RowBox[{
     FractionBox["q", 
      RowBox[{"q", "-", "1"}]], " ", "x"}], "-", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"q", "-", "1"}]], " ", 
     SubscriptBox["z", 
      StyleBox["F",
       FontSlant->"Plain"]]}]}]}], 
