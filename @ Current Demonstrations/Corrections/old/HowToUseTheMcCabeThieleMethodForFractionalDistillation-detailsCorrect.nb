 CellID->44996256,ExpressionUUID->"4d0ad3a2-3c4d-427f-a72d-78430ec67e5b"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["F", TraditionalForm]], "InlineMath",ExpressionUUID->
  "3accd06d-be74-42fa-969f-b78c40cdcbb1"],
 " is the feed molar flow rate. The ",
 Cell[BoxData[
  FormBox["q", TraditionalForm]], "InlineMath",ExpressionUUID->
  "bbd28fa1-4627-4264-8ec3-a958ddc51011"],
 "-line can be derived from the following mass balance:"
}], "DetailNotes",
 CellChangeTimes->{{3.6896189035163565`*^9, 3.6896189503327847`*^9}, {
  3.689618983265969*^9, 3.689619086967473*^9}, {3.689619366435804*^9, 
  3.6896194164144516`*^9}, {3.691267808140308*^9, 3.6912678088405066`*^9}, {
  3.694812150774469*^9, 3.69481215333322*^9}, {3.777666030231905*^9, 
  3.7776660481731243`*^9}, {3.777666317059399*^9, 3.777666333430324*^9}, {
  3.777916330672723*^9, 3.7779163311621685`*^9}},
 CellID->2150528,ExpressionUUID->"9faa12f5-fdfe-4d2c-9bc1-153c2e2d98cf"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"y", " ", 
    RowBox[{"(", 
     RowBox[{"V", "-", 
      OverscriptBox["V", "_"]}], ")"}]}], "=", 
   RowBox[{
    RowBox[{"x", " ", 
     RowBox[{"(", 
      RowBox[{"L", "-", 
       OverscriptBox["L", "_"]}], ")"}]}], "+", 
    RowBox[{
     SubscriptBox["x", 
      StyleBox["D",
       FontSlant->"Plain"]], " ", "D"}], "+", 
    RowBox[{
     SubscriptBox["x", 
      StyleBox["B",
       FontSlant->"Plain"]], " ", "B"}]}]}], 
  TraditionalForm]], \
"InlineMath",ExpressionUUID->"014a35a8-d9c7-4f4e-8b46-b4bae05b7f4a"]], \
"DetailNotes",
 CellChangeTimes->{{3.6896189035163565`*^9, 3.6896189503327847`*^9}, {
   3.689618983265969*^9, 3.689619086967473*^9}, {3.689619366435804*^9, 
   3.689619473340741*^9}, {3.777666339176493*^9, 3.777666339177494*^9}, 
   3.777666372848166*^9},
 CellID->159732261,ExpressionUUID->"21307744-526c-4b15-8cd1-9742e4b5d98c"],

Cell[TextData[{
 "By combining an overall material balance around the column ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["z", 
      StyleBox["F",
       FontSlant->"Plain"]], " ", "F"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", 
       StyleBox["D",
        FontSlant->"Plain"]], " ", "D"}], "+", 
     RowBox[{
      SubscriptBox["x", 
       StyleBox["B",
        FontSlant->"Plain"]], " ", "B"}]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"5c7b4863-52e6-40f1-98f9-b446d5775d83"],
 " (where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["z", 
    StyleBox["F",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",ExpressionUUID->
  "1b16b3ff-0118-49f4-b1bb-8a3f8dac6cb7"],
 " is the mole fraction of methanol in the feed), a material balance around \
the feed stage ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"F", "+", "L", "+", 
     OverscriptBox["V", "_"]}], "=", 
    RowBox[{
     OverscriptBox["L", "_"], "+", "V"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"db8918a6-e54c-4927-bce1-4e902cc0e790"],
 ", and the definition of ",
 Cell[BoxData[
  FormBox["q", TraditionalForm]], "InlineMath",ExpressionUUID->
  "e16952ae-024f-491f-b453-10117ba58b03"],
 ", the ",
 Cell[BoxData[
  FormBox["q", TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "6bf0bd8b-61e6-4614-9c7c-507b7820e811"],
 "-line can be written in slope-intercept form:"
}], "DetailNotes",
 CellChangeTimes->{{3.6896189035163565`*^9, 3.6896189503327847`*^9}, {
  3.689618983265969*^9, 3.689619086967473*^9}, {3.689619366435804*^9, 
  3.689619473340741*^9}, {3.777666339846607*^9, 3.7776663523790812`*^9}, {
