  FormBox[
   RowBox[{"q", "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       OverscriptBox["L", "_"], "-", "L"}], "F"], "=", 
     RowBox[{"1", "+", 
      FractionBox[
       RowBox[{
        OverscriptBox["V", "_"], "-", "V"}], "F"]}]}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"fd761f4b-0c1d-4387-be0c-5b1c08123bbe"],
