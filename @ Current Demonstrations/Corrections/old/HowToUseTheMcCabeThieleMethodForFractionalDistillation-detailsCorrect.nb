  FormBox[
   RowBox[{
    SubscriptBox["z", 
     StyleBox["F",
      FontSlant->"Plain"]], "=", "0.5"}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"c087291a-6669-4f5f-ad41-50b4610655ad"],
