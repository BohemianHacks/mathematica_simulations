Cell["\<\
Isotherms and isobars are solved using the Van der Waals equation of state:\
\>", "DetailNotes",
 CellChangeTimes->{{3.6189412868763404`*^9, 3.618941329194761*^9}, 
   3.6781262861990633`*^9, {3.6785549741972027`*^9, 3.6785549780198565`*^9}},
 CellID->60133961,ExpressionUUID->"6ec61244-45e6-476b-9ca0-c18fd24d3946"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"P", " ", "=", 
    RowBox[{
     FractionBox[
      RowBox[{" ", 
       RowBox[{"R", " ", "T"}]}], 
      RowBox[{"V", "-", "b"}]], "-", 
     FractionBox["a", 
      SuperscriptBox["V", "2"]]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"6dd8c1ef-067f-47ee-81a1-a29877f366c4"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6189412868763404`*^9, 3.618941362465664*^9}, {
  3.619470201275498*^9, 3.619470207146143*^9}, {3.6781262933816433`*^9, 
  3.6781262934816437`*^9}},
 CellID->64309552,ExpressionUUID->"4a1748af-d8e5-4f7b-9df0-3abab222b274"],

Cell["rearranged:", "DetailNotes",
 CellChangeTimes->{{3.6189412868763404`*^9, 3.618941377244509*^9}},
 CellID->385298819,ExpressionUUID->"b58427d6-e71f-4c91-84fb-f9a515c5538d"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"T", " ", "=", " ", 
    RowBox[{
     FractionBox["1", "R"], " ", 
     RowBox[{"(", 
      RowBox[{"P", "+", 
       FractionBox["a", 
        SuperscriptBox["V", "2"]]}], ")"}], " ", 
     RowBox[{"(", 
      RowBox[{"V", "-", "b"}], ")"}]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"eb9b54d7-319f-4bfc-adfd-c90ae515e5c0"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6189412868763404`*^9, 3.618941406072158*^9}, 
   3.61947021314244*^9},
 CellID->106567780,ExpressionUUID->"62f76c1b-13f0-430e-a57f-e97f517c3349"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"a", "=", 
    RowBox[{
     FractionBox["27", "64"], " ", 
     FractionBox[
      RowBox[{
       SuperscriptBox["R", "2"], " ", 
       SuperscriptBox[
        SubscriptBox["T", 
         StyleBox["c",
          FontSlant->"Plain"]], "2"]}], 
      SubscriptBox["P", 
       StyleBox["c",
        FontSlant->"Plain"]]]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"e1361d1c-c249-4d53-94d8-72c7edb9c4f9"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6189412868763404`*^9, 3.618941417189794*^9}, {
   3.618941454049902*^9, 3.6189414731799965`*^9}, 3.619470214222437*^9, {
   3.619544874021208*^9, 3.619544893729476*^9}},
 CellID->17241382,ExpressionUUID->"40116ebd-ef28-4ea6-8263-d7b81b6badf9"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"b", " ", "=", " ", 
    FractionBox[
     RowBox[{"R", " ", 
      SubscriptBox["T", 
       StyleBox["c",
        FontSlant->"Plain"]]}], 
     RowBox[{"8", " ", 
      SubscriptBox["P", 
       StyleBox["c",
        FontSlant->"Plain"]]}]]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"238b8f83-a528-43f7-8b0d-1b4b916030c2"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.6189412868763404`*^9, 3.618941417189794*^9}, {
   3.618941454049902*^9, 3.6189414866037645`*^9}, {3.6189415419609303`*^9, 
   3.6189415419649305`*^9}, 3.619470215437434*^9, {3.619544907280245*^9, 
   3.619544913903812*^9}},
 CellID->301082822,ExpressionUUID->"39c22656-8d5a-4492-bb79-7ae12ae961d8"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["P", TraditionalForm]], "InlineMath",ExpressionUUID->
  "6c942388-a656-40ea-aa29-c442cb7e7a67"],
 " is pressure (MPa), ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]], "InlineMath",ExpressionUUID->
  "66e23959-fa4c-45b2-af5d-cc4c6a830bf8"],
 " is the ideal gas constant (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"[", 
     RowBox[{"MPa", " ", 
      SuperscriptBox["cm", "3"]}], "]"}], "/", 
    RowBox[{"[", 
     RowBox[{"mol", " ", "K"}], "]"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"bf714eea-1f73-405d-91de-fca4d5f3189a"],
 "), ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]], "InlineMath",ExpressionUUID->
  "b61b1d02-29a0-464d-960d-e1b7d2ee46f2"],
 " is temperature (K), ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]], "InlineMath",ExpressionUUID->
  "f92ceb20-4a02-44c9-9297-402748d821f1"],
 " is molar volume (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["cm", "3"], "/", "mol"}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"e3067272-e072-451b-8195-6ac5654c5991"],
 "), ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]], "InlineMath",ExpressionUUID->
  "29aab697-af6d-4461-ab53-2748a9ab6778"],
 " and ",
 Cell[BoxData[
  FormBox["b", TraditionalForm]], "InlineMath",ExpressionUUID->
  "aefaebe9-57ee-45cd-9eac-17afb3156d7a"],
 " are van der Waals constants, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["T", 
    StyleBox["c",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",ExpressionUUID->
  "a34adcb9-1b6b-4f71-8450-c651341a2970"],
 " is the critical temperature of water (K), and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["P", 
    StyleBox["c",
     FontSlant->"Plain"]], TraditionalForm]], "InlineMath",ExpressionUUID->
  "ecc96525-7a2f-497e-bcb2-2b3c4c3b294e"],
 " is the critical pressure (MPa)."
}], "DetailNotes",
 CellChangeTimes->{{3.6189412868763404`*^9, 3.618941417189794*^9}, {
  3.618941454049902*^9, 3.6189414866037645`*^9}, {3.6189415419609303`*^9, 
  3.6189417219042225`*^9}, {3.619544918787484*^9, 3.619544924293387*^9}, {
  3.619547791093883*^9, 3.6195478000728083`*^9}, {3.6500500279227247`*^9, 
  3.650050027926725*^9}, {3.650050303767502*^9, 3.6500503355403194`*^9}, {
  3.6781263096538415`*^9, 3.678126320309516*^9}, {3.6785549878336005`*^9, 
  3.678554994181734*^9}, {3.6791527487122636`*^9, 3.6791527503403673`*^9}},
