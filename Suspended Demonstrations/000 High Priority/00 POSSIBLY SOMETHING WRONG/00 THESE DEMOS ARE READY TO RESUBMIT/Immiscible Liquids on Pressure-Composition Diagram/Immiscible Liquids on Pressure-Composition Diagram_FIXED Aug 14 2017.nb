  FormBox[
   StyleBox[Cell[
    "b", "InlineMath",ExpressionUUID->"e0ee13ba-af92-48ab-b944-e8521225d03a"],
    FontSlant->"Plain"], TraditionalForm]], "InlineMath",ExpressionUUID->
  "bce79847-67ac-48a5-812a-a35e4caf7a29"],
