Cell[TextData[{
 "This Demonstration shows ",
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "-", "x", "-", "y"}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"03a4d38f-fb5e-4360-8916-3d79db4e8717"],
