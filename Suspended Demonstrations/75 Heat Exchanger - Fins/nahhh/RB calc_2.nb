Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["Re", 
     StyleBox["h",
      FontSlant->"Plain"]], "=", 
    FractionBox[
     RowBox[{"4", " ", 
      SubscriptBox["m", 
       StyleBox["h",
        FontSlant->"Plain"]]}], 
     RowBox[{"\[Pi]", " ", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["D", 
         StyleBox["o",
          FontSlant->"Plain"]], "-", 
        SubscriptBox["D", 
         RowBox[{"2", ",", 
          StyleBox["i",
           FontSlant->"Plain"]}]]}], ")"}], " ", 
      SubscriptBox["\[Mu]", 
       StyleBox["h",
        FontSlant->"Plain"]]}]]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "113b0abf-88a6-4cc0-88a0-c98b0e277d62"],
 ","
}], "DetailNotes",
 CellChangeTimes->{{3.718370764735552*^9, 3.718370832390422*^9}},
 CellID->20605246,ExpressionUUID->"15b58975-f806-4f04-838c-1fd941c264f9"],

Cell[TextData[{
 "if ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["Re", 
     StyleBox["h",
      FontSlant->"Plain"]], "\[GreaterEqual]", "2300"}], TraditionalForm]], 
  "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "8540aa7c-80fb-4e13-8ef9-7f7a9261e71e"],
 " then ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["Nu", 
     StyleBox["h",
      FontSlant->"Plain"]], "=", 
    RowBox[{"0.023", " ", 
     SubsuperscriptBox["Re", 
      StyleBox["h",
       FontSlant->"Plain"], 
      RowBox[{"4", "/", "5"}]], " ", 
     SubsuperscriptBox["Pr", 
      StyleBox["h",
       FontSlant->"Plain"], "0.3"]}]}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "38eaf673-87c9-4565-89f6-53f80785f9fc"],
 ", otherwise ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["Nu", 
     StyleBox["h",
      FontSlant->"Plain"]], "=", "4.36"}], TraditionalForm]], "InlineMath",
  FormatType->"TraditionalForm",ExpressionUUID->
  "99a22df9-99d7-4211-9f12-046912ba403a"],
 ","
}], "DetailNotes",
