  FormBox[
   RowBox[{
    SubscriptBox["x", 
     StyleBox["U",
      FontSlant->"Plain"]], "=", "0.31"}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"1cb8ea26-da13-412c-a915-c62d6cfbe441"],
