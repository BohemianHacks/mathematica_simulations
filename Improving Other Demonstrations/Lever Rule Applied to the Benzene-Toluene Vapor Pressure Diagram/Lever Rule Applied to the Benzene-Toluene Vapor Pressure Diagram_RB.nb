   3.484668737177558*^9, 3.484668793886397*^9}, {3.484668937462166*^9, 
   3.484668944059978*^9}, {3.4850216319753833`*^9, 3.485021646740914*^9}, {
   3.485030075280332*^9, 3.485030202106592*^9}, 3.4850987265040073`*^9},
